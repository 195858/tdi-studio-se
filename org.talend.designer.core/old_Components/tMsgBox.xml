<COMPONENT>
   <HEADER NAME="tMsgBox" ID="" DISPLAY_NAME="Message Box" PLATEFORM="WIN32" SERIAL="" VERSION="0.101" STATUS="ALPHA" ICON="tMsgBox32.ico" COMPATIBILITY="WIN32" AUTHOR="talend" RELEASE_DATE="20050320A" FAMILY="GUI">
      <SIGNATURE></SIGNATURE>
   </HEADER>
   <DISPLAY>
      <POSITION LEFT="100" TOP="100" WIDTH="49" HEIGHT="49"/>
      <LABEL FORMAT="label : __NAME__" VALUE="" SHOW="true" DEFAULT=""/>
      <HINT FORMAT="hint : __NAME__" VALUE="" SHOW="false" DEFAULT=""/>
   </DISPLAY>
   <DOCUMENTATION>
      <URL>tMsgBox_help.html</URL>
   </DOCUMENTATION>
   <CODE LANGUAGE="Perl" LOCALISATION="INTERNAL" URI=".">
      <VALUE></VALUE>
      <CALL><![CDATA[
my $mResult = &tMsgBox(
  Title    => __TITLE__,
  Message  => __MESSAGE__,
  Buttons  => __BUTTONS__, 
  Icons    => __ICON__ );
]]></CALL>
   </CODE>
   <CONNECTORS>
      <CONNECTOR DIRECTION="IN" CTYPE="START" DISPLAY_NAME="Start">
         <OTYPES>
            <OTYPE>TFileDesc</OTYPE>
         </OTYPES>
      </CONNECTOR>
      <CONNECTOR DIRECTION="OUT" CTYPE="BEFORE" DISPLAY_NAME="Before">
         <OTYPES>
            <OTYPE>TFileDesc</OTYPE>
            <OTYPE>THashRow (FirstRow)</OTYPE>
         </OTYPES>
         <CONNECTIONS>
            <TO COMPONENT_ID="AA" CTYPE="AA" OTYPE="AA"/>
            <TO COMPONENT_ID="AA" CTYPE="AA" OTYPE="AA"/>
            <FROM COMPONENT_ID="AA" CTYPE="AA" OTYPE="AA"/>
            <FROM COMPONENT_ID="AA" CTYPE="AA" OTYPE="AA"/>
         </CONNECTIONS>
      </CONNECTOR>
      <CONNECTOR DIRECTION="OUT" CTYPE="AFTER" DISPLAY_NAME="After">
         <OTYPES>
            <OTYPE>TFileDesc</OTYPE>
            <OTYPE>THashRow (LastRow + RowNumber)</OTYPE>
         </OTYPES>
      </CONNECTOR>
      <CONNECTOR DIRECTION="OUT" CTYPE="OK" DISPLAY_NAME="Ok">
         <OTYPES>
            <OTYPE>TFileDesc</OTYPE>
            <OTYPE>THashRow (LastRow + RowNumber)</OTYPE>
            <OTYPE>TErrorNumber</OTYPE>
            <OTYPE>TErrorMessage</OTYPE>
         </OTYPES>
      </CONNECTOR>
      <CONNECTOR DIRECTION="OUT" CTYPE="ERROR" DISPLAY_NAME="Error">
         <OTYPES>
            <OTYPE>TFileDesc</OTYPE>
            <OTYPE>THashRow (LastRow + RowNumber)</OTYPE>
            <OTYPE>TErrorNumber</OTYPE>
            <OTYPE>TErrorMessage</OTYPE>
         </OTYPES>
      </CONNECTOR>
   </CONNECTORS>
   <PARAMETERS>
      <PARAMETER NAME="TITLE" DISPLAY_NAME="Title" TYPE="STRING" FIELD="TEXT" SHOW="YES" REQUIRED="No" DEFAULT="'Integration Studio'">
         <VALUE>'Message'</VALUE>
      </PARAMETER>
      <PARAMETER NAME="MESSAGE" DISPLAY_NAME="Message" TYPE="STRING" FIELD="MEMO" SHOW="YES" REQUIRED="No" DEFAULT="">
         <VALUE>'Hello !'</VALUE>
      </PARAMETER>
      <PARAMETER NAME="BUTTONS" DISPLAY_NAME="Buttons" TYPE="INTEGER" FIELD="CLOSED_LIST" SHOW="YES" REQUIRED="No" DEFAULT="OK (0)">
         <VALUE>OK (0)</VALUE>
         <ITEMS>
            <ITEM IID="0" VALUE="OK (0)"/>
            <ITEM IID="1" VALUE="Ok and Cancel (1)"/>
            <ITEM IID="2" VALUE="Abort, Retry, and Ignore (2)"/>
            <ITEM IID="3" VALUE="Yes, No and Cancel (3)"/>
            <ITEM IID="4" VALUE="Yes and No (4)"/>
            <ITEM IID="5" VALUE="Retry and Cancel (5)"/>
         </ITEMS>
      </PARAMETER>
      <PARAMETER NAME="ICON" DISPLAY_NAME="Icon" TYPE="STRING" FIELD="CLOSED_LIST" SHOW="YES" REQUIRED="No" DEFAULT="MB_ICONINFORMATION">
         <VALUE>MB_ICONINFORMATION</VALUE>
         <ITEMS>
            <ITEM VALUE="MB_ICONSTOP" IID="Win32::MB_ICONSTOP"/>
            <ITEM VALUE="MB_ICONQUESTION" IID="Win32::MB_ICONQUESTION"/>
            <ITEM VALUE="MB_ICONEXCLAMATION" IID="Win32::MB_ICONEXCLAMATION"/>
            <ITEM VALUE="MB_ICONINFORMATION" IID="Win32::MB_ICONINFORMATION"/>
         </ITEMS>
      </PARAMETER>
   </PARAMETERS>
   <RETURNS>
      <RETURN NAME="ERROR_CODE" DISPLAY_NAME="Error Code" TYPE="INTEGER" AVAILABILITY="After" CODE="$gv{tMsgBox}{Error}{Code}"/>
      <RETURN NAME="ERROR_MESSAGE" DISPLAY_NAME="Error Message" TYPE="STRING" AVAILABILITY="After" CODE="$gv{tMsgBox}{Error}{Message}"/>
   </RETURNS>
</COMPONENT>
