      </PARAMETER>
      <PARAMETER NAME="INCLUDEHEADER" DISPLAY_NAME="Include Header" TYPE="STRING" FIELD="TEXT" SHOW="YES" REQUIRED="No" DEFAULT="0" READONLY="FALSE">
         <VALUE>''</VALUE>
      </PARAMETER>
      <PARAMETER NAME="FILEDEF" DISPLAY_NAME="File Definition" TYPE="STRING" FIELD="TEXT" SHOW="YES" REQUIRED="No" DEFAULT="" READONLY="TRUE">
         <VALUE/>
      </PARAMETER>
            <PARAMETER NAME="SCHEMATYPE" DISPLAY_NAME="Schema Type" TYPE="STRING" FIELD="CLOSED_LIST" SHOW="YES" REQUIRED="No" DEFAULT="Built-in">
