<COMPONENT>
   <HEADER NAME="tSound" ID="" DISPLAY_NAME="Sound" PLATEFORM="WIN32" SERIAL="" VERSION="0.101" STATUS="ALPHA" ICON="tSound32.ico" COMPATIBILITY="WIN32" AUTHOR="talend" RELEASE_DATE="20050320A" FAMILY="GUI">
      <SIGNATURE></SIGNATURE>
   </HEADER>
   <DISPLAY>
      <POSITION LEFT="100" TOP="100" WIDTH="49" HEIGHT="49"/>
      <LABEL FORMAT="label : __NAME__" VALUE="" SHOW="true" DEFAULT=""/>
      <HINT FORMAT="hint : __NAME__" VALUE="" SHOW="false" DEFAULT=""/>
   </DISPLAY>
   <DOCUMENTATION>
      <URL>tSound_help.html</URL>
   </DOCUMENTATION>
   <CODE LANGUAGE="Perl" LOCALISATION="INTERNAL" URI=".">
      <VALUE/>
      <CALL><![CDATA[
my $mResult = &tSound(
  File    => __FILE__,
  Volume  => __VOLUME__);
]]></CALL>
   </CODE>
   <CONNECTORS>
      <CONNECTOR DIRECTION="IN" CTYPE="START" DISPLAY_NAME="Start">
         <OTYPES>
            <OTYPE>TFileDesc</OTYPE>
         </OTYPES>
      </CONNECTOR>
      <CONNECTOR DIRECTION="OUT" CTYPE="BEFORE" DISPLAY_NAME="Before">
         <OTYPES>
            <OTYPE>TFileDesc</OTYPE>
            <OTYPE>THashRow (FirstRow)</OTYPE>
         </OTYPES>
         <CONNECTIONS>
            <TO COMPONENT_ID="AA" CTYPE="AA" OTYPE="AA"/>
            <TO COMPONENT_ID="AA" CTYPE="AA" OTYPE="AA"/>
            <FROM COMPONENT_ID="AA" CTYPE="AA" OTYPE="AA"/>
            <FROM COMPONENT_ID="AA" CTYPE="AA" OTYPE="AA"/>
         </CONNECTIONS>
      </CONNECTOR>
      <CONNECTOR DIRECTION="OUT" CTYPE="AFTER" DISPLAY_NAME="After">
         <OTYPES>
            <OTYPE>TFileDesc</OTYPE>
            <OTYPE>THashRow (LastRow + RowNumber)</OTYPE>
         </OTYPES>
      </CONNECTOR>
      <CONNECTOR DIRECTION="OUT" CTYPE="OK" DISPLAY_NAME="Ok">
         <OTYPES>
            <OTYPE>TFileDesc</OTYPE>
            <OTYPE>THashRow (LastRow + RowNumber)</OTYPE>
            <OTYPE>TErrorNumber</OTYPE>
            <OTYPE>TErrorMessage</OTYPE>
         </OTYPES>
      </CONNECTOR>
      <CONNECTOR DIRECTION="OUT" CTYPE="ERROR" DISPLAY_NAME="Error">
         <OTYPES>
            <OTYPE>TFileDesc</OTYPE>
            <OTYPE>THashRow (LastRow + RowNumber)</OTYPE>
            <OTYPE>TErrorNumber</OTYPE>
            <OTYPE>TErrorMessage</OTYPE>
         </OTYPES>
      </CONNECTOR>
   </CONNECTORS>
   <PARAMETERS>
      <PARAMETER NAME="VOLUME" DISPLAY_NAME="Volume (%)" TYPE="STRING" FIELD="TEXT" SHOW="YES" REQUIRED="100" DEFAULT="100">
         <VALUE>100</VALUE>
      </PARAMETER>
      <PARAMETER NAME="FILE" DISPLAY_NAME="File" TYPE="STRING" FIELD="CLOSED_LIST" SHOW="YES" REQUIRED="No" DEFAULT="SystemDefault">
         <VALUE>SystemDefault</VALUE>
         <ITEMS>
            <ITEM VALUE="SystemDefault" IID="SystemDefault"/>
            <ITEM VALUE="SystemAsterisk" IID="SystemAsterisk"/>
            <ITEM VALUE="SystemExclamation" IID="SystemExclamation"/>
            <ITEM VALUE="SystemExit" IID="SystemExit"/>
            <ITEM VALUE="SystemHand" IID="SystemHand"/>
            <ITEM VALUE="SystemQuestion" IID="SystemQuestion"/>
            <ITEM VALUE="SystemStart" IID="SystemStart"/>
         </ITEMS>
      </PARAMETER>
   </PARAMETERS>
   <RETURNS>
      <RETURN NAME="ERROR_CODE" DISPLAY_NAME="Error Code" TYPE="INTEGER" AVAILABILITY="After" CODE="$gv{tSound}{Error}{Code}"/>
      <RETURN NAME="ERROR_MESSAGE" DISPLAY_NAME="Error Message" TYPE="STRING" AVAILABILITY="After" CODE="$gv{tSound}{Error}{Message}"/>
   </RETURNS>
</COMPONENT>
