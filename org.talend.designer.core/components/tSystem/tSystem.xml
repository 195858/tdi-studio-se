<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    ICON32="tSystem.png"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"    MAX_OUTPUT="0" MAX_INPUT="0" />
    <CONNECTOR CTYPE="RUN_ERROR" MAX_OUTPUT="0" MAX_INPUT="0" />
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="COMMAND" FIELD="TEXT" REQUIRED="true" NUM_ROW="1">
      <DEFAULT>'echo HELLO'</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="OUTPUT" FIELD="CLOSED_LIST" NUM_ROW="2">
       <ITEMS DEFAULT="OUTPUT_TO_CONSOLE">
          <ITEM NAME="OUTPUT_TO_CONSOLE" VALUE="OUTPUT_TO_CONSOLE" />
          <ITEM NAME="RETRIEVE_OUTPUT"   VALUE="RETRIEVE_OUTPUT" />
       </ITEMS>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION>
    <TEMPLATES>
      <TEMPLATE MULTIPLE_METHODS="true"/>
    </TEMPLATES>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="OUTPUT" TYPE="STRING" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
