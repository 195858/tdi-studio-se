<!-- $Id$ -->
<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA" ICON="tDBOutput32.ico" COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A" STARTABLE="false" SCHEMA_AUTO_PROPAGATE="true">
		<SIGNATURE/>
	</HEADER>
   <DOCUMENTATION>
      <URL/>
   </DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
		<CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="REFERENCE"/>
		<CONNECTOR CTYPE="RUN_OK"/>
		<CONNECTOR CTYPE="RUN_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>
   <PARAMETERS>
      <PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" NUM_ROW="1" REPOSITORY_VALUE="DATABASE" />
      <PARAMETER NAME="TYPE" FIELD="CLOSED_LIST" NUM_ROW="2" REPOSITORY_VALUE="TYPE">
         <ITEMS CODE_LANGUAGE="perl" DEFAULT="MYSQL">
<!-- in the value, we give "Perl dbtype; Perl driver; Talend dbtype". -->
<!-- See org.talend.core/mappingMetadataTypes.xml for Talend dbtypes  -->
            <ITEM NAME="MYSQL"      VALUE="mysql;mysql;MYSQL"                REPOSITORY_ITEM="MYSQL"/>
            <ITEM NAME="PGSQL"      VALUE="postgresql;Pg;POSTGRESQL"         REPOSITORY_ITEM="PGSQL"/>
            <ITEM NAME="ORACLE"     VALUE="oracle;oracle;Oracle Oci"         REPOSITORY_ITEM="ORACLE"/>
            <ITEM NAME="ODBC"       VALUE="undef;odbc;MICROSOFT SQL SERVER"  REPOSITORY_ITEM="ODBC"/>
            <ITEM NAME="MSSQL_ODBC" VALUE="mssql;odbc;MICROSOFT SQL SERVER"  REPOSITORY_ITEM="ODBC_JDBC"/>
         </ITEMS>
      </PARAMETER>

      <PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="3" REPOSITORY_VALUE="SERVER_NAME" NOT_SHOW_IF="(TYPE=='MSSQL_ODBC' or TYPE=='ODBC')">
         <DEFAULT>''</DEFAULT>
      </PARAMETER>
      <PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="3" REPOSITORY_VALUE="PORT" NOT_SHOW_IF="(TYPE=='MSSQL_ODBC' or TYPE=='ODBC')">
        <DEFAULT>''</DEFAULT>
	<DEFAULT IF="TYPE=='MYSQL'">'3306'</DEFAULT>
        <DEFAULT IF="TYPE=='PGSQL'">'5432'</DEFAULT>
      </PARAMETER>
      <PARAMETER NAME="DBNAME" FIELD="TEXT" NUM_ROW="3" REPOSITORY_VALUE="SID">
         <DEFAULT>''</DEFAULT>
      </PARAMETER>

      <PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="4" REPOSITORY_VALUE="USERNAME">
         <DEFAULT>''</DEFAULT>
      </PARAMETER>
      <PARAMETER NAME="PASS" FIELD="TEXT" NUM_ROW="4" REPOSITORY_VALUE="PASSWORD">
         <DEFAULT>''</DEFAULT>
      </PARAMETER>

      <PARAMETER NAME="TABLE" FIELD="TEXT" NUM_ROW="5">
         <DEFAULT>''</DEFAULT>
      </PARAMETER>

      <PARAMETER NAME="ACTION" FIELD="CLOSED_LIST" NUM_ROW="6">
         <ITEMS CODE_LANGUAGE="perl" DEFAULT="NONE">
            <ITEM NAME="NONE"              VALUE="NONE" />
            <ITEM NAME="DROP_CREATE_TABLE" VALUE="DROP_CREATE_TABLE" NOT_SHOW_IF="TYPE=='ODBC'"/>
            <ITEM NAME="CREATE_TABLE"      VALUE="CREATE_TABLE" NOT_SHOW_IF="TYPE=='ODBC'"/>
            <ITEM NAME="CLEAR_TABLE"       VALUE="CLEAR_TABLE" />
         </ITEMS>
      </PARAMETER>

      <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="7">
         <DEFAULT>''</DEFAULT>
      </PARAMETER>

     <PARAMETER NAME="ENCODING" FIELD="TEXT" NUM_ROW="8" REQUIRED="true">
       <DEFAULT>'ISO-8859-15'</DEFAULT>
     </PARAMETER>
   </PARAMETERS>

   <CODEGENERATION>
     <TEMPLATES>
      <TEMPLATE NAME="java" MULTIPLE_METHODS="true"/>
      <TEMPLATE NAME="perl" MULTIPLE_METHODS="true"/>
     </TEMPLATES>
     <IMPORTS>
       <IMPORT NAME="DBI" MODULE="DBI" REQUIRED="true" />
       <IMPORT NAME="DBD-Oracle" MODULE="DBD::Oracle" REQUIRED="false" />
       <IMPORT NAME="DBD-mysql" MODULE="DBD::Mysql" REQUIRED="false" />
       <IMPORT NAME="DBD-ODBC" MODULE="DBD::ODBC" REQUIRED="false" />
     </IMPORTS>
   </CODEGENERATION>
   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
