      <TEMPLATE MULTIPLE_METHODS="true"/>
    </TEMPLATES>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="CURRENT_ITERATION" TYPE="INT" AVAILABILITY="FLOW"/>
  </RETURNS>
