<!-- $Id$ -->
<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    ICON="tFileInputExcel.png"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A" STARTABLE="true"
    SCHEMA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE />
  </HEADER>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1" />
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1" />
    <CONNECTOR CTYPE="REFERENCE" />
    <CONNECTOR CTYPE="RUN_OK" />
    <CONNECTOR CTYPE="RUN_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" NUM_ROW="1" REPOSITORY_VALUE="DELIMITED" />

    <PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="2" REQUIRED="true" REPOSITORY_VALUE="FILE_PATH">
      <DEFAULT>'C:\talend_files\in.xls'</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SHEETNAME" FIELD="TEXT" NUM_ROW="3" REQUIRED="true" REPOSITORY_VALUE="SHEETNAME">
      <DEFAULT>'first sheet'</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="HEADER" FIELD="TEXT" NUM_ROW="4" REPOSITORY_VALUE="HEADER">
      <DEFAULT>0</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="FOOTER" FIELD="TEXT" NUM_ROW="4" REPOSITORY_VALUE="FOOTER">
      <DEFAULT>0</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="LIMIT" FIELD="TEXT" NUM_ROW="4" REPOSITORY_VALUE="LIMIT">
      <DEFAULT />
    </PARAMETER>

    <PARAMETER NAME="FIRST_COLUMN" FIELD="TEXT" NUM_ROW="5" REPOSITORY_VALUE="FIRST_COLUMN">
      <DEFAULT>'1'</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="LAST_COLUMN" FIELD="TEXT" NUM_ROW="5" REPOSITORY_VALUE="LAST_COLUMN">
      <DEFAULT/>
    </PARAMETER>

    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="6" />

    <PARAMETER NAME="ENCODING" FIELD="TEXT" NUM_ROW="7" REQUIRED="true" REPOSITORY_VALUE="ENCODING">
      <DEFAULT>'ISO-8859-15'</DEFAULT>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION>
    <TEMPLATES>
      <TEMPLATE MULTIPLE_METHODS="true" />
    </TEMPLATES>
    <IMPORTS>
      <IMPORT NAME="Spreadsheet-ParseExcel" MODULE="Spreadsheet::ParseExcel" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER" />
  </RETURNS>
</COMPONENT>