<!-- $Id$ -->
<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		ICON="tFileInOut.ico" COMPATIBILITY="ALL" AUTHOR="Talend"
		RELEASE_DATE="20050320A" STARTABLE="true"
		SCHEMA_AUTO_PROPAGATE="false">
		<SIGNATURE />
	</HEADER>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="BEFORE" MAX_INPUT="1" />
		<CONNECTOR CTYPE="AFTER" MAX_INPUT="1" />
		<CONNECTOR CTYPE="REFERENCE" />
		<CONNECTOR CTYPE="RUN_OK" />
		<CONNECTOR CTYPE="RUN_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="FILENAME_INPUT" FIELD="FILE" NUM_ROW="2"
			REQUIRED="true">
			<DEFAULT>'C:\talend_files\in.csv'</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FILENAME_OUTPUT" FIELD="FILE" NUM_ROW="3"
			REQUIRED="true">
			<DEFAULT>'C:\talend_files\in.csv'</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FIELDSEPARATOR_INPUT" FIELD="TEXT" NUM_ROW="4">
			<DEFAULT>';'</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FIELDSEPARATOR_OUTPUT" FIELD="TEXT" NUM_ROW="4">
			<DEFAULT>';'</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="APPEND" FIELD="CHECK" REQUIRED="true" NUM_ROW="5">
      		<DEFAULT>false</DEFAULT>
    	</PARAMETER>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="6">
			<DEFAULT/> 
		</PARAMETER>
	</PARAMETERS>
	<CODEGENERATION>
		<TEMPLATES INPUT="tFileInputDelimited" OUTPUT="tFileOutputDelimited">
			<TEMPLATE NAME="tFileInputDelimited" CTYPE="FLOW" LINK_TO="tFileOutputDelimited" />
			<TEMPLATE NAME="tFileOutputDelimited" />
			<TEMPLATE NAME="tMsgBox" CTYPE="AFTER" LINK_TO="tFileInputDelimited" />
			<TEMPLATE_PARAM SOURCE="tFileInOut.FILENAME_INPUT" TARGET="tFileInputDelimited.FILENAME" />
			<TEMPLATE_PARAM SOURCE="tFileInOut.FILENAME_OUTPUT" TARGET="tFileOutputDelimited.FILENAME" />
			<TEMPLATE_PARAM SOURCE="tFileInOut.FIELDSEPARATOR_INPUT" TARGET="tFileInputDelimited.FIELDSEPARATOR" />
			<TEMPLATE_PARAM SOURCE="tFileInOut.FIELDSEPARATOR_OUTPUT" TARGET="tFileOutputDelimited.FIELDSEPARATOR" />
			<TEMPLATE_PARAM SOURCE="tFileInOut.FILENAME_OUTPUT" TARGET="tMsgBox.MESSAGE"/>
			<TEMPLATE_PARAM SOURCE="tFileInOut.APPEND" TARGET="tFileOutputDelimited.APPEND"/>
		</TEMPLATES>
	</CODEGENERATION>
	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>