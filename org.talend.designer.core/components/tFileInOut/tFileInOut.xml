<!-- $Id$ -->
<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		ICON="tFileInOut.ico" COMPATIBILITY="ALL" AUTHOR="Talend"
		RELEASE_DATE="20050320A" STARTABLE="true"
		SCHEMA_AUTO_PROPAGATE="false">
		<SIGNATURE />
	</HEADER>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="BEFORE" MAX_INPUT="1" />
		<CONNECTOR CTYPE="AFTER" MAX_INPUT="1" />
		<CONNECTOR CTYPE="REFERENCE" />
		<CONNECTOR CTYPE="RUN_OK" />
		<CONNECTOR CTYPE="RUN_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="FILENAME_INPUT" FIELD="FILE" NUM_ROW="2"
			REQUIRED="true">
			<DEFAULT>'C:\talend_files\in.csv'</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FILENAME_OUTPUT" FIELD="FILE" NUM_ROW="3"
			REQUIRED="true">
			<DEFAULT>'C:\talend_files\in.csv'</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FIELDSEPARATOR_INPUT" FIELD="TEXT" NUM_ROW="4">
			<DEFAULT>';'</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FIELDSEPARATOR_OUTPUT" FIELD="TEXT" NUM_ROW="4">
			<DEFAULT>';'</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="APPEND" FIELD="CHECK" REQUIRED="true" NUM_ROW="5">
      		<DEFAULT>false</DEFAULT>
    	</PARAMETER>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="6">
			<DEFAULT/> 
		</PARAMETER>
	</PARAMETERS>
	<CODEGENERATION>
		<TEMPLATES INPUT="tFileInputDelimited1" OUTPUT="tFileOutputDelimited1">
			<TEMPLATE NAME="tFileInputDelimited1" COMPONENT="tFileInputDelimited">
				<LINK_TO NAME="tFileOutputDelimited1" CTYPE="FLOW" />
				<LINK_TO NAME="tMsgBox1" CTYPE="AFTER" />
				<LINK_TO NAME="tMsgBox2" CTYPE="BEFORE" />
			</TEMPLATE>
			<TEMPLATE NAME="tFileOutputDelimited1" COMPONENT="tFileOutputDelimited"/>
			<TEMPLATE NAME="tMsgBox1" COMPONENT="tMsgBox"/>
			<TEMPLATE NAME="tMsgBox2" COMPONENT="tMsgBox"/>
			<TEMPLATE_PARAM SOURCE="self.FILENAME_INPUT" TARGET="tFileInputDelimited1.FILENAME" />
			<TEMPLATE_PARAM SOURCE="self.FILENAME_OUTPUT" TARGET="tFileOutputDelimited1.FILENAME" />
			<TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR_INPUT" TARGET="tFileInputDelimited1.FIELDSEPARATOR" />
			<TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR_OUTPUT" TARGET="tFileOutputDelimited1.FIELDSEPARATOR" />
			<TEMPLATE_PARAM SOURCE="self.FILENAME_INPUT" TARGET="tMsgBox1.MESSAGE"/>
			<TEMPLATE_PARAM SOURCE="self.FILENAME_OUTPUT" TARGET="tMsgBox2.MESSAGE"/>
			<TEMPLATE_PARAM SOURCE="self.APPEND" TARGET="tFileOutputDelimited1.APPEND"/>
		</TEMPLATES>
	</CODEGENERATION>
	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>