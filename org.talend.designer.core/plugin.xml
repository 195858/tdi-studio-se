            class="org.talend.designer.core.ui.preferences.DesignerPreferencePage"
            id="org.talend.designer.core.ui.preferences.TalendPreferencePage"
            name="%designer">
      </page>
      <page
            category="org.talend.core.prefs"
            class="org.talend.designer.core.ui.preferences.StatsAndLogsPreferenceNodePage"
            id="org.talend.designer.core.ui.preferences.StatsAndLogsPreferenceNodePage"
            name="%statsAndLogs">
      </page>
      <page
            category="org.talend.designer.core.ui.preferences.StatsAndLogsPreferenceNodePage"
            class="org.talend.designer.core.ui.preferences.PERLStatsAndLogsPreferencePage"
            id="org.talend.designer.core.ui.preferences.PERLStatsAndLogsPreferencePage"
            name="Perl"/>
      <page
            category="org.talend.designer.core.ui.preferences.StatsAndLogsPreferenceNodePage"
            class="org.talend.designer.core.ui.preferences.JAVAStatsAndLogsPreferencePage"
            id="org.talend.designer.core.ui.preferences.JAVAStatsAndLogsPreferencePage"
            name="Java"/>
      <page
            category="org.talend.core.prefs"
            class="org.talend.designer.core.ui.preferences.PropertiesPreferencePage"
            id="org.talend.designer.core.properties.memo.text.font"
            name="Memo Text Font">
      </page>
   </extension>
