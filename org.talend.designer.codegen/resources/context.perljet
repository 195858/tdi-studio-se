<%@ jet 
	package="org.talend.designer.codegen.translators" 
	imports="
		org.talend.core.model.process.IContextParameter 
		java.util.List
	" 
	class="Context" 
%>

<%
	List<IContextParameter> parameterList = (List<IContextParameter>) argument;
	for (IContextParameter parameter : parameterList) {
%>
$_context{<%=parameter.getName() %>} = <%=parameter.getValue() %>;
<%
	}
%>

1;
