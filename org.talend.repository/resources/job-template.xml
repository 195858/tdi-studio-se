<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

	<!-- Talend Job OSGi blueprint descriptor -->

	<bean id="job" class="@JOBCLASSNAME@" />

	<service ref="job">
		<interfaces>
			<value>routines.system.api.TalendJob</value>
<!--		Commented out because of TESB-3529 until the better solution found
	 		<value>routines.system.api.TalendESBJob</value>  -->
		</interfaces>
		<service-properties>
			<entry key="name" value="@JOBNAME@" />
			<entry key="type" value="@TYPE@" />
		</service-properties>
	</service>

</blueprint>