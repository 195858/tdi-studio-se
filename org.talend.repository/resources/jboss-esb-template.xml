<?xml version = "1.0" encoding = "UTF-8"?>
<jbossesb
	xmlns="http://anonsvn.labs.jboss.com/labs/jbossesb/trunk/product/etc/schemas/xml/jbossesb-1.0.1.xsd"
	parameterReloadSecs="5">
	<providers>
		<jms-provider name="JBossMQ" connection-factory="ConnectionFactory">
			<jms-bus busid="#JobAlias#GwChannel">
				<jms-message-filter dest-type="QUEUE"
					dest-name="queue/#QueueName#" />
			</jms-bus>
			<jms-bus busid="#JobAlias#EsbChannel">
				<jms-message-filter dest-type="QUEUE" dest-name="queue/B" />
			</jms-bus>
		</jms-provider>
	</providers>
	<services>
		<service category="GeneratedESB" name="SimpleListener"
			description="Generated ESB Job Export">
			<listeners>
				<jms-listener name="JMS-Gateway" busidref="#JobAlias#GwChannel"
					is-gateway="true" />
				<jms-listener name="JMS-ESBListener" busidref="#JobAlias#EsbChannel" />
			</listeners>
			<actions mep="OneWay">
				<action name="#JobAlias#"
					class="#JobPackage#.#JobName#ListenerAction"
					process="#JobName#">
				</action>
			</actions>
		</service>
	</services>
</jbossesb>