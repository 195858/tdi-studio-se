<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.talend.core.service">
      <Service
            class="org.talend.designer.components.model.ComponentsService"
            serviceId="IComponentsService"/>
      <Service
            class="org.talend.designer.components.ComponentsLocalProviderService"
            serviceId="ComponentsLocalProviderService"/>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.talend.core.prefs"
            class="org.talend.designer.components.ui.ComponentsPreferencePage"
            id="org.talend.designer.components.localprovider.page1"
            name="%components"/>
      <page
            category="org.talend.designer.components.localprovider.page1"
            class="org.talend.designer.components.preference.labelformat.ComponentsFormatPreferencePage"
            id="org.talend.designer.components.localprovider.page1"
            name="%format">
      </page>
   </extension>
   <extension
         point="org.talend.core.components_provider">
      <ComponentsProvider
            class="org.talend.designer.components.model.UserComponentsProvider"
            folderName="user"
            id="org.talend.designer.components.model.UserComponentsProvider">
      </ComponentsProvider>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.talend.designer.components.ui.ComponentPreferenceInitializer">
      </initializer>
   </extension>

</plugin>
