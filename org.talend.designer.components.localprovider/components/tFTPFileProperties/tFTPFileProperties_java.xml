<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>Internet/FTP</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="10">
			<TABLE READONLY="true">
				<COLUMN NAME="abs_path" TYPE="id_String" LENGTH="255" />
				<COLUMN NAME="dirname" TYPE="id_String" LENGTH="255" />
				<COLUMN NAME="basename" TYPE="id_String" LENGTH="255" />
				<COLUMN NAME="size" TYPE="id_Long" LENGTH="20" />
				<COLUMN NAME="mtime" TYPE="id_Long" LENGTH="20" />
				<COLUMN NAME="mtime_string" TYPE="id_String" LENGTH="20" />
			</TABLE>
		</PARAMETER>

  
	    <PARAMETER
	      NAME="USE_EXISTING_CONNECTION"
	      FIELD="CHECK"
	      NUM_ROW="11"
	      >
	      <DEFAULT>false</DEFAULT>
	    </PARAMETER>   
	    
	    <PARAMETER
	      NAME="CONNECTION"
	      FIELD="COMPONENT_LIST"
	      FILTER="tFTPConnection"
	      NUM_ROW="11"
	      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
	    />		

		<PARAMETER NAME="HOST" FIELD="TEXT" REQUIRED="true" NUM_ROW="15"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>"localhost"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PORT" FIELD="TEXT" REQUIRED="true" NUM_ROW="15"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>21</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true"	NUM_ROW="20"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>"anonymous"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PASSWORD" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="20" NOT_SHOW_IF="((SFTP=='true') AND (AUTH_METHOD == 'PUBLICKEY')) OR (USE_EXISTING_CONNECTION == 'true')">
			<DEFAULT>"suomynona"</DEFAULT>
		</PARAMETER>
		
	    <PARAMETER
	      NAME="PRIVATEKEY"
	      FIELD="FILE"
	      NUM_ROW="21"
	      SHOW_IF="(SFTP=='true') AND (AUTH_METHOD == 'PUBLICKEY')"
	    >
	      <DEFAULT>"/.ssh/id_dsa"</DEFAULT>
	    </PARAMETER>
	
		<PARAMETER
	      NAME="PASSPHRASE"
	      FIELD="TEXT"
	      NUM_ROW="22"
	      SHOW_IF="(SFTP=='true') AND (AUTH_METHOD == 'PUBLICKEY')"
	    >
	      <DEFAULT>"secret"</DEFAULT>
	    </PARAMETER> 		
		
		<PARAMETER NAME="REMOTEDIR" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="60">
			<DEFAULT>"."</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="FILENAME" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="70">
			<DEFAULT>"filename.suffix"</DEFAULT>
		</PARAMETER>
		
	    <PARAMETER NAME="SFTP" FIELD="CHECK" NUM_ROW="80">
	      <DEFAULT>false</DEFAULT>
	    </PARAMETER>
	    
	    <PARAMETER
	      NAME="AUTH_METHOD"
	      FIELD="CLOSED_LIST"
	      NUM_ROW="80"
	      SHOW_IF="(SFTP=='true')"
	    >
	      <ITEMS DEFAULT="PASSWORD">
	        <ITEM NAME="PUBLICKEY"  VALUE="PUBLICKEY"/>
	        <ITEM NAME="PASSWORD"   VALUE="PASSWORD"/>
	      </ITEMS>
	    </PARAMETER>
    <PARAMETER NAME="CONNECT_MODE" FIELD="CLOSED_LIST" NUM_ROW="100" SHOW_IF="(SFTP=='false')">
      <ITEMS DEFAULT="PASSIVE">
        <ITEM NAME="PASSIVE"  VALUE="PASSIVE" />        
     	<ITEM NAME="ACTIVE"  VALUE="ACTIVE" />
      </ITEMS>
    </PARAMETER>
    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="110"
      REQUIRED="true">
       <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>     
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
	
		<PARAMETER NAME="USE_PROXY" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="85" REPOSITORY_VALUE="USE_PROXY">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	
		<PARAMETER NAME="PROXY_HOST" FIELD="TEXT" 
			NUM_ROW="86" SHOW_IF="USE_PROXY == 'true'" REPOSITORY_VALUE="PROXY_HOST">
			<DEFAULT>"61.163.92.4"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PORT" FIELD="TEXT"
			NUM_ROW="87" SHOW_IF="USE_PROXY == 'true'" REPOSITORY_VALUE="PROXY_PORT">
			<DEFAULT>"1080"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PROXY_USERNAME" FIELD="TEXT" 
			NUM_ROW="88" SHOW_IF="USE_PROXY == 'true'" REPOSITORY_VALUE="PROXY_USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PASSWORD" FIELD="TEXT"
			NUM_ROW="89" SHOW_IF="USE_PROXY == 'true'" REPOSITORY_VALUE="PROXY_PASSWORD">
			<DEFAULT>""</DEFAULT>
		</PARAMETER> 	  
	
		<PARAMETER NAME="IGNORE_FAILURE_AT_QUIT" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="90" SHOW_IF="(SFTP=='false')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>
	
	<CODEGENERATION>
		<IMPORTS>
	       <IMPORT NAME="Java-FTP" MODULE="edtftpj.jar" REQUIRED="true" />
	       <IMPORT NAME="Java-SFTP" MODULE="jsch-0.1.36.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>
	<RETURNS />
</COMPONENT>