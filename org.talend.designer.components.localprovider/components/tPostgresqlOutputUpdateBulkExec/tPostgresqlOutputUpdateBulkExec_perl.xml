<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:POSTGRESQL"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="20"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="20"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>MySQL</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="21"
      REPOSITORY_VALUE="PORT"
    >
      <DEFAULT>5432</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA_DB"
      FIELD="TEXT"
      NUM_ROW="31"
      REPOSITORY_VALUE="SCHEMA"
    >
      <DEFAULT>'public'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="41"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="42"
      REQUIRED="true"
    >
      <DEFAULT>'ISO-8859-15'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="80"
    >
      <DEFAULT>'employee'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="90"
      REQUIRED="true"
    >
      <DEFAULT>'__COMP_DEFAULT_FILE_DIR__/postgresql_bulk.txt'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DELIMITER"
      FIELD="TEXT"
      NUM_ROW="100"
      REQUIRED="true"
    >
      <DEFAULT>';'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="110"
    >
      <DEFAULT/> 
    </PARAMETER>

    <PARAMETER
      NAME="FIELD_OPTIONS"
      FIELD="TABLE"
      REQUIRED="true"
      NUM_ROW="120"
      NB_LINES="5"
    >
      <ITEMS BASED_ON_SCHEMA="true">
        <ITEM NAME="UPDATE_KEY" VALUE="false" FIELD="CHECK" />
       </ITEMS>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
    <TEMPLATES INPUT="tPOB" OUTPUT="tPUBE">

      <TEMPLATE NAME="tPOB" COMPONENT="tPostgresqlOutputBulk">
        <LINK_TO NAME="tPUBE" CTYPE="THEN_RUN"/>
      </TEMPLATE>
      
      <TEMPLATE NAME="tPUBE" COMPONENT="tPostgresqlUpdateBulkExec"/>

      <TEMPLATE_PARAM SOURCE="self.HOST"   TARGET="tPUBE.HOST" />
      <TEMPLATE_PARAM SOURCE="self.DBNAME" TARGET="tPUBE.DBNAME" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA_DB" TARGET="tPUBE.SCHEMA_DB" />
      <TEMPLATE_PARAM SOURCE="self.PORT"   TARGET="tPUBE.PORT"   />
      <TEMPLATE_PARAM SOURCE="self.USER"   TARGET="tPUBE.USER" />
      <TEMPLATE_PARAM SOURCE="self.PASS"   TARGET="tPUBE.PASS" />
      <TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="tPUBE.ENCODING" />
      <TEMPLATE_PARAM SOURCE="self.TABLE"  TARGET="tPUBE.TABLE" />

      <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="tPOB.SCHEMA" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="tPUBE.SCHEMA" />

      <TEMPLATE_PARAM
        SOURCE="self.FIELD_OPTIONS"
        TARGET="tMUBE.FIELD_OPTIONS"
      />

      <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tPOB.FILENAME" />
      <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tPUBE.FILENAME" />

      <TEMPLATE_PARAM SOURCE="self.DELIMITER"  TARGET="tPOB.DELIMITER" />
      <TEMPLATE_PARAM SOURCE="self.DELIMITER"  TARGET="tPUBE.DELIMITER" />

    </TEMPLATES>
  </CODEGENERATION>
  <RETURNS/>
</COMPONENT>
