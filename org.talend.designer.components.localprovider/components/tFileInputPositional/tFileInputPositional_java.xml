<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070210A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="POSITIONAL"
    />
  
    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="2"
      REQUIRED="true"
      REPOSITORY_VALUE="FILE_PATH"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.csv"</DEFAULT>
    </PARAMETER>
  
    <PARAMETER
      NAME="ROWSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="ROW_SEPARATOR"
    >
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>
  
    <PARAMETER
      NAME="PATTERN"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PATTERN"
    >
      <DEFAULT>"5,4,5"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="HEADER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="HEADER"
    >
      <DEFAULT>0</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FOOTER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="FOOTER"
    >
      <DEFAULT>0</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LIMIT"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="LIMIT"
    >
      <DEFAULT/>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="5"
    >
      <DEFAULT/> 
    </PARAMETER>

    <PARAMETER
      NAME="REMOVE_EMPTY_ROW"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="5"
      REPOSITORY_VALUE="REMOVE_EMPTY_ROW"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="6"
      REQUIRED="true"
      REPOSITORY_VALUE="ENCODING"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
  </RETURNS>
