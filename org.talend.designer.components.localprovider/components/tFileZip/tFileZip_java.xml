<COMPONENT>
   <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"      
	COMPATIBILITY="ALL"
    AUTHOR="Talend"
 	RELEASE_DATE="20050320A"
    STARTABLE="true"
   >
   <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="SOURCE" FIELD="DIRECTORY" REQUIRED="true" NUM_ROW="1">
      <DEFAULT>"C:/zip"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="TARGET" FIELD="FILE" REQUIRED="true" NUM_ROW="2">
      <DEFAULT>"c:/zip/12345.zip"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="LEVEL" FIELD="CLOSED_LIST" REQUIRED="true" NUM_ROW="3">
      <ITEMS DEFAULT="9">
        <ITEM NAME="LEVEL0" VALUE="0" />
        <ITEM NAME="LEVEL1" VALUE="1" />
        <ITEM NAME="LEVEL2" VALUE="2" />
        <ITEM NAME="LEVEL3" VALUE="3" />
        <ITEM NAME="LEVEL4" VALUE="4" />
        <ITEM NAME="LEVEL5" VALUE="5" />
        <ITEM NAME="LEVEL6" VALUE="6" />
        <ITEM NAME="LEVEL7" VALUE="7" />
        <ITEM NAME="LEVEL8" VALUE="8" />
        <ITEM NAME="LEVEL9" VALUE="9" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="ALL_FILES" FIELD="CHECK" REQUIRED="true" NUM_ROW="4">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
     
    <PARAMETER NAME="MASK" FIELD="TABLE" REQUIRED="false" NUM_ROW="5" SHOW_IF="ALL_FILES == 'false'">
      <ITEMS>
     	<ITEM NAME="FILEMASK" />               
      </ITEMS>
    </PARAMETER>    
  </PARAMETERS>

  <CODEGENERATION/> 
  
  <RETURNS/>
  
