<!-- $Id: tFTP_perl.xml 1528 2007-01-19 17:03:36 +0000 (星期五, 19 一月 2007) smallet $ -->
<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
       SERIAL=""
      VERSION="0.101"
       STATUS="ALPHA"
         
COMPATIBILITY="ALL"
       AUTHOR="Talend"
 RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>
  <PARAMETERS>
    <PARAMETER NAME="HOST" FIELD="TEXT" REQUIRED="true" NUM_ROW="1">
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="PORT" FIELD="TEXT" REQUIRED="true" NUM_ROW="1">
      <DEFAULT>21</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true" NUM_ROW="2">
      <DEFAULT>"anonymous"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="PASSWORD" FIELD="TEXT" REQUIRED="true" NUM_ROW="2">
      <DEFAULT>"suomynona"</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="REMOTEDIR" FIELD="TEXT" REQUIRED="false" NUM_ROW="3">
      <DEFAULT>"."</DEFAULT>
    </PARAMETER>  
    <PARAMETER 
    	NAME="OVERWRITE" 
    	FIELD="CLOSED_LIST" 
    	NUM_ROW="3" >
    	<ITEMS DEFAULT="always">
     	<ITEM NAME="NEVER" VALUE="never" />
        <ITEM NAME="ALWAYS"  VALUE="always" />        
        <ITEM NAME="SIZE_DIFFER"  VALUE="size_differ" />        
      </ITEMS>
    </PARAMETER> 
    <PARAMETER NAME="FILES" FIELD="TABLE" REQUIRED="false" NUM_ROW="4" NB_LINES="4">
      <ITEMS>
        <ITEM NAME="FILEMASK" />
        <ITEM NAME="NEWNAME" />
      </ITEMS>
    </PARAMETER>
  </PARAMETERS>
  <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="Java-FTP" MODULE="edtftpj-1.5.4.jar" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>
  <RETURNS/>
</COMPONENT>