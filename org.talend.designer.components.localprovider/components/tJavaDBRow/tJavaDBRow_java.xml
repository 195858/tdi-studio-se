<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070326A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:JAVADB"
    />
    
    <PARAMETER
      NAME="FRAMEWORK_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="2"
    >
      <ITEMS DEFAULT="EMBEDED">
        <ITEM NAME="EMBEDED"    VALUE="EMBEDED" />
        <ITEM NAME="JCCJDBC"     VALUE="JCCJDBC"/>
        <ITEM NAME="DERBYCLIENT"     VALUE="DERBYCLIENT"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="CONNECTION_FLAG"
      FIELD="CHECK"
      NUM_ROW="2"
      NOT_SHOW_IF="FRAMEWORK_TYPE=='EMBEDED'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>          

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
      REQUIRED="true"
      NOT_SHOW_IF="FRAMEWORK_TYPE=='EMBEDED'"       
    >
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
      REQUIRED="true"
      NOT_SHOW_IF="FRAMEWORK_TYPE=='EMBEDED'"       
    >
      <DEFAULT>"1527"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="3"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >    
      <DEFAULT>JavaDB</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="SID"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DBPATH"
      FIELD="DIRECTORY"
      NUM_ROW="4"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>         
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="USERNAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="PASSWORD"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="QUERYSTORE"
      FIELD="QUERYSTORE_TYPE"
      NUM_ROW="7"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="8"
    >
      <DEFAULT>"select * from mytable"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="TEXT"
      NUM_ROW="9"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="COMMIT_EVERY"
      FIELD="TEXT"
      NUM_ROW="10"
      REQUIRED="true"
    >
      <DEFAULT>100</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="12"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="Driver-JavaDB" MODULE="derby.jar" REQUIRED="true" />
       <IMPORT NAME="Driver-JavaDB" MODULE="derbyclient.jar" REQUIRED="true" />
       <IMPORT NAME="Driver-JavaDB" MODULE="derbynet.jar" REQUIRED="true"/>
       <IMPORT NAME="Driver-JavaDB" MODULE="db2jcc.jar" REQUIRED="true"/>
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>