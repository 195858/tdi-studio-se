<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070326A"
    STARTABLE="true"
    DATA_AUTO_PROPAGATE="false"
    HAS_CONDITIONAL_OUTPUTS="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:JAVADB"
    />
    
    <PARAMETER
      NAME="FRAMEWORK_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="2"
      REPOSITORY_VALUE="FRAMEWORK_TYPE"
    >
      <ITEMS DEFAULT="EMBEDED">
        <ITEM NAME="EMBEDED"    VALUE="EMBEDED" />
        <ITEM NAME="JCCJDBC"     VALUE="JCCJDBC"/>
        <ITEM NAME="DERBYCLIENT"     VALUE="DERBYCLIENT"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="CONNECTION_FLAG"
      FIELD="CHECK"
      NUM_ROW="2"
      NOT_SHOW_IF="FRAMEWORK_TYPE=='EMBEDED'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>          

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
      REQUIRED="true"
      NOT_SHOW_IF="FRAMEWORK_TYPE=='EMBEDED'"       
    >
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
      REQUIRED="true"
      NOT_SHOW_IF="FRAMEWORK_TYPE=='EMBEDED'"       
    >
      <DEFAULT>"1527"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="3"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >    
      <DEFAULT>JavaDB</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="SID"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DBPATH"
      FIELD="DIRECTORY"
      NUM_ROW="4"
      REQUIRED="true"
      SHOW_IF="FRAMEWORK_TYPE=='EMBEDED'"
      REPOSITORY_VALUE="DIRECTORY"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>         
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="USERNAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="PASSWORD"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA_FLOW"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SCHEMA_REJECT"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="6"
      CONTEXT="REJECT"
      SHOW="true">
      <TABLE READONLY="true">
        <COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>    

	<PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="QUERYSTORE"
      FIELD="QUERYSTORE_TYPE"
      NUM_ROW="7"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="8"
    >
      <DEFAULT>"select * from mytable"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="9"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
    <PARAMETER
      NAME="PROPAGATE_RECORD_SET"
      FIELD="CHECK"
      NUM_ROW="5"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="RECORD_SET_COLUMN"
      FIELD="COLUMN_LIST"
      NUM_ROW="5"
      SHOW_IF="PROPAGATE_RECORD_SET == 'true'"
    />
  
  	<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="9"
		REQUIRED="true">
		<DEFAULT>"ISO-8859-15"</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="COMMIT_EVERY" FIELD="TEXT" NUM_ROW="9"
		REQUIRED="true">
		<DEFAULT>10000</DEFAULT>
	</PARAMETER>
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="Driver-JavaDB" MODULE="derby.jar" REQUIRED="true" />
       <IMPORT NAME="Driver-JavaDB" MODULE="derbyclient.jar" REQUIRED="true" />
       <IMPORT NAME="Driver-JavaDB" MODULE="derbynet.jar" REQUIRED="true"/>
       <IMPORT NAME="Driver-JavaDB" MODULE="db2jcc.jar" REQUIRED="true"/>
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>