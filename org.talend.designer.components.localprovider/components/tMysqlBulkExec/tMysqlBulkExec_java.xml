<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="RUN_IF" MAX_OUTPUT="1"/>    
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:MYSQL"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="2"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="2"
      REPOSITORY_VALUE="PORT"
    >
      <DEFAULT>"3306"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="2"
      REPOSITORY_VALUE="SID"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="USERNAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PASSWORD"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="4"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM NAME="NONE" VALUE="NONE" />
        <ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE"/>
        <ITEM NAME="CREATE" VALUE="CREATE"/>
        <ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
        <ITEM NAME="CLEAR" VALUE="CLEAR"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="4"
    >
      <DEFAULT>"employee"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="5"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/mysql_bulk.txt"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LINES_TERMINATED_BY"
      FIELD="TEXT"
      NUM_ROW="6"
      REQUIRED="true"
    >
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="FIELDS_TERMINATED_BY"
      FIELD="TEXT"
      NUM_ROW="6"
      REQUIRED="true"
    >
      <DEFAULT>";"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ESCAPED_BY"
      FIELD="TEXT"
      NUM_ROW="6"
      REQUIRED="true"
    >
      <DEFAULT>"\\"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="ENCLOSED_BY"
      FIELD="TEXT"
      NUM_ROW="6"
      REQUIRED="true"
    >
      <DEFAULT>"\""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DATA_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="7"
      REQUIRED="true"
    >
      <ITEMS DEFAULT="INSERT">
      	<ITEM NAME="INSERT" VALUE="INSERT" />
        <ITEM NAME="REPLACE" VALUE="REPLACE" />
        <ITEM NAME="UPDATE" VALUE="UPDATE" />
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="8"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>             

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="9"
      REQUIRED="true"
    >
      <DEFAULT>"UTF-8"</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="10"
      SHOW="false"
    >       
      <DEFAULT>mysql_id</DEFAULT>
    </PARAMETER>

   </PARAMETERS>

   <CODEGENERATION>
    <IMPORTS>
      <IMPORT
        NAME="Mysql_Driver"
        MODULE="mysql-connector-java-5.1.0-bin.jar"
        REQUIRED="true"
      />
    </IMPORTS>
  </CODEGENERATION>

   <RETURNS />
</COMPONENT>
