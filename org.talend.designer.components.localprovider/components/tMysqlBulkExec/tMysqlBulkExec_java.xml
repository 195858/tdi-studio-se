<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="RUN_ERROR" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="RUN_IF" MAX_OUTPUT="1"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:MYSQL"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
    >
      <DEFAULT>"3306"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SID"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="6"
    >
      <DEFAULT>"employee"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="7"
      REQUIRED="true"
    >
      <DEFAULT>"C:/talend_files/mysql_bulk.txt"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELDS_TERMINATED_BY"
      FIELD="TEXT"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>";"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="10"
      REQUIRED="true"
    >
      <DEFAULT>"UTF-8"</DEFAULT>
    </PARAMETER>

   </PARAMETERS>

   <CODEGENERATION>
    <IMPORTS>
      <IMPORT
        NAME="Mysql_Driver"
        MODULE="mysql-connector-java-5.1.0-bin.jar"
        REQUIRED="true"
      />
    </IMPORTS>
  </CODEGENERATION>

   <RETURNS />
</COMPONENT>
