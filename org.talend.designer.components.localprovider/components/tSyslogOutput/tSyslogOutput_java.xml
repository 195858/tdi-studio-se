<COMPONENT>
  <HEADER 
    AUTHOR="Talend" 
    COMPATIBILITY="ALL" 
    DATA_AUTO_PROPAGATE="false" 
    SCHEMA_AUTO_PROPAGATE="false"
    PLATEFORM="ALL" 
    RELEASE_DATE="20080229A" 
    SERIAL="" 
    STARTABLE="false" 
    STATUS="ALPHA" 
    VERSION="0.102"
  >
    <SIGNATURE/> 
  </HEADER> 
  
  <FAMILIES> 
    <FAMILY>File/Output</FAMILY> 
  </FAMILIES>  
  
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>
  
  <PARAMETERS>

    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="10" READONLY="true">
      <TABLE READONLY="true">
        <!-- <COLUMN NAME="facility" TYPE="id_String" LENGTH="255"/> -->
        <COLUMN NAME="TimeStamp" TYPE="id_String" LENGTH="255"/>
        <COLUMN NAME="Host" TYPE="id_String" LENGTH="255"/>
        <!-- <COLUMN NAME="level" TYPE="id_String" LENGTH="255"/> -->
        <COLUMN NAME="Program" TYPE="id_String" LENGTH="255"/>
        <COLUMN NAME="Pid" TYPE="id_String" LENGTH="255"/>
        <!-- <COLUMN NAME="msgid" TYPE="id_String" LENGTH="255"/> -->
        <COLUMN NAME="Text" TYPE="id_String" LENGTH="255"/>
      </TABLE>
    </PARAMETER>

    <PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="20" REQUIRED="true">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/syslog.log"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="NOTICE" FIELD="LABEL" NUM_ROW="30" REQUIRED="true">
      <DEFAULT>Notice: make sure number of input columns greater than or equal to 5</DEFAULT>
    </PARAMETER>
    
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
    <PARAMETER NAME="LOCALE" FIELD="text" NUM_ROW="1" REQUIRED="true">
      <DEFAULT>"en"</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>
  
  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="syslog" MODULE="syslog.jar" REQUIRED="true"/>
    </IMPORTS>
  </CODEGENERATION>
  
  <RETURNS/>

</COMPONENT>