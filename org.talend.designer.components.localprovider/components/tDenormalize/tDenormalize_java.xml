<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="4">
      <DEFAULT/>
    </PARAMETER>
    
    <PARAMETER
      NAME="DELIMITER"
      FIELD="TEXT"
      REQUIRED="true"
      NUM_ROW="5"
    >
      <DEFAULT>","</DEFAULT>
    </PARAMETER>    

    <PARAMETER
      NAME="MERGE"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="6"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER> 

    <PARAMETER
      NAME="GROUPS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="8"
      NB_LINES="5"
    >
      <ITEMS>
        <ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION>
    <TEMPLATES INPUT="Array" OUTPUT="DenormalizeIn">

      <TEMPLATE NAME="Array" COMPONENT="tArray">
        <LINK_TO NAME="DenormalizeIn" CTYPE="THEN_RUN"/>
      </TEMPLATE>
      
      <TEMPLATE NAME="DenormalizeIn" COMPONENT="tDenormalizeIn"/>
      
      <TEMPLATE_PARAM SOURCE="self.GROUPS" TARGET="DenormalizeIn.GROUPS"/>
      <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="DenormalizeIn.SCHEMA"/>
      <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="Array.SCHEMA"/>
      <TEMPLATE_PARAM SOURCE="self.UNIQUE_NAME" TARGET="Array.DESTINATION"/>
      <TEMPLATE_PARAM SOURCE="self.UNIQUE_NAME" TARGET="DenormalizeIn.ORIGIN" />
      <TEMPLATE_PARAM SOURCE="self.DELIMITER" TARGET="DenormalizeIn.DELIMITER" />
      <TEMPLATE_PARAM SOURCE="self.MERGE" TARGET="DenormalizeIn.MERGE" />
    </TEMPLATES>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
