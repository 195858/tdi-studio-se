<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="200100305A"
		STARTABLE="true"  SCHEMA_AUTO_PROPAGATE="false"
    	DATA_AUTO_PROPAGATE="false"
    	HAS_CONDITIONAL_OUTPUTS="true"
		>
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>Talend MDM</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

	   <PARAMETER
	      NAME="SCHEMA"
	      FIELD="SCHEMA_TYPE"
	      REQUIRED="true"
	      NUM_ROW="10"
	    >
	    </PARAMETER>
	    
		<PARAMETER NAME="MDMURL" FIELD="TEXT" NUM_ROW="20"
			REQUIRED="true">
			<DEFAULT>"http://localhost:8080/talend/TalendPort"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>"userName"</DEFAULT>
		</PARAMETER>	
			
		<PARAMETER NAME="PASSWORD" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="UNIVERSE" FIELD="TEXT"
			NUM_ROW="30">
			<DEFAULT></DEFAULT>
		</PARAMETER>
	
		<PARAMETER NAME="CONCEPT" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
	
		<PARAMETER NAME="DATACLUSTER" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>		
        
	  	<PARAMETER NAME="NOTE" FIELD="LABEL" NUM_ROW="50">
	    	<DEFAULT>Add all the keys in order below for the entity</DEFAULT>
	    </PARAMETER>
	    
	    <PARAMETER NAME="KEYS" FIELD="TABLE" NUM_ROW="51">
	    	<ITEMS>
	    		<ITEM NAME="KEY" FIELD="PREV_COLUMN_LIST" />
	    	</ITEMS>
	    </PARAMETER>
	    
	    <PARAMETER NAME="USE_DROP_ITEM" FIELD="CHECK" NUM_ROW="60">
	    	<DEFAULT>false</DEFAULT>
	    </PARAMETER>
	    
	    <PARAMETER NAME="PART_PATH" FIELD="TEXT" NUM_ROW="60" SHOW_IF="USE_DROP_ITEM=='true'">
	    	<DEFAULT>"/"</DEFAULT>
	    </PARAMETER>
		
	    <PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="70">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

	</PARAMETERS>
	
	<ADVANCED_PARAMETERS/>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="MDM-Webservice" MODULE="mdm_webservice.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-axis" MODULE="axis.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-jaxrpc" MODULE="jaxrpc.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-saaj" MODULE="saaj.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-wsdl" MODULE="wsdl4j-1.6.2.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-discovery" MODULE="commons-discovery-0.2.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-logging" MODULE="commons-logging-1.1.jar" REQUIRED="true" /> 
      <IMPORT NAME="SOAP-JavaMail" MODULE="mail.jar" REQUIRED="true" />  
      <IMPORT NAME="SOAP-JAF" MODULE="activation.jar" REQUIRED="true" />   
    </IMPORTS>
  </CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>