<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="INPUT_FILE"
      FIELD="FILE"
      NUM_ROW="10"
    >
      <DEFAULT>'__COMP_DEFAULT_FILE_DIR__/in.txt.asc'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="OUTPUT_FILE"
      FIELD="FILE"
      NUM_ROW="20"
    >
      <DEFAULT>'__COMP_DEFAULT_FILE_DIR__/in.txt'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="GPGBIN"
      FIELD="TEXT"
      NUM_ROW="30"
    >
      <DEFAULT>'/usr/bin/gpg'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SECRET_KEY"
      FIELD="TEXT"
      NUM_ROW="40"
    >
      <DEFAULT>'0x2B59D29E'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASSPHRASE"
      FIELD="TEXT"
      NUM_ROW="50"
    >
      <DEFAULT>'very secret passphrase'</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="FILE" TYPE="STRING" AVAILABILITY="FLOW"/>
    <RETURN NAME="FILEPATH" TYPE="STRING" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>