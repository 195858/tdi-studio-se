<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.1" STATUS="ALPHA"
		COMPATIBILITY="ALL" 
		AUTHOR="Pawel Maczka" 
		RELEASE_DATE="20081020A"
		STARTABLE="false"     
		SCHEMA_AUTO_PROPAGATE="true"
		DATA_AUTO_PROPAGATE="false"
	>
		<SIGNATURE></SIGNATURE>
	</HEADER>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MIN_INPUT="1" MAX_OUTPUT="0"/>
  	<CONNECTOR CTYPE="MERGE"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
  
  <PARAMETERS>
    
	<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="1" REPOSITORY_VALUE="EBCDIC" />
	
    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="20"
      REQUIRED="true"
      REPOSITORY_VALUE="FILE_PATH"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/data.ebcdic"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="COPYBOOK"
      FIELD="FILE"
      NUM_ROW="30"
      REQUIRED="true"
      REPOSITORY_VALUE="XC2J_FILE"
	>
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/copybook.xc2j"</DEFAULT>
    </PARAMETER>
            
    <PARAMETER
      NAME="HEADER"
      FIELD="TEXT"
      NUM_ROW="40"
    >
      <DEFAULT/>
    </PARAMETER>
    
    <PARAMETER
      NAME="FOOTER"
      FIELD="TEXT"
      NUM_ROW="40"
    >
      <DEFAULT/>
    </PARAMETER>
    
    <PARAMETER
      NAME="LIMIT"
      FIELD="TEXT"
      NUM_ROW="40"
    >
      <DEFAULT />
    </PARAMETER>
    
    <PARAMETER
      NAME="REMOVE_EMPTY_ROW"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="50"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
    <PARAMETER 
    	NAME="DIE_ON_ERROR" 
    	FIELD="CHECK" 
    	NUM_ROW="60">
	<DEFAULT>false</DEFAULT>
	</PARAMETER>

  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="10"
      REQUIRED="true"
      REPOSITORY_VALUE="ENCODING"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="RANDOM"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="20"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="NB_RANDOM"
      FIELD="TEXT"
      REQUIRED="true"
      NUM_ROW="20"
      SHOW_IF="RANDOM == 'true'"
    >
      <DEFAULT>10</DEFAULT>
    </PARAMETER>

  </ADVANCED_PARAMETERS>
  
  <CODEGENERATION>
  <IMPORTS>
	<IMPORT NAME="Talend File Enhanced" MODULE="talend_file_enhanced_20070724.jar" REQUIRED="true" />
	<IMPORT NAME="cobol2j.sf.net" MODULE="cobol2j-1.5.jar" REQUIRED="true" />
			<IMPORT NAME="cb2xml.sf.net" MODULE="cb2xml.jar"
				REQUIRED="true" />
			<IMPORT NAME="JAXB-api" MODULE="jaxb-api-2.2.jar"
				REQUIRED="true" />
			<IMPORT NAME="JAXB-impl" MODULE="jaxb-impl-2.1.8.jar"
				REQUIRED="true" />
			<IMPORT NAME="jsr173" MODULE="jsr173_1.0_api.jar"
				REQUIRED="true" />
			<IMPORT NAME="commons-lang" MODULE="commons-lang-2.4.jar"
				REQUIRED="true" />
			<IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar"
				REQUIRED="true" />	
  </IMPORTS>
  </CODEGENERATION>
  
  <RETURNS/>

</COMPONENT>