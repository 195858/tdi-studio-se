<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="9"
    >
      <DEFAULT/> 
    </PARAMETER>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:MYSQL"
    />

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="20"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tMysqlConnection"
      NUM_ROW="20"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="21"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>'localhost'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>'3306'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SID"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="USERNAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="60"
      REPOSITORY_VALUE="PASSWORD"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="61"
      REQUIRED="true"
    >
      <DEFAULT>'ISO-8859-15'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="70"
    >
      <DEFAULT>'employee'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELD_OPTIONS"
      FIELD="TABLE"
      REQUIRED="true"
      NUM_ROW="71"
      NB_LINES="5"
    >
      <ITEMS BASED_ON_SCHEMA="true">
        <ITEM NAME="UPDATE_KEY" VALUE="false" FIELD="CHECK" />
       </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="100"
      REQUIRED="true"
    >
      <DEFAULT>'/tmp/mysql_data.txt'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ROWSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="110"
      REPOSITORY_VALUE="ROW_SEPARATOR"
    >
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELDSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="120"
      REPOSITORY_VALUE="FIELD_SEPARATOR"
    >
      <DEFAULT>';'</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
    <TEMPLATES INPUT="tMOB" OUTPUT="tMUBE">

      <TEMPLATE NAME="tMOB" COMPONENT="tMysqlOutputBulk">
        <LINK_TO NAME="tMUBE" CTYPE="THEN_RUN"/>
      </TEMPLATE>
      
      <TEMPLATE NAME="tMUBE" COMPONENT="tMysqlUpdateBulkExec"/>

      <TEMPLATE_PARAM
        SOURCE="self.USE_EXISTING_CONNECTION"
        TARGET="tMUBE.USE_EXISTING_CONNECTION"
      />

      <TEMPLATE_PARAM
        SOURCE="self.CONNECTION"
        TARGET="tMUBE.CONNECTION"
      />

      <TEMPLATE_PARAM SOURCE="self.HOST"   TARGET="TMUBE.HOST" />
      <TEMPLATE_PARAM SOURCE="self.DBNAME" TARGET="TMUBE.DBNAME" />
      <TEMPLATE_PARAM SOURCE="self.PORT"   TARGET="TMUBE.PORT"   />
      <TEMPLATE_PARAM SOURCE="self.USER"   TARGET="TMUBE.USER" />
      <TEMPLATE_PARAM SOURCE="self.PASS"   TARGET="TMUBE.PASS" />
      <TEMPLATE_PARAM SOURCE="self.ENCODING" TARGET="TMUBE.ENCODING" />
      <TEMPLATE_PARAM SOURCE="self.TABLE"  TARGET="TMUBE.TABLE" />

      <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="tMOB.SCHEMA" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA" TARGET="tMUBE.SCHEMA" />

      <TEMPLATE_PARAM
        SOURCE="self.FIELD_OPTIONS"
        TARGET="tMUBE.FIELD_OPTIONS"
      />

      <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tMOB.FILENAME" />
      <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tMUBE.FILENAME" />

      <TEMPLATE_PARAM SOURCE="self.ROWSEPARATOR" TARGET="tMOB.ROWSEPARATOR" />

      <TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR"  TARGET="tMOB.FIELDSEPARATOR" />
      <TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR"  TARGET="tMUBE.FIELDS_TERMINATED_BY" />

    </TEMPLATES>
  </CODEGENERATION>
  <RETURNS/>
</COMPONENT>
