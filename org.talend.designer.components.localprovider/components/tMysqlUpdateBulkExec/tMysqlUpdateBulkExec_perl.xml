<COMPONENT>

	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="RUN_IF" MAX_OUTPUT="1" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="20" REPOSITORY_VALUE="DATABASE:MYSQL" />

		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="21">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"
			FILTER="tMysqlConnection" NUM_ROW="21"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'" />

		<PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="30"
			REPOSITORY_VALUE="SERVER_NAME"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>'localhost'</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="40"
			REPOSITORY_VALUE="PORT"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>'3306'</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DBNAME" FIELD="TEXT" NUM_ROW="50"
			REPOSITORY_VALUE="SID"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>''</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="60"
			REPOSITORY_VALUE="USERNAME"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>''</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASS" FIELD="TEXT" NUM_ROW="70"
			REPOSITORY_VALUE="PASSWORD"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>''</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="71"
			REQUIRED="true">
			<DEFAULT>'ISO-8859-15'</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TABLE" FIELD="TEXT" NUM_ROW="80">
			<DEFAULT>'employee'</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="90"
			REQUIRED="true">
			<DEFAULT>
				'__COMP_DEFAULT_FILE_DIR__/mysql_bulk.txt'
			</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FIELDS_TERMINATED_BY" FIELD="TEXT"
			NUM_ROW="100" REQUIRED="true">
			<DEFAULT>';'</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="110">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="FIELD_OPTIONS" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="120" NB_LINES="5">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="UPDATE_KEY" VALUE="false" FIELD="CHECK" />
			</ITEMS>
		</PARAMETER>

	</PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="DBI" MODULE="DBI" REQUIRED="true" />
			<IMPORT NAME="DBD-mysql" MODULE="DBD::mysql"
				REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS />
</COMPONENT>
