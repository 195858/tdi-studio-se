<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    EXTENSION="org.talend.designer.scd"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/DB2</FAMILY>
    <FAMILY>Business Intelligence/DB SCD</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MIN_INPUT="1" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER 
  	  NAME="USE_EXISTING_CONNECTION" 
  	  FIELD="CHECK"
	  NUM_ROW="5">
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER 
	  NAME="CONNECTION" 
	  FIELD="COMPONENT_LIST"
	  FILTER="tDB2Connection" 
	  NUM_ROW="5"
	  SHOW_IF="USE_EXISTING_CONNECTION == 'true'" 
	 />
    
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:IBM_DB2"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'" 
    />
	
    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"50000"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="31"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLESCHEMA"
      FIELD="TEXT"
      NUM_ROW="31"
      REPOSITORY_VALUE="SCHEMA"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="50"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="60"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="61">
         <DEFAULT/>  
    </PARAMETER>

    <PARAMETER
      NAME="SURROGATE_KEY"
      FIELD="COLUMN_LIST"
      NUM_ROW="62"
      SHOW="false"
    >
      <DEFAULT></DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SK_CREATION"
      FIELD="CLOSED_LIST"
      NUM_ROW="62"
      SHOW="false"
    >
      <ITEMS DEFAULT="AUTO_INCREMENT">
        <ITEM NAME="AUTO_INCREMENT"   VALUE="AUTO_INCREMENT" />
        <ITEM NAME="DB_SEQUENCE"      VALUE="DB_SEQUENCE"/>
        <ITEM NAME="INPUT_FIELD"      VALUE="INPUT_FIELD"/>
        <ITEM NAME="ROUTINE"          VALUE="ROUTINE"/>
        <ITEM NAME="TABLE_MAX"        VALUE="TABLE_MAX"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SK_DB_SEQUENCE"
      FIELD="TEXT"
      NUM_ROW="62"
      SHOW_IF="SK_CREATION == 'DB_SEQUENCE'"
      SHOW="false"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SK_INPUT_FIELD"
      FIELD="PREV_COLUMN_LIST"
      NUM_ROW="62"
      SHOW_IF="SK_CREATION == 'INPUT_FIELD'"
      SHOW="false"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SK_ROUTINE"
      FIELD="TEXT"
      NUM_ROW="62"
      SHOW_IF="SK_CREATION == 'ROUTINE'"
      SHOW="false"
    >
      <DEFAULT>Numeric.sequence("seq", 1, 1)</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SOURCE_KEYS"
      FIELD="TABLE"
      NUM_ROW="63"
      NB_LINES="2"
      SHOW="false"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="USE_SAVE_MEM_MODE"
      FIELD="CHECK"
      NUM_ROW="64"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USE_L0"
      FIELD="CHECK"
      NUM_ROW="68"
      SHOW="false"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L0_FIELDS"
      FIELD="TABLE"
      NUM_ROW="69"
      NB_LINES="5"
      SHOW_IF="USE_L0 == 'true'"
      SHOW="false"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="USE_L1"
      FIELD="CHECK"
      NUM_ROW="70"
      SHOW="false"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L1_FIELDS"
      FIELD="TABLE"
      NUM_ROW="71"
      NB_LINES="5"
      SHOW_IF="USE_L1 == 'true'"
      SHOW="false"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="ONLY_UPDATE_ACTIVE"
      FIELD="CHECK"
      NUM_ROW="72"
      SHOW_IF="(USE_L1 == 'true' and USE_L2 == 'true')"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>	

    <PARAMETER
      NAME="USE_L2"
      FIELD="CHECK"
      NUM_ROW="80"
      SHOW="false"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L2_FIELDS"
      FIELD="TABLE"
      NUM_ROW="81"
      NB_LINES="5"
      SHOW_IF="USE_L2 == 'true'"
      SHOW="false"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="L2_STARTDATE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="82"
      SHOW_IF="USE_L2 == 'true'"
      SHOW="false"
    >
    </PARAMETER>

    <PARAMETER
      NAME="L2_STARTDATE_VALUE"
      FIELD="CLOSED_LIST"
      NUM_ROW="82"
      SHOW_IF="USE_L2 == 'true'"
      SHOW="false"
    >
      <ITEMS DEFAULT="JOB_START_TIME">
        <ITEM NAME="JOB_START_TIME"   VALUE="JOB_START_TIME" />
        <ITEM NAME="INPUT_FIELD"      VALUE="INPUT_FIELD"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="L2_STARTDATE_INPUT_FIELD"
      FIELD="PREV_COLUMN_LIST"
      NUM_ROW="82"
      SHOW_IF="(USE_L2 == 'true' and L2_STARTDATE_VALUE == 'INPUT_FIELD')"
      SHOW="false"
    >
    </PARAMETER>

    <PARAMETER
      NAME="L2_ENDDATE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="83"
      SHOW_IF="USE_L2 == 'true'"
      SHOW="false"
    >
    </PARAMETER>

    <PARAMETER
      NAME="L2_ENDDATE_VALUE"
      FIELD="CLOSED_LIST"
      NUM_ROW="83"
      SHOW_IF="USE_L2 == 'true'"
      SHOW="false"
    >
      <ITEMS DEFAULT="NULL_VALUE">
        <ITEM NAME="NULL_VALUE"   VALUE="NULL_VALUE" />
        <ITEM NAME="FIXED_VALUE"  VALUE="FIXED_VALUE"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="L2_ENDDATE_FIXED_VALUE"
      FIELD="TEXT"
      NUM_ROW="83"
      SHOW_IF="(USE_L2 == 'true' and L2_ENDDATE_VALUE == 'FIXED_VALUE')"
      SHOW="false"
    >
      <DEFAULT>"2500"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USE_L2_ACTIVE"
      FIELD="CHECK"
      NUM_ROW="84"
      SHOW_IF="USE_L2 == 'true'"
      SHOW="false"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L2_ACTIVE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="84"
      SHOW_IF="(USE_L2_ACTIVE == 'true' and USE_L2 == 'true')"
      SHOW="false"
    >
    </PARAMETER>

    <PARAMETER
      NAME="USE_L2_VERSION"
      FIELD="CHECK"
      NUM_ROW="85"
      SHOW_IF="USE_L2 == 'true'"
      SHOW="false"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L2_VERSION_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="85"
      SHOW_IF="(USE_L2_VERSION == 'true' and USE_L2=='true')"
      SHOW="false"
    >
    </PARAMETER>

    <PARAMETER
    	NAME="NOTE"
    	FIELD="LABEL"
    	NUM_ROW="85"
		SHOW_IF="(USE_L2_VERSION == 'true' and USE_L2=='true')"
		SHOW="false"
    >
    	<DEFAULT>(Version must be an Integer)</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USE_L3"
      FIELD="CHECK"
      NUM_ROW="90"
      SHOW="false"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L3_FIELDS"
      FIELD="TABLE"
      NUM_ROW="100"
      NB_LINES="5"
      SHOW_IF="USE_L3 == 'true'"
      SHOW="false"
    >
      <ITEMS>
        <ITEM NAME="CURRENT_VALUE" FIELD="PREV_COLUMN_LIST" />
        <ITEM NAME="PREV_VALUE" FIELD="COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>
  </PARAMETERS>

  <ADVANCED_PARAMETERS>
  	<PARAMETER
      NAME="DEBUG"
      FIELD="CHECK"
      NUM_ROW="110"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>  

  <CODEGENERATION>
    <IMPORTS>
      	<IMPORT NAME="Driver-DB2" MODULE="db2jcc.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-DB2" MODULE="db2jcc_license_cu.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-DB2" MODULE="db2jcc_license_cisuz.jar" REQUIRED="true" />      
    </IMPORTS>
  </CODEGENERATION>
   
  <RETURNS>
    <RETURN NAME="NB_LINE_UPDATED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_INSERTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>