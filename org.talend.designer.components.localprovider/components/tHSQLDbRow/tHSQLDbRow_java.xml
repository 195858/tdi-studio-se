<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070326A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:HSQLDB"
    />
    
    <PARAMETER
      NAME="RUNNING_MODE"
      FIELD="CLOSED_LIST"
      NUM_ROW="2"
    >
      <ITEMS DEFAULT="HSQLDB_SERVER">
        <ITEM NAME="HSQLDB_SERVER" VALUE="HSQLDB_SERVER" />
        <ITEM NAME="HSQLDB_WEBSERVER" VALUE="HSQLDB_WEBSERVER"/>
        <ITEM NAME="HSQLDB_INPROGRESS_PERSISTENT" VALUE="HSQLDB_INPROGRESS_PERSISTENT"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="TLS"
      FIELD="CHECK"
      NUM_ROW="2"
      SHOW_IF="(RUNNING_MODE=='HSQLDB_SERVER' or RUNNING_MODE=='HSQLDB_WEBSERVER')"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>         

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="(RUNNING_MODE=='HSQLDB_SERVER' or RUNNING_MODE=='HSQLDB_WEBSERVER')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="(RUNNING_MODE=='HSQLDB_SERVER' or RUNNING_MODE=='HSQLDB_WEBSERVER')"
    >
      <DEFAULT>"9001"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="3"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>HSQLDb</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBPATH"
      FIELD="DIRECTORY"
      NUM_ROW="4"
      SHOW_IF="RUNNING_MODE=='HSQLDB_INPROGRESS_PERSISTENT'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="SID"
      SHOW_IF="RUNNING_MODE=='HSQLDB_INPROGRESS_PERSISTENT'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DATABASE_ALIAS"
      FIELD="TEXT"
      NUM_ROW="4"
      NOT_SHOW_IF="RUNNING_MODE=='HSQLDB_INPROGRESS_PERSISTENT'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER> 
             
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="USERNAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
       
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="PASSWORD"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="QUERYSTORE"
      FIELD="QUERYSTORE_TYPE"
      NUM_ROW="7"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="8"
    >
      <DEFAULT>"select * from mytable"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="TEXT"
      NUM_ROW="9"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="COMMIT_EVERY"
      FIELD="TEXT"
      NUM_ROW="10"
      REQUIRED="true"
    >
      <DEFAULT>100</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="12"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
     <IMPORTS>
		<IMPORT NAME="Driver-HSQLDb" MODULE="hsqldb.jar" REQUIRED="true"/>
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>