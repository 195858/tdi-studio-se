<COMPONENT>

	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070312A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>Business/Salesforce</FAMILY>
	<FAMILY>Cloud/Salesforce</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="ENDPOINT" FIELD="TEXT" NUM_ROW="10" REQUIRED="true">
			<DEFAULT>"https://www.salesforce.com/services/Soap/u/25.0"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="20" REQUIRED="true">
			<DEFAULT>"youremail@yourcompany.com"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASS" FIELD="TEXT" NUM_ROW="30" REQUIRED="true">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>
		<PARAMETER 
			NAME="TIMEOUT" 
			FIELD="TEXT"
			NUM_ROW="32"
			SHOW_IF="BULK_CONNECTION == 'false'">
			<DEFAULT>60000</DEFAULT>
		</PARAMETER>
		
		<PARAMETER 
	       NAME="BULK_CONNECTION" 
	       FIELD="CHECK" 
	       REQUIRED="true"
		   NUM_ROW="35">
		   <DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="API_VERSION" FIELD="TEXT" NUM_ROW="35" REQUIRED="true" SHOW_IF="BULK_CONNECTION=='true'">
			<DEFAULT>"25.0"</DEFAULT>
		</PARAMETER>
	
		<PARAMETER 
	       NAME="NEED_COMPRESSION" 
	       FIELD="CHECK" 
	       REQUIRED="true"
		   NUM_ROW="40">
		   <DEFAULT IF="(BULK_CONNECTION == 'true')">true</DEFAULT>
		   <DEFAULT IF="(BULK_CONNECTION == 'false')">false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER 
	       NAME="HTTP_TRACEMESSAGE" 
	       FIELD="CHECK" 
	       REQUIRED="true"
		   NUM_ROW="40"
		   SHOW_IF="BULK_CONNECTION == 'true'">
		   <DEFAULT>false</DEFAULT>
		</PARAMETER>
	
		<PARAMETER 
	       NAME="USE_PROXY" 
	       FIELD="CHECK" 
	       REQUIRED="true"
		   NUM_ROW="50" 
		   REPOSITORY_VALUE="UES_PROXY">
		   <DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER 
			NAME="PROXY_HOST" 
			FIELD="TEXT"
			NUM_ROW="60"
			SHOW_IF="USE_PROXY == 'true'" 
			REPOSITORY_VALUE="PROXY_HOST">
			<DEFAULT>"61.163.92.4"</DEFAULT>
		</PARAMETER>
	
		<PARAMETER 
			NAME="PROXY_PORT" 
			FIELD="TEXT"
			NUM_ROW="70" 
			SHOW_IF="USE_PROXY == 'true'" 
			REPOSITORY_VALUE="PROXY_PORT">
			<DEFAULT>"1080"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER 
			NAME="PROXY_USERNAME" 
			FIELD="TEXT" 
			NUM_ROW="80" 
			SHOW_IF="USE_PROXY == 'true'" 
			REPOSITORY_VALUE="PROXY_USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER 
			NAME="PROXY_PASSWORD" 
			FIELD="TEXT"
			NUM_ROW="90" 
			SHOW_IF="USE_PROXY == 'true'" 
			REPOSITORY_VALUE="PROXY_PASSWORD">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
  
		<PARAMETER 
			NAME="CLIENT_ID" 
			FIELD="TEXT"
			NUM_ROW="1" 
			SHOW_IF="BULK_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
	</ADVANCED_PARAMETERS>
  
