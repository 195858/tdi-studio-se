			<IMPORT NAME="axis2-kernel" MODULE="axis2-kernel-1.6.2.jar"
				REQUIRED="true" />
			<IMPORT NAME="axis2-transport-http" MODULE="axis2-transport-http-1.6.2.jar" REQUIRED="true" />
			<IMPORT NAME="axis2-transport-local" MODULE="axis2-transport-local-1.6.2.jar" REQUIRED="true" />
			<IMPORT NAME="commons-codec" MODULE="commons-codec-1.3.jar"
				REQUIRED="true" />
			<IMPORT NAME="commons-httpclient"
				MODULE="commons-httpclient-3.1.jar" REQUIRED="true" />
			<IMPORT NAME="commons-logging" MODULE="commons-logging-1.1.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="httpcore" MODULE="httpcore-4.0.jar"
				REQUIRED="true" />
			<IMPORT NAME="mail" MODULE="mail-1.4.jar"
				REQUIRED="true" />
			<IMPORT NAME="neethi" MODULE="neethi-3.0.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="wsdl4j" MODULE="wsdl4j-1.6.2.jar"
				BundleID="org.apache.servicemix.bundles.wsdl4j" REQUIRED="true" />
			<IMPORT NAME="XmlSchema" MODULE="xmlschema-core-2.0.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="axis2-adb" MODULE="axis2-adb-1.6.2.jar"
				REQUIRED="true" />
