<COMPONENT>
   <HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA" COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A" STARTABLE="false" SCHEMA_AUTO_PROPAGATE="true" VISIBLE="false">
      <SIGNATURE></SIGNATURE>
   </HEADER>

  <FAMILIES>
    <FAMILY>Technical</FAMILY>
  </FAMILIES>
   <DOCUMENTATION>
      <URL/>
   </DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK"/>
		<CONNECTOR CTYPE="COMPONENT_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
	</CONNECTORS>
   <PARAMETERS>
     <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
       <DEFAULT/> 
     </PARAMETER>
     
	<PARAMETER NAME="KEYS_MANAGEMENT" FIELD="CLOSED_LIST" NUM_ROW="50">
		<ITEMS DEFAULT="UNIQUE_MATCH">		
			<ITEM NAME="ALL_ROWS" VALUE="ALL_ROWS" />
			<ITEM NAME="UNIQUE_MATCH" VALUE="UNIQUE_MATCH" />
			<ITEM NAME="FIRST_MATCH" VALUE="FIRST_MATCH" />
			<!-- <ITEM NAME="LAST_MATCH" VALUE="LAST_MATCH" /> -->
			<ITEM NAME="ALL_MATCHES" VALUE="ALL_MATCHES	" />
		</ITEMS>
	</PARAMETER>

	<PARAMETER NAME="DATA_WRITE_MODEL" FIELD="CLOSED_LIST" NUM_ROW="60" >
		<ITEMS DEFAULT="PERSISTENT">
			<ITEM NAME="PERSISTENT" VALUE="PERSISTENT" />
			<ITEM NAME="MEMORY" VALUE="MEMORY" />
		</ITEMS>
	</PARAMETER>	
	
     <PARAMETER NAME="TEMPORARY_DATA_DIRECTORY" FIELD="DIRECTORY" NUM_ROW="70" GROUP="STORE_ON_DISK"
     SHOW_IF="(DATA_WRITE_MODEL == 'PERSISTENT')"
     >
     <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/temp"</DEFAULT>
    </PARAMETER>


	<PARAMETER NAME="ROWS_BUFFER_SIZE" FIELD="TEXT" REQUIRED="false" NUM_ROW="80" GROUP="STORE_ON_DISK"
	SHOW_IF="(DATA_WRITE_MODEL == 'PERSISTENT')"
	>
		<DEFAULT>2000000</DEFAULT>
	</PARAMETER>
	     
   </PARAMETERS>
   
   <CODEGENERATION>
    <IMPORTS>
      <IMPORT
        NAME="Advanced_Persistent_Lookup"
        MODULE="advancedPersistentLookupLib.jar"
        REQUIRED="true"
      />
      <IMPORT
        NAME="ApacheCommonsCollection32"
        MODULE="commons-collections-3.2.jar"
        REQUIRED="true"
      />
    </IMPORTS>
  </CODEGENERATION>
  
   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
