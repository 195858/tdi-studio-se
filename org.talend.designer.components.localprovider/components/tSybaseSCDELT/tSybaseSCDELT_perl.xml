<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
    SCHEMA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/Sybase</FAMILY>
    <FAMILY>Business_Intelligence/DB_SCD</FAMILY>
  </FAMILIES>
  
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MIN_INPUT="0" MAX_INPUT="1" MAX_OUTPUT="0" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>
  
  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:SYBASE"
    />

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tSybaseConnection"
      NUM_ROW="2"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="2"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>SybaseASE</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SERVER"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>'SYBASE'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="3"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>Sybase ASE</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>'sa'</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE_SRC"
      FIELD="TEXT"
      NUM_ROW="100"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="110"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="120"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM
          NAME="NONE"
          VALUE="NONE"
        />
        <ITEM
          NAME="DROP_CREATE"
          VALUE="DROP_CREATE"
        />
        <ITEM
          NAME="CREATE"
          VALUE="CREATE"
        />
        <ITEM
          NAME="CREATE_IF_NOT_EXISTS"
          VALUE="CREATE_IF_NOT_EXISTS"
        />
        <ITEM
          NAME="CLEAR"
          VALUE="CLEAR"
        />
        <ITEM
          NAME="TRUNCATE"
          VALUE="TRUNCATE"
        />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="130"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SOURCE_KEYS"
      FIELD="TABLE"
      NUM_ROW="140"
      NB_LINES="2"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="USE_L1"
      FIELD="CHECK"
      NUM_ROW="150"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="L1_FIELDS"
      FIELD="TABLE"
      NUM_ROW="160"
      NB_LINES="5"
      SHOW_IF="USE_L1 == 'true'"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="USE_L2"
      FIELD="CHECK"
      NUM_ROW="170"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="L2_FIELDS"
      FIELD="TABLE"
      NUM_ROW="180"
      NB_LINES="5"
      SHOW_IF="USE_L2 == 'true'"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="L2_STARTDATE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="190"
      SHOW_IF="USE_L2 == 'true'"
    >
    </PARAMETER>
    
    <PARAMETER
      NAME="L2_ENDDATE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="200"
      SHOW_IF="USE_L2 == 'true'"
    >
    </PARAMETER>
    
    <PARAMETER
      NAME="USE_L2_ACTIVE"
      FIELD="CHECK"
      NUM_ROW="210"
      SHOW_IF="USE_L2 == 'true'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="L2_ACTIVE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="220"
      SHOW_IF="USE_L2_ACTIVE == 'true'"
    >
    </PARAMETER>
    
    <PARAMETER
      NAME="USE_L2_VERSION"
      FIELD="CHECK"
      NUM_ROW="230"
      SHOW_IF="USE_L2 == 'true'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="L2_VERSION_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="250"
      SHOW_IF="USE_L2_VERSION == 'true'"
    >
    </PARAMETER>
    
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="10"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REQUIRED="true"
    >
      <DEFAULT>'ISO-8859-15'</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DEBUG"
      FIELD="CHECK"
      NUM_ROW="20"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
  
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="DBI" MODULE="DBI" REQUIRED="true" />
       <IMPORT NAME="DBD-Sybase" MODULE="DBD::Sybase" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
  
  <RETURNS />
</COMPONENT>