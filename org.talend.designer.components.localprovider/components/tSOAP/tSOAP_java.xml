<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>Internet</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="5">
			<TABLE READONLY="true">
				<COLUMN NAME="Header" TYPE="id_String" READONLY="true" />
				<COLUMN NAME="Body"	TYPE="id_String" READONLY="true" />
				<COLUMN NAME="Fault" TYPE="id_String" READONLY="true" />
			</TABLE>
		</PARAMETER>
		<PARAMETER NAME="USE_NTLM" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="NTLM_DOMAIN" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="10" SHOW_IF="USE_NTLM == 'true'">
			<DEFAULT>"192.168.0.100"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="NEED_AUTH" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="AUTH_USERNAME" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="20" SHOW_IF="NEED_AUTH == 'true'">
			<DEFAULT>"username"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="AUTH_PASSWORD" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="20" SHOW_IF="NEED_AUTH == 'true'">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_PROXY" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_HOST" FIELD="TEXT" 
			NUM_ROW="31" SHOW_IF="USE_PROXY == 'true'">
			<DEFAULT>"61.163.92.4"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PORT" FIELD="TEXT"
			NUM_ROW="32" SHOW_IF="USE_PROXY == 'true'">
			<DEFAULT>8080</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_USERNAME" FIELD="TEXT" 
			NUM_ROW="33" SHOW_IF="USE_PROXY == 'true'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PASSWORD" FIELD="TEXT"
			NUM_ROW="34" SHOW_IF="USE_PROXY == 'true'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="NEED_SSL_TO_TRUSTSERVER" FIELD="CHECK" REQUIRED="true" NUM_ROW="40">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SSL_TRUSTSERVER_TRUSTSTORE" FIELD="TEXT" NUM_ROW="45" SHOW_IF="NEED_SSL_TO_TRUSTSERVER == 'true'">
			<DEFAULT>"C:/clientTrustStore.trustStore"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SSL_TRUSTSERVER_PASSWORD" FIELD="TEXT" NUM_ROW="45" SHOW_IF="NEED_SSL_TO_TRUSTSERVER == 'true'">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>
		
		
		
		<PARAMETER NAME="ENDPOINT" FIELD="TEXT" REQUIRED="true" NUM_ROW="50">
			<DEFAULT>"http://www.deeptraining.com/webservices/weather.asmx"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="ACTION" FIELD="TEXT" REQUIRED="true" NUM_ROW="60">
			<DEFAULT>"http://litwinconsulting.com/webservices/GetWeather"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SOAP_VERSION" FIELD="CLOSED_LIST" NUM_ROW="70">
      		<ITEMS DEFAULT="SOAP_1.1">
        		<ITEM NAME="SOAP_1.1"   VALUE="Soap11" />
        		<ITEM NAME="SOAP_1.2"   VALUE="Soap12" />
        	</ITEMS>
    	</PARAMETER>	
		<PARAMETER NAME="SOAPMESSAGE" FIELD="MEMO" NUM_ROW="90" NB_LINES="10">
			<DEFAULT>""</DEFAULT>
  		</PARAMETER>
  		
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS/>
	

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="activation" MODULE="activation.jar" REQUIRED="true" />
			<IMPORT NAME="jaxqname" MODULE="jax-qname.jar" REQUIRED="true" />
			<IMPORT NAME="jaxpapi" MODULE="jaxp-api.jar" REQUIRED="true" />
			<IMPORT NAME="saajapi" MODULE="saaj-api.jar" REQUIRED="true" />
			<IMPORT NAME="saajimpl" MODULE="saaj-impl.jar" REQUIRED="true" />
			<IMPORT NAME="jdom" MODULE="jdom-1.1.jar" REQUIRED="true" />
			<IMPORT NAME="talendsoap" MODULE="talend-soap.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS/>
		
	
</COMPONENT>