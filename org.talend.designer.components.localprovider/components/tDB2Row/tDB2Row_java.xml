<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070326A"
    STARTABLE="true"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:IBM_DB2"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
    >
      <DEFAULT>"50000"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SID"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    	<PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="3"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>IBM DB2</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA_FLOW"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="5"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA_REJECT"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="5"
      CONTEXT="REJECT"
      SHOW="true">
      <TABLE READONLY="true">
        <COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER> 

	<PARAMETER
      NAME="DBTABLE"
      FIELD="DBTABLE"
      NUM_ROW="5"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="QUERYSTORE"
      FIELD="QUERYSTORE_TYPE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="7"
    >
      <DEFAULT>"select * from mytable"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="TEXT"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="COMMIT_EVERY"
      FIELD="TEXT"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>10000</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="8"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      	<IMPORT NAME="Driver-DB2" MODULE="db2jcc.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-DB2" MODULE="db2jcc_license_cu.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-DB2" MODULE="db2jcc_license_cisuz.jar" REQUIRED="true" />      
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>