	<PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="9"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
        
    <PARAMETER 
		NAME="TRIM_ALL_COLUMN" 
		FIELD="CHECK" 
		NUM_ROW="30">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER 
		NAME="TRIM_COLUMN" 
		FIELD="TABLE" 
		NUM_ROW="40"
		NB_LINES="5" 
		SHOW_IF="TRIM_ALL_COLUMN == 'false'">
		<ITEMS BASED_ON_SCHEMA="true">
			<ITEM NAME="TRIM" FIELD="CHECK"></ITEM>
		</ITEMS>
	</PARAMETER>		
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Driver-Hadoop-0.20.203" MODULE="hadoop-core-0.20.203.0.jar" REQUIRED="true" />
       		<IMPORT NAME="Driver-Commons-Log4j-1.2.15" MODULE="log4j-1.2.15.jar" REQUIRED="true" />
			<IMPORT NAME="Driver-Hive-Exec-0.7.1" MODULE="hive-exec-0.7.1.jar" REQUIRED="true" />
			<IMPORT NAME="Driver-Hive-Jdbc-0.7.1" MODULE="hive-jdbc-0.7.1.jar" REQUIRED="true" />
			<IMPORT NAME="Driver-Hive-Metastore-0.7.1" MODULE="hive-metastore-0.7.1.jar" REQUIRED="true" />
       		<IMPORT NAME="Driver-Hive-Service-0.7.1" MODULE="hive-service-0.7.1.jar" REQUIRED="true" />
       		<IMPORT NAME="Driver-LibFb303" MODULE="libfb303.jar" REQUIRED="true" />
			<IMPORT NAME="Driver-slf4j-api-1.6.1" MODULE="slf4j-api-1.6.1.jar" REQUIRED="true" />
			<IMPORT NAME="Driver-slf4j-log4j12-1.6.1" MODULE="slf4j-log4j12-1.6.1.jar" REQUIRED="true" />
       		<IMPORT NAME="Driver-Commons-Logging-1.1.1" MODULE="commons-logging-1.1.1.jar" REQUIRED="true" />
       		<IMPORT NAME="Driver-Commons-Logging-API-1.0.4" MODULE="commons-logging-api-1.0.4.jar" REQUIRED="true" />
     	</IMPORTS>
  </CODEGENERATION>

  <RETURNS>
   	<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>