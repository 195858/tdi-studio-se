<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
       SERIAL=""
      VERSION="0.101"
       STATUS="ALPHA"
COMPATIBILITY="ALL"
       AUTHOR="Talend"
 RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="POST"
      FIELD="CHECK"
      NUM_ROW="10"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER> 
    
    <PARAMETER
    	NAME="PRINT"
    	FIELD="CHECK"
    	NUM_ROW="10"
    	SHOW_IF="POST == 'true'"
    >
    </PARAMETER> 

    <PARAMETER
       NAME="URI"
       FIELD="TEXT"
       REQUIRED="true"
       NUM_ROW="20"
       SHOW_IF="POST == 'false'"
    >
      <DEFAULT>"http://talendforge.org/file_fetch.txt"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DIRECTORY"
      FIELD="DIRECTORY"
      NUM_ROW="30"
      REQUIRED="true"
      SHOW_IF="POST == 'false'"
    >
      <DEFAULT>"C:"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FILENAME"
      FIELD="TEXT"
      REQUIRED="false"
      NUM_ROW="40"
      SHOW_IF="POST == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
   
    <PARAMETER
      NAME="POSTURL"
      FIELD="TEXT"
      REQUIRED="false"
      NUM_ROW="50"
      SHOW_IF="POST == 'true'"
    >
      <DEFAULT>"http://127.0.0.1:8080/test/upload.jsp"</DEFAULT>
    </PARAMETER> 
              
	<PARAMETER 
		NAME="NEED_AUTH"
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="60"
		SHOW_IF="POST == 'true'"
	 >
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
    
    <PARAMETER
      NAME="AUTH_USERNAME"
      FIELD="TEXT"
      REQUIRED="false"
      NUM_ROW="60"
      SHOW_IF="(POST == 'true') AND (NEED_AUTH == 'true')"
    >
      <DEFAULT>"username"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="AUTH_PASSWORD"
      FIELD="TEXT"
      REQUIRED="false"
      NUM_ROW="60"
      SHOW_IF="(POST == 'true') AND (NEED_AUTH == 'true')"
    >
      <DEFAULT>"password"</DEFAULT>
    </PARAMETER> 
    
    <PARAMETER
      NAME="TIMEOUT"
      FIELD="TEXT"
      REQUIRED="false"
      NUM_ROW="70"
      SHOW_IF="POST == 'true'"
    >
      <DEFAULT>1000</DEFAULT>
    </PARAMETER>     
    
    <PARAMETER 
      NAME="COMMON_PARAMS" 
      FIELD="TABLE" NUM_ROW="80" 
      NB_LINES="8" 
      SHOW_IF="POST == 'true'"
    >
      <ITEMS>
        <ITEM NAME="COMMON_PARAMS_NAME" />
        <ITEM NAME="COMMON_PARAMS_VALUE"/>
      </ITEMS>
    </PARAMETER>
    
    
    </PARAMETERS>
    
    <ADVANCED_PARAMETERS>
    
    <PARAMETER
      NAME="UPLOAD"
      FIELD="CHECK"
      NUM_ROW="10"
      SHOW_IF="POST == 'true'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER> 
    
    <PARAMETER 
      NAME="FILE_PARAMS" 
      FIELD="TABLE" 
      NUM_ROW="20" 
      NB_LINES="8" 
      SHOW_IF="(POST=='true') AND (UPLOAD == 'true')"
    >
      <ITEMS>
        <ITEM NAME="FILE_PARAMS_NAME" />
        <ITEM NAME="FILE_PARAMS_VALUE"/>
      </ITEMS>
    </PARAMETER> 
    
    </ADVANCED_PARAMETERS>

  <CODEGENERATION>
   <IMPORTS>
       <IMPORT NAME="COMMONS-httpclient" MODULE="commons-httpclient-3.0.1.jar" REQUIRED="true" />
       <IMPORT NAME="COMMONS-Logging" MODULE="commons-logging-1.1.jar" REQUIRED="true" />
  	  <IMPORT NAME="COMMONS-codec" MODULE="commons-codec-1.3.jar" REQUIRED="true" />
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS/>

</COMPONENT>