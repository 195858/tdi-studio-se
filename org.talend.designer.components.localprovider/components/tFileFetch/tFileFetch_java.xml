<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
       SERIAL=""
      VERSION="0.101"
       STATUS="ALPHA"
COMPATIBILITY="ALL"
       AUTHOR="Talend"
 RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
  <PARAMETER NAME="POST" FIELD="CHECK" NUM_ROW="3" SHOW="true">
  <DEFAULT>true</DEFAULT>
  </PARAMETER>
  <PARAMETER
    	NAME="PRINT"
    	FIELD="CHECK"
    	NUM_ROW="5"
    	SHOW="true"
    >
    </PARAMETER>    
    
    <PARAMETER 
       NAME="USE_PROXY" 
       FIELD="CHECK" 
       REQUIRED="true"
	   NUM_ROW="10" 
	   REPOSITORY_VALUE="UES_PROXY">
	   <DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER 
		NAME="PROXY_HOST" 
		FIELD="TEXT"
		NUM_ROW="16"
		SHOW_IF="USE_PROXY == 'true'" 
		REPOSITORY_VALUE="PROXY_HOST">
		<DEFAULT>"61.163.92.4"</DEFAULT>
	</PARAMETER>
	
	<PARAMETER 
		NAME="PROXY_PORT" 
		FIELD="TEXT"
		NUM_ROW="17" 
		SHOW_IF="USE_PROXY == 'true'" 
		REPOSITORY_VALUE="PROXY_PORT">
		<DEFAULT>"8080"</DEFAULT>
	</PARAMETER>
		
	<PARAMETER 
		NAME="PROXY_USERNAME" 
		FIELD="TEXT" 
		NUM_ROW="18" 
		SHOW_IF="USE_PROXY == 'true'" 
		REPOSITORY_VALUE="PROXY_USERNAME">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	
	<PARAMETER 
		NAME="PROXY_PASSWORD" 
		FIELD="TEXT"
		NUM_ROW="19" 
		SHOW_IF="USE_PROXY == 'true'" 
		REPOSITORY_VALUE="PROXY_PASSWORD">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
    
    <PARAMETER
       NAME="URI"
       FIELD="TEXT"
       REQUIRED="true"
       NUM_ROW="20"
       SHOW="true"
    >
      <DEFAULT>"http://talendforge.org/file_fetch.txt"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DIRECTORY"
      FIELD="DIRECTORY"
      NUM_ROW="30"
      REQUIRED="true"     
    >
      <DEFAULT>"C:"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FILENAME"
      FIELD="TEXT"
      REQUIRED="true"
      NUM_ROW="40"    
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
              
	<PARAMETER 
		NAME="NEED_AUTH"
		FIELD="CHECK" 
		REQUIRED="true"
		NUM_ROW="50"
	 >
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
    
    <PARAMETER
      NAME="AUTH_USERNAME"
      FIELD="TEXT"
      REQUIRED="false"
      NUM_ROW="50"
      SHOW_IF="(NEED_AUTH == 'true')"
    >
      <DEFAULT>"username"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="AUTH_PASSWORD"
      FIELD="TEXT"
      REQUIRED="false"
      NUM_ROW="50"
      SHOW_IF=" (NEED_AUTH == 'true')"
    >
      <DEFAULT>"password"</DEFAULT>
    </PARAMETER>   
      
    
    <PARAMETER
      NAME="TIMEOUT"
      FIELD="TEXT"
      REQUIRED="false"
      NUM_ROW="70"
    >
      <DEFAULT>1000</DEFAULT>
    </PARAMETER>     
    
    <PARAMETER 
      NAME="COMMON_PARAMS" 
      FIELD="TABLE" NUM_ROW="80" 
      NB_LINES="10"
      SHOW_IF="POST == 'true'"
    >
      <ITEMS>
        <ITEM NAME="COMMON_PARAMS_NAME" />
        <ITEM NAME="COMMON_PARAMS_VALUE"/>
      </ITEMS>
    </PARAMETER>
    
    
    </PARAMETERS>
    
    <ADVANCED_PARAMETERS>
    
    
    <PARAMETER
      NAME="UPLOAD"
      FIELD="CHECK"
      NUM_ROW="10"
      SHOW_IF="POST == 'true'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER> 
    
    <PARAMETER 
      NAME="FILE_PARAMS" 
      FIELD="TABLE" 
      NUM_ROW="20" 
      NB_LINES="8"
	  SHOW_IF="(POST == 'true')AND(UPLOAD == 'true')"
    >
      <ITEMS>
        <ITEM NAME="FILE_PARAMS_NAME" />
        <ITEM NAME="FILE_PARAMS_VALUE"/>
      </ITEMS>
    </PARAMETER> 
    
    </ADVANCED_PARAMETERS>

  <CODEGENERATION>
   <IMPORTS>
       <IMPORT NAME="COMMONS-httpclient" MODULE="commons-httpclient-3.0.1.jar" REQUIRED="true" />
       <IMPORT NAME="COMMONS-Logging" MODULE="commons-logging-1.1.jar" REQUIRED="true" />
  	   <IMPORT NAME="COMMONS-codec" MODULE="commons-codec-1.3.jar" REQUIRED="true" />
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS/>

</COMPONENT>