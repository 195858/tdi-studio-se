<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070314A"
    STARTABLE="false"
    DATA_AUTO_PROPAGATE="false"
    HAS_CONDITIONAL_OUTPUTS="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:INTERBASE"
    />
    
	<PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="2"
      SHOW="true"
      REQUIRED="true"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>"Interbase"</DEFAULT>
    </PARAMETER>     
    
    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="2"
      REQUIRED="true"
      REPOSITORY_VALUE="SID"
    >
      <DEFAULT>"out.gdb"</DEFAULT>
    </PARAMETER>
	<PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="2"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>Interbase</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="TABLE"
      FIELD="DBTABLE"
      NUM_ROW="4"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="5"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM NAME="NONE" VALUE="NONE" />
        <ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE"/>
        <ITEM NAME="CREATE" VALUE="CREATE"/>
        <ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
        <ITEM NAME="CLEAR" VALUE="CLEAR"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="DATA_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="5"
    >
      <ITEMS DEFAULT="INSERT">
        <ITEM NAME="INSERT"           VALUE="INSERT" />
        <ITEM NAME="UPDATE"           VALUE="UPDATE"/>
        <ITEM NAME="INSERT_OR_UPDATE" VALUE="INSERT_OR_UPDATE"/>
        <ITEM NAME="UPDATE_OR_INSERT" VALUE="UPDATE_OR_INSERT"/>
        <ITEM NAME="DELETE"           VALUE="DELETE"/>
      </ITEMS>
    </PARAMETER>
        
    <PARAMETER
      NAME="SCHEMA_FLOW"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SCHEMA_REJECT"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="6"
      CONTEXT="REJECT"
      SHOW="true">
      <TABLE READONLY="true">
        <COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>    
    
    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="7"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
	<PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="10"
      SHOW="false"
    >       
      <DEFAULT>interbase_id</DEFAULT>
    </PARAMETER>    
    
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>
  	<PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="7"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ADD_COLS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="8"
      NB_LINES="3"
    >
      <ITEMS>

        <ITEM NAME="NAME" VALUE='"colname"' />
		<ITEM NAME="DATATYPE" VALUE="VARCHAR(50)" NOT_SHOW_IF="(TABLE_ACTION=='NONE') or (TABLE_ACTION=='CLEAR')"/>	
        <ITEM NAME="SQL" VALUE='"now()"' />

        <ITEM NAME="POS" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="BEFORE">
            <ITEM NAME="BEFORE" VALUE="BEFORE"/>
            <ITEM NAME="AFTER" VALUE="AFTER"/>
            <ITEM NAME="REPLACE" VALUE="REPLACE"/>
          </ITEMS>
        </ITEM>

        <ITEM NAME="REFCOL" FIELD="COLUMN_LIST" />

      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="COMMIT_EVERY"
      FIELD="TEXT"
      NUM_ROW="7"
      REQUIRED="true"
    >
      <DEFAULT>10000</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="Driver-Interbase" MODULE="interclient.jar" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>
   
  <RETURNS>
    <RETURN NAME="NB_LINE"          TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_UPDATED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_INSERTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_DELETED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>