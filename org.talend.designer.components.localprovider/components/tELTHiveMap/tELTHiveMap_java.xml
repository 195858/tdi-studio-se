<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20120802A"
		STARTABLE="true" EXTENSION="org.talend.designer.dbmap"
		SCHEMA_AUTO_PROPAGATE="false" DATA_AUTO_PROPAGATE="false"
		HAS_CONDITIONAL_OUTPUTS="true">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>ELT/Map/Hive</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="TABLE" BUILTIN="true" MIN_INPUT="1"
			MIN_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="2">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" FILTER="tHiveConnection"
			NUM_ROW="2" SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
			DYNAMIC_SETTINGS="true" />

		<PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="10">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="LINK_STYLE" FIELD="CLOSED_LIST" NUM_ROW="10">
			<ITEMS DEFAULT="">
				<ITEM NAME="AUTO" VALUE="AUTO" />
				<ITEM NAME="BEZIER_CURVE" VALUE="BEZIER_CURVE" />
				<ITEM NAME="LINE" VALUE="LINE" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="DISTRIBUTION" FIELD="CLOSED_LIST" NUM_ROW="25"
			REQUIRED="true" GROUP="VERSION" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<ITEMS DEFAULT="HORTONWORKS">
				<ITEM NAME="HORTONWORKS" VALUE="HORTONWORKS" />
				<ITEM NAME="APACHE" VALUE="APACHE" />
				<ITEM NAME="CLOUDERA" VALUE="CLOUDERA" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="HIVE_VERSION" FIELD="CLOSED_LIST" NUM_ROW="25"
			REQUIRED="true" GROUP="VERSION" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<ITEMS DEFAULT="HDP_1_0">
				<ITEM NAME="HDP_1_0" VALUE="HDP_1_0" SHOW_IF="DISTRIBUTION=='HORTONWORKS'" />
				<ITEM NAME="APACHE_0_20_203" VALUE="APACHE_0_20_203" SHOW_IF="DISTRIBUTION=='APACHE'" />
				<ITEM NAME="APACHE_1_0_0" VALUE="APACHE_1_0_0" SHOW_IF="DISTRIBUTION=='APACHE'" />
				<ITEM NAME="Cloudera_CDH4" VALUE="Cloudera_CDH4" SHOW_IF="DISTRIBUTION=='CLOUDERA'" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="CONNECTION_MODE" FIELD="CLOSED_LIST"
			NUM_ROW="30" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION=='false'">
			<ITEMS DEFAULT="EMBEDDED">
				<ITEM NAME="EMBEDDED" VALUE="EMBEDDED" NOT_SHOW_IF="HIVE_VERSION=='APACHE_0_20_203'" />
				<ITEM NAME="STANDALONE" VALUE="STANDALONE" NOT_SHOW_IF="HIVE_VERSION=='HDP_1_0'" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="40"
			REPOSITORY_VALUE="SERVER_NAME" REQUIRED="true"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="40"
			REPOSITORY_VALUE="PORT" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>"10000"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DBNAME" FIELD="TEXT" NUM_ROW="45"
			REPOSITORY_VALUE="SID" REQUIRED="true"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false' AND CONNECTION_MODE=='STANDALONE')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TYPE" FIELD="TEXT" NUM_ROW="50" SHOW="false"
			REPOSITORY_VALUE="TYPE">
			<DEFAULT>Hive</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="60"
			REPOSITORY_VALUE="USERNAME" REQUIRED="true"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASS" FIELD="TEXT" NUM_ROW="60"
			REPOSITORY_VALUE="PASSWORD" REQUIRED="true"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>


		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="70"
			REQUIRED="true" SHOW="false">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAPPING" FIELD="MAPPING_TYPE" NUM_ROW="70"
			SHOW="true" READONLY="true">
			<DEFAULT>hive_id</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PREVIEW" FIELD="IMAGE" SHOW="true" NUM_ROW="80">
			<DEFAULT />
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
	</CODEGENERATION>

	<RETURNS>
	</RETURNS>

	<PLUGINDEPENDENCIES>
		<PLUGINDEPENDENCY ID="org.talend.designer.abstractmap" />
	</PLUGINDEPENDENCIES>
</COMPONENT>
