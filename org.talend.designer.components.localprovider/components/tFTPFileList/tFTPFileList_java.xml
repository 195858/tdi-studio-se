<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.100" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Edouard GUERIN" RELEASE_DATE="20071027A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="HOST" FIELD="TEXT" REQUIRED="true" NUM_ROW="10">
			<DEFAULT>"localhost"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PORT" FIELD="TEXT" REQUIRED="true" NUM_ROW="10">
			<DEFAULT>21</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="20">
			<DEFAULT>"anonymous"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PASSWORD" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="20" NOT_SHOW_IF="(SFTP=='true') AND (AUTH_METHOD == 'PUBLICKEY')">
			<DEFAULT>"suomynona"</DEFAULT>
		</PARAMETER>
		
	    <PARAMETER
	      NAME="PRIVATEKEY"
	      FIELD="FILE"
	      NUM_ROW="21"
	      SHOW_IF="(SFTP=='true') AND (AUTH_METHOD == 'PUBLICKEY')"
	    >
	      <DEFAULT>"/.ssh/id_dsa"</DEFAULT>
	    </PARAMETER>
	
		<PARAMETER
	      NAME="PASSPHRASE"
	      FIELD="TEXT"
	      NUM_ROW="22"
	      SHOW_IF="(SFTP=='true') AND (AUTH_METHOD == 'PUBLICKEY')"
	    >
	      <DEFAULT>"secret"</DEFAULT>
	    </PARAMETER> 		
		
		<PARAMETER NAME="REMOTEDIR" FIELD="TEXT" REQUIRED="false"
			NUM_ROW="40">
			<DEFAULT>"."</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="DIR_FULL" FIELD="CHECK" NUM_ROW="50">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
	    <PARAMETER NAME="SFTP" FIELD="CHECK" NUM_ROW="80">
	      <DEFAULT>false</DEFAULT>
	    </PARAMETER>
	    
	    <PARAMETER
	      NAME="AUTH_METHOD"
	      FIELD="CLOSED_LIST"
	      NUM_ROW="80"
	      SHOW_IF="(SFTP=='true')"
	    >
	      <ITEMS DEFAULT="PASSWORD">
	        <ITEM NAME="PUBLICKEY"  VALUE="PUBLICKEY"/>
	        <ITEM NAME="PASSWORD"   VALUE="PASSWORD"/>
	      </ITEMS>
	    </PARAMETER>
	     		
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
	
		<PARAMETER NAME="UES_PROXY" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="85" REPOSITORY_VALUE="UES_PROXY">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	
		<PARAMETER NAME="PROXY_HOST" FIELD="TEXT" 
			NUM_ROW="86" SHOW_IF="UES_PROXY == 'true'" REPOSITORY_VALUE="PROXY_HOST">
			<DEFAULT>"61.163.92.4"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PORT" FIELD="TEXT"
			NUM_ROW="87" SHOW_IF="UES_PROXY == 'true'" REPOSITORY_VALUE="PROXY_PORT">
			<DEFAULT>"1080"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PROXY_USERNAME" FIELD="TEXT" 
			NUM_ROW="88" SHOW_IF="UES_PROXY == 'true'" REPOSITORY_VALUE="PROXY_USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PASSWORD" FIELD="TEXT"
			NUM_ROW="89" SHOW_IF="UES_PROXY == 'true'" REPOSITORY_VALUE="PROXY_PASSWORD">
			<DEFAULT>""</DEFAULT>
		</PARAMETER> 	  
	
	</ADVANCED_PARAMETERS>
	
	<CODEGENERATION>
		<IMPORTS>
	       <IMPORT NAME="Java-FTP" MODULE="edtftpj.jar" REQUIRED="true" />
	       <IMPORT NAME="Java-SFTP" MODULE="jsch-0.1.36.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>
	<RETURNS>
		<RETURN NAME="CURRENT_FILE" TYPE="id_String" AVAILABILITY="FLOW" />
		<RETURN NAME="CURRENT_FILEPATH" TYPE="id_String" AVAILABILITY="FLOW" />
		<RETURN NAME="NB_FILE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>