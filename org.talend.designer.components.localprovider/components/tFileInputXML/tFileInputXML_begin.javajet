	class XML_NS_RMV_<%=cid %>{
	
        public void removeNamespace(org.dom4j.Document reader, org.dom4j.Document writer) {
            org.dom4j.Element elemReader = reader.getRootElement();
            org.dom4j.Element elemTo = writer.addElement(elemReader.getName());
            elemTo.setText(elemReader.getText());
            for (org.dom4j.Attribute attri : (List<org.dom4j.Attribute>) elemReader.attributes()) {
                elemTo.add((org.dom4j.Attribute) attri.clone());
            }
            removeSubNamespace(elemReader, elemTo);
        }
    
        public void removeSubNamespace(org.dom4j.Element elemFrom, org.dom4j.Element elemTo) {
            for (org.dom4j.Element subFrom : (List<org.dom4j.Element>) elemFrom.elements()) {
                org.dom4j.Element tmpElemTo = elemTo.addElement(subFrom.getName());
                tmpElemTo.setText(subFrom.getText());
                for (org.dom4j.Attribute attri : (List<org.dom4j.Attribute>) subFrom.attributes()) {
                    tmpElemTo.add((org.dom4j.Attribute) attri.clone());
                }
                removeSubNamespace(subFrom, tmpElemTo);
            }
        }
	}
