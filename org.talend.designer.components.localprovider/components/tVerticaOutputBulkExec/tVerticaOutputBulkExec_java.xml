<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070316A"
    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="2"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="2"
      REPOSITORY_VALUE="PORT"
    >
      <DEFAULT>"5433"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="2"
      REPOSITORY_VALUE="SID"
    >
      <DEFAULT>"db name"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="USERNAME"
    >
      <DEFAULT>"root"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PASSWORD"
    >
      <DEFAULT>"password"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="4"
    >
      <DEFAULT>"table name"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="5"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="6"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.csv"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="FIELDSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="7"
      REPOSITORY_VALUE="FIELD_SEPARATOR"
    >
      <DEFAULT>";"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="NULL_STRING"
      FIELD="TEXT"
      NUM_ROW="7"
      REPOSITORY_VALUE="NULL_STRING"
    >
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER 
    	NAME="APPEND"
        FIELD="CHECK" 
        REQUIRED="true" 
        NUM_ROW="7" 
        REPOSITORY_VALUE="APPEND">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    
   	<PARAMETER 
   		NAME="INCLUDEHEADER" 
   		FIELD="CHECK" 
   		NUM_ROW="7">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER 
    	NAME="ENCODING" 
    	FIELD="ENCODING_TYPE" 
    	NUM_ROW="8" 
    	REQUIRED="true" 
    	REPOSITORY_VALUE="ENCODING">
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="9"
      SHOW="false"
    >       
      <DEFAULT>id_MSSQL</DEFAULT>
    </PARAMETER>
    
  </PARAMETERS>

  <CODEGENERATION>
    <TEMPLATES INPUT="tMOB" OUTPUT="tMBE">

      <TEMPLATE NAME="tMOB" COMPONENT="tVerticaOutputBulk">
        <LINK_TO NAME="tMBE" CTYPE="RUN_OK"/>
      </TEMPLATE>
      
      <TEMPLATE NAME="tMBE" COMPONENT="tVerticaBulkExec"/>  
      
      <TEMPLATE_PARAM SOURCE="self.FILENAME"   TARGET="tMOB.FILENAME" />
      <TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR"   TARGET="tMOB.FIELDSEPARATOR" />
      <TEMPLATE_PARAM SOURCE="self.APPEND"   TARGET="tMOB.APPEND" />
      <TEMPLATE_PARAM SOURCE="self.INCLUDEHEADER"   TARGET="tMOB.INCLUDEHEADER" />
      <TEMPLATE_PARAM SOURCE="self.ENCODING"   TARGET="tMOB.ENCODING" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA"   TARGET="tMOB.SCHEMA" />
      
      <TEMPLATE_PARAM SOURCE="self.HOST"   TARGET="tMBE.HOST" />
      <TEMPLATE_PARAM SOURCE="self.PORT"   TARGET="tMBE.PORT" />
      <TEMPLATE_PARAM SOURCE="self.DBNAME"   TARGET="tMBE.DBNAME" />
      <TEMPLATE_PARAM SOURCE="self.USER"   TARGET="tMBE.USER" />
      <TEMPLATE_PARAM SOURCE="self.PASS"   TARGET="tMBE.PASS" />
      <TEMPLATE_PARAM SOURCE="self.TABLE"   TARGET="tMBE.TABLE" />
      <TEMPLATE_PARAM SOURCE="self.SCHEMA"   TARGET="tMBE.SCHEMA" />
      <TEMPLATE_PARAM SOURCE="self.FILENAME"   TARGET="tMBE.REMOTE_FILENAME" />
      <TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR"   TARGET="tMBE.FIELDS_TERMINATED_BY" />
      <TEMPLATE_PARAM SOURCE="self.NULL_STRING"   TARGET="tMBE.MULL_STRING" />
      <TEMPLATE_PARAM SOURCE="self.MAPPING"   TARGET="tMBE.MAPPING" />
      
      

    </TEMPLATES>
  </CODEGENERATION>
  <RETURNS/>
</COMPONENT>
