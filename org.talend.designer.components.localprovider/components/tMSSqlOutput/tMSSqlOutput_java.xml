<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070327A"
		STARTABLE="false" DATA_AUTO_PROPAGATE="false">
		<SIGNATURE />
	</HEADER>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" />
		<CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1" />
		<CONNECTOR CTYPE="REFERENCE" />
		<CONNECTOR CTYPE="RUN_OK" />
		<CONNECTOR CTYPE="RUN_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER
	      NAME="USE_EXISTING_CONNECTION"
	      FIELD="CHECK"
	      NUM_ROW="1"
	    >
	      <DEFAULT>false</DEFAULT>
	    </PARAMETER>    
	
	    <PARAMETER
	      NAME="CONNECTION"
	      FIELD="COMPONENT_LIST"
	      FILTER="tMSSqlConnection"
	      NUM_ROW="1"
	      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
	    />
	
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="2" REPOSITORY_VALUE="DATABASE:SQL_SERVER" SHOW_IF="USE_EXISTING_CONNECTION == 'false'"/>

		<PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="3"
			REPOSITORY_VALUE="SERVER_NAME" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="3"
			REPOSITORY_VALUE="PORT" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>"1433"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DBNAME" FIELD="TEXT" NUM_ROW="4"
			REPOSITORY_VALUE="SID" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
<PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="3"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>MSSQL</DEFAULT>
    </PARAMETER>

		<PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="5"
			REPOSITORY_VALUE="USERNAME" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASS" FIELD="TEXT" NUM_ROW="5"
			REPOSITORY_VALUE="PASSWORD" REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TABLE" FIELD="DBTABLE" NUM_ROW="6"
			REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATA_ACTION" FIELD="CLOSED_LIST" NUM_ROW="7">
			<ITEMS DEFAULT="INSERT">
				<ITEM NAME="INSERT" VALUE="INSERT" />
				<ITEM NAME="UPDATE" VALUE="UPDATE" />
				<ITEM NAME="INSERT_OR_UPDATE" VALUE="INSERT_OR_UPDATE" />
				<ITEM NAME="UPDATE_OR_INSERT" VALUE="UPDATE_OR_INSERT" />
				<ITEM NAME="DELETE" VALUE="DELETE" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="CLEAR_TABLE" FIELD="CHECK" NUM_ROW="7">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		
		<PARAMETER NAME="SCHEMA_FLOW" FIELD="SCHEMA_TYPE" NUM_ROW="8">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
	    <PARAMETER
	      NAME="SCHEMA_REJECT"
	      FIELD="SCHEMA_TYPE"
	      REQUIRED="true"
	      NUM_ROW="8"
	      CONTEXT="REJECT"
	      SHOW="true">
	      <TABLE READONLY="true">
	        <COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
	        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
	      </TABLE>
	    </PARAMETER>		
		
		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="9"
			REQUIRED="true">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ADD_COLS" FIELD="TABLE" REQUIRED="false"
			NOT_SHOW_IF="(TABLE_ACTION=='DROP_CREATE' or TABLE_ACTION=='CREATE' or TABLE_ACTION=='CREATE_IF_NOT_EXISTS')"
			NUM_ROW="10" NB_LINES="3">
			<ITEMS>

				<ITEM NAME="NAME" VALUE='"colname"' />

        		 <ITEM NAME="SQL" VALUE='"now()"' />

				<ITEM NAME="POS" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="BEFORE">
						<ITEM NAME="BEFORE" VALUE="BEFORE" />
						<ITEM NAME="AFTER" VALUE="AFTER" />
						<ITEM NAME="REPLACE" VALUE="REPLACE" />
					</ITEMS>
				</ITEM>

				<ITEM NAME="REFCOL" FIELD="COLUMN_LIST" />

			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="COMMIT_EVERY" FIELD="TEXT" NUM_ROW="9"
			REQUIRED="true">
			<DEFAULT>10000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="9">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Driver-jTDS" MODULE="jtds-1.2.jar"
				REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="NB_LINE_UPDATED" TYPE="id_Integer"
			AVAILABILITY="AFTER" />
		<RETURN NAME="NB_LINE_INSERTED" TYPE="id_Integer"
			AVAILABILITY="AFTER" />
		<RETURN NAME="NB_LINE_DELETED" TYPE="id_Integer"
			AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>