			</ITEMS>
		</PARAMETER>
		
		<!--
		<PARAMETER NAME="HAS_SCHEMA_A" FIELD="CHECK" CONTEXT="A" REQUIRED="true" NUM_ROW="10"><DEFAULT>true</DEFAULT></PARAMETER>
