		<PARAMETER NAME="LOOP_PATH_A" FIELD="TEXT" CONTEXT="A" REQUIRED="false" NUM_ROW="11" SHOW_IF="(HAS_SCHEMA_A == 'true')" ><DEFAULT>"/header"</DEFAULT></PARAMETER>
		<PARAMETER NAME="MAPPING_A" CONTEXT="A" FIELD="TABLE" REQUIRED="true" NUM_ROW="12" NB_LINES="5" REPOSITORY_VALUE="XML_MAPPING" SHOW_IF="(HAS_SCHEMA_A == 'true')" >
