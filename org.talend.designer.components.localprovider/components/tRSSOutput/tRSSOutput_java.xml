<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>Internet</FAMILY>
  </FAMILIES>
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
   <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>
  <PARAMETERS>
    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="1"
      REQUIRED="true"
      REPOSITORY_VALUE="FILE_PATH"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.xml"</DEFAULT>
    </PARAMETER>
      <PARAMETER
      NAME="APPEND"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
     <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="2"
      REQUIRED="true"
      REPOSITORY_VALUE="ENCODING"
    >
      <DEFAULT>"UTF-8"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER 
    NAME="TITLE" 
    FIELD="TEXT" 
    REQUIRED="true"
    GROUP="CHANNEL"
    NUM_ROW="3"> 
    <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER 
    NAME="DESCRIPTION" 
    FIELD="TEXT" 
    REQUIRED="true"
    GROUP="CHANNEL"
    NUM_ROW="4"> 
    <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER 
    NAME="PUBDATE" 
    FIELD="TEXT" 
    REQUIRED="true"
    GROUP="CHANNEL"
    NUM_ROW="5"> 
    <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER 
    NAME="LINK" 
    FIELD="TEXT" 
    REQUIRED="true"
    GROUP="CHANNEL"
    NUM_ROW="6"> 
    <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="ELEMENTS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="7"
      NB_LINES="8"
    >
      <ITEMS>
        <ITEM NAME="ELEMENT_NAME" FIELD="String" />
        <ITEM NAME="ELEMENT_VALUE" FIELD="String"/>
      </ITEMS>
    </PARAMETER>    
    
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="8">
     </PARAMETER>
  </PARAMETERS>
 <CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="Java-DOM4J" MODULE="dom4j-1.6.1.jar" REQUIRED="true" />
        <IMPORT NAME="Java-JAXEN" MODULE="jaxen-1.1.1.jar" REQUIRED="true" />
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
  </RETURNS>

</COMPONENT>