<COMPONENT>

	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070314A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR NAME="SCHEMA_TARGET" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

	<SQLPATTERNS DB="Teradata">
		<SQLPATTERN NAME="Aggregate"/>
	</SQLPATTERNS>

	<PARAMETERS>
	
		<PARAMETER
	      NAME="CONNECTION"
	      FIELD="COMPONENT_LIST"
	      FILTER="tTeradataConnection"
	      NUM_ROW="2"
	    />

		<PARAMETER 
		  NAME="DATABASE_NAME" 
		  FIELD="TEXT" 
		  NUM_ROW="3"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER 
		  NAME="TABLE_NAME" 
		  FIELD="TEXT" 
		  NUM_ROW="4"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER 
		  NAME="SCHEMA" 
		  FIELD="SCHEMA_TYPE" 
		  NUM_ROW="4"
		>
			<DEFAULT>''</DEFAULT>
		</PARAMETER>
		
		<PARAMETER 
		  NAME="TABLE_NAME_TARGET" 
		  FIELD="TEXT" 
		  NUM_ROW="5"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER 
		  NAME="SCHEMA_TARGET" 
		  FIELD="SCHEMA_TYPE" 
		  NUM_ROW="5"
		  REQUIRED="true"
		  CONTEXT="SCHEMA_TARGET"
		>
			<DEFAULT>''</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
		  NAME="OPERATION"
		  FIELD="TABLE"
		  REQUIRED="false"
		  NUM_ROW="6"
		  NB_LINES="4"
		>
		  <ITEMS>
            <ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" CONTEXT="SCHEMA_TARGET"/>
			<ITEM NAME="FUNCTION" FIELD="CLOSED_LIST">
			  <ITEMS DEFAULT="COUNT">
				<ITEM NAME="COUNT"  VALUE="count"/>
				<ITEM NAME="MIN"    VALUE="min"/>
				<ITEM NAME="MAX"    VALUE="max"/>
				<ITEM NAME="AVG"    VALUE="avg"/>
				<ITEM NAME="SUM"    VALUE="sum"/>
				<ITEM NAME="FIRST"    VALUE="first"/>
				<ITEM NAME="LAST"    VALUE="last"/>
				<ITEM NAME="LIST"    VALUE="list"/>
				<ITEM NAME="DISTINCT"    VALUE="distinct"/>            
			  </ITEMS>
			</ITEM>
			<ITEM NAME="INPUT_COLUMN" FIELD="COLUMN_LIST" />
			<ITEM NAME="IGNORE_NULL" FIELD="CHECK" />
		  </ITEMS>
		</PARAMETER>
		
	    <PARAMETER
		  NAME="GROUPBY"
		  FIELD="TABLE"
		  NUM_ROW="7"
		  NB_LINES="4"
		>
		  <ITEMS>
            <ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" CONTEXT="SCHEMA_TARGET"/>
			<ITEM NAME="INPUT_COLUMN" FIELD="COLUMN_LIST" />
		  </ITEMS>
		</PARAMETER>

		<PARAMETER NAME="MAPPING" FIELD="MAPPING_TYPE" NUM_ROW="99"
			SHOW="false" READONLY="true">
			<DEFAULT>teradata_id</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<CODEGENERATION></CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW" />
	</RETURNS>
</COMPONENT>