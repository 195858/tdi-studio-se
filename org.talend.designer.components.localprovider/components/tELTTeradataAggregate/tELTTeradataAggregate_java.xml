<COMPONENT>

	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070314A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="TABLE" MAX_INPUT="0" />
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<SQLPATTERNS DB="Teradata"/>

	<PARAMETERS>
	
		<PARAMETER
	      NAME="CONNECTION"
	      FIELD="COMPONENT_LIST"
	      FILTER="tTeradataConnection"
	      NUM_ROW="2"
	    />
	    
	    <PARAMETER
	      NAME="STAGING_TABLE_CREATION"
	      FIELD="CHECK"
	      NUM_ROW="3"
	    >
	      <DEFAULT>false</DEFAULT>
	    </PARAMETER>  
	    
	    
		<PARAMETER 
		  NAME="ELT_TABLE_NAME" 
		  FIELD="TEXT" 
		  NUM_ROW="4"
		  SHOW_IF="STAGING_TABLE_CREATION == 'true'"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER
	      NAME="TABLE_ACTION"
	      FIELD="CLOSED_LIST"
	      NUM_ROW="4"
	      SHOW_IF="STAGING_TABLE_CREATION == 'true'"
	    >
	      <ITEMS DEFAULT="CREATE">
	        <ITEM NAME="CREATE" VALUE="CREATE"/>
	        <ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
	        <ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE"/>
	        <ITEM NAME="DROP_IF_EXISTS_AND_CREATE" VALUE="DROP_IF_EXISTS_AND_CREATE" />
	      </ITEMS>
	    </PARAMETER>

		<PARAMETER 
		  NAME="SCHEMA" 
		  FIELD="SCHEMA_TYPE" 
		  NUM_ROW="5"
		>
			<DEFAULT>''</DEFAULT>
		</PARAMETER>

		<PARAMETER 
		  NAME="DATABASE_NAME" 
		  FIELD="TEXT" 
		  NUM_ROW="6"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER 
		  NAME="TABLE_NAME" 
		  FIELD="TEXT" 
		  NUM_ROW="7"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER 
		  NAME="TABLE_NAME_TARGET" 
		  FIELD="TEXT" 
		  NUM_ROW="7"
		>
			<DEFAULT>""</DEFAULT>
		</PARAMETER>


		<PARAMETER
		  NAME="OPERATION"
		  FIELD="TABLE"
		  REQUIRED="false"
		  NUM_ROW="8"
		  NB_LINES="4"
		>
		  <ITEMS>
			<ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
			<ITEM NAME="FUNCTION" FIELD="CLOSED_LIST">
			  <ITEMS DEFAULT="COUNT">
				<ITEM NAME="COUNT"  VALUE="count"/>
				<ITEM NAME="MIN"    VALUE="min"/>
				<ITEM NAME="MAX"    VALUE="max"/>
				<ITEM NAME="AVG"    VALUE="avg"/>
				<ITEM NAME="SUM"    VALUE="sum"/>
				<ITEM NAME="FIRST"    VALUE="first"/>
				<ITEM NAME="LAST"    VALUE="last"/>
				<ITEM NAME="LIST"    VALUE="list"/>
				<ITEM NAME="DISTINCT"    VALUE="distinct"/>            
			  </ITEMS>
			</ITEM>
			<ITEM NAME="INPUT_COLUMN" FIELD="COLUMN_LIST" />
			<ITEM NAME="IGNORE_NULL" FIELD="CHECK" />
		  </ITEMS>
		</PARAMETER>
		
	    <PARAMETER
		  NAME="GROUPBY"
		  FIELD="TABLE"
		  NUM_ROW="9"
		  NB_LINES="4"
		>
		  <ITEMS>
			<ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
			<ITEM NAME="INPUT_COLUMN" FIELD="COLUMN_LIST" />
		  </ITEMS>
		</PARAMETER>

		<PARAMETER NAME="MAPPING" FIELD="MAPPING_TYPE" NUM_ROW="99"
			SHOW="false" READONLY="true">
			<DEFAULT>teradata_id</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<CODEGENERATION></CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW" />
	</RETURNS>
</COMPONENT>