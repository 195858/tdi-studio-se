<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070314A"
    STARTABLE="true"
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:ORACLE"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />
	
    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tOracleConnection"
      NUM_ROW="2"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />         
    
    <PARAMETER
      NAME="CONNECTION_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="3"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <ITEMS DEFAULT="ORACLE_SID">
        <ITEM NAME="ORACLE_SID"           VALUE="ORACLE_SID" />
        <ITEM NAME="ORACLE_SERVICE_NAME"  VALUE="ORACLE_SERVICE_NAME"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"1521"</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="SID"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
     <PARAMETER 
      NAME="SCHEMA_DB"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="SCHEMA"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="4"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>Oracle</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="USERNAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="PASSWORD"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="7"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SP_NAME"
      FIELD="TEXT"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>myfunction</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="IS_FUNCTION"
      FIELD="CHECK"
      NUM_ROW="9"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="RETURN"
      FIELD="COLUMN_LIST"
      NUM_ROW="9"
      SHOW_IF="IS_FUNCTION == 'true'"
    >
    </PARAMETER>
    
	<PARAMETER
      NAME="SP_ARGS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="10"
      NB_LINES="3"
    >
      <ITEMS>
        <ITEM NAME="COLUMN" FIELD="COLUMN_LIST"/>
        
        <ITEM NAME="TYPE" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="IN">
            <ITEM NAME="IN" VALUE="IN"/>
            <ITEM NAME="OUT" VALUE="OUT"/>
            <ITEM NAME="INOUT" VALUE="INOUT"/>
          </ITEMS>
        </ITEM>
      </ITEMS>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
		<IMPORT NAME="Driver-Oracle" MODULE="ojdbc14.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
   
  <RETURNS/>
</COMPONENT>
