<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
       SERIAL=""
      VERSION="0.101"
       STATUS="ALPHA"
         
COMPATIBILITY="ALL"
       AUTHOR="Talend"
 RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Logs_Errors</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1" READONLY="true">
      <TABLE>
        <COLUMN NAME="moment" TYPE="datetime"/>
        <COLUMN NAME="pid" TYPE="string" LENGTH="20"/>
        <COLUMN NAME="project" TYPE="string" LENGTH="50"/>
        <COLUMN NAME="job" TYPE="string" LENGTH="50"/>
        <COLUMN NAME="language" TYPE="string" LENGTH="5"/>
        <COLUMN NAME="origin" TYPE="string" LENGTH="255"/>
        <COLUMN NAME="status" TYPE="string" LENGTH="10"/>
        <COLUMN NAME="substatus" TYPE="string" LENGTH="255"/>
        <COLUMN NAME="description" TYPE="string" LENGTH="255"/>
      </TABLE>
    </PARAMETER>

   </PARAMETERS>

   <CODEGENERATION/>

   <RETURNS/>

</COMPONENT>
