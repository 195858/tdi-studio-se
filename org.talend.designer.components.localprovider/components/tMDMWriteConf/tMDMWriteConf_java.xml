<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20090320A"
		DATA_AUTO_PROPAGATE="false" SCHEMA_AUTO_PROPAGATE="false"
		TECHNICAL="true" STARTABLE="false"
		HAS_CONDITIONAL_OUTPUTS="true">
		<SIGNATURE></SIGNATURE>
	</HEADER>

  <FAMILIES>
    <FAMILY>Talend MDM</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" NOT_SHOW_IF="(DATA_ACTION == 'INSERT' OR DIE_ON_ERROR == 'true')"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="10">
			<DEFAULT />
		</PARAMETER>
 		
	    <PARAMETER
	      NAME="SCHEMA_REJECT"
	      FIELD="SCHEMA_TYPE"
	      REQUIRED="true"
	      NUM_ROW="11"
	      CONTEXT="REJECT"
	      SHOW="true">
	      <TABLE READONLY="true">
	        <COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
	        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
	      </TABLE>
	    </PARAMETER> 
	    
		<PARAMETER NAME="DESTINATION" FIELD="TEXT" NUM_ROW="12"
			REQUIRED="true" SHOW="false">
			<DEFAULT>tMDMOutputIn_1</DEFAULT>
		</PARAMETER>
		 
	    <PARAMETER
	      NAME="XMLFIELD"
	      FIELD="COLUMN_LIST"
	      REQUIRED="false"
	      NUM_ROW="15"
	    >
	    </PARAMETER>

		<PARAMETER NAME="MDMURL" FIELD="TEXT" NUM_ROW="20"
			REQUIRED="true">
			<DEFAULT>"http://localhost:8080/talend/TalendPort"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USERNAME" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>"userName"</DEFAULT>
		</PARAMETER>	
			
		<PARAMETER NAME="PASSWORD" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="30">
			<DEFAULT>"password"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="UNIVERSE" FIELD="TEXT"
			NUM_ROW="30">
			<DEFAULT></DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATAMODEL" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATACLUSTER" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

	    <PARAMETER NAME="RETURN_IDS" FIELD="TABLE" NUM_ROW="42" SHOW_IF="(EXTENDINSERT=='false') or (USE_PARTIAL_UPDATE=='true')">
	      <ITEMS>
	        <ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
	      </ITEMS>
	    </PARAMETER>

		<PARAMETER NAME="ISUPDATE" FIELD="CHECK" REQUIRED="true"
				 NUM_ROW="45" SHOW_IF="USE_PARTIAL_UPDATE=='false'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="WITHREPORT" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="50" SHOW_IF="USE_PARTIAL_UPDATE=='false'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="SOURCE" FIELD="TEXT" NUM_ROW="60" REQUIRED="true"  SHOW_IF="(WITHREPORT=='true') and (USE_PARTIAL_UPDATE=='false')">
			<DEFAULT>"TOS"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ISINVOKE" FIELD="CHECK" REQUIRED="true" SHOW_IF="(WITHREPORT=='true') and (USE_PARTIAL_UPDATE=='false')"
				 NUM_ROW="60">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="ADD_TASKID" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="70" SHOW_IF="USE_PARTIAL_UPDATE=='false'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="TASKID" FIELD="TEXT" NUM_ROW="80" REQUIRED="true"  SHOW_IF="(ADD_TASKID=='true') and (USE_PARTIAL_UPDATE=='false')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="USE_PARTIAL_UPDATE" FIELD="CHECK" REQUIRED="true" GROUP="PARTIAL_UPDATE" NUM_ROW="90" SHOW_IF="WITHREPORT=='false'">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PIVOT" FIELD="TEXT" NUM_ROW="100" REQUIRED="true" GROUP="PARTIAL_UPDATE" SHOW_IF="(WITHREPORT=='false') and (USE_PARTIAL_UPDATE=='true')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="OVERWRITE" FIELD="CHECK" REQUIRED="true" GROUP="PARTIAL_UPDATE" NUM_ROW="110" SHOW_IF="(WITHREPORT=='false') and (USE_PARTIAL_UPDATE=='true')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="KEY" FIELD="TEXT" NUM_ROW="120" GROUP="PARTIAL_UPDATE" SHOW_IF="(WITHREPORT=='false') and (USE_PARTIAL_UPDATE=='true')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="POSITION" FIELD="TEXT" NUM_ROW="130" GROUP="PARTIAL_UPDATE" SHOW_IF="(WITHREPORT=='false') and (USE_PARTIAL_UPDATE=='true')">
			<DEFAULT></DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="150">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>	
		<PARAMETER NAME="EXTENDINSERT" FIELD="CHECK" NUM_ROW="8"
			REQUIRED="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="COMMIT_LEVEL" FIELD="TEXT" NUM_ROW="9"
			REQUIRED="true" SHOW_IF="EXTENDINSERT == 'true'">
			<DEFAULT>200</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="STORE_FLOW" FIELD="CHECK"
			REQUIRED="false" NUM_ROW="100" SHOW='false'>
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="MDM-Webservice" MODULE="mdm_webservice.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-axis" MODULE="axis.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-jaxrpc" MODULE="jaxrpc.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-saaj" MODULE="saaj.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-wsdl" MODULE="wsdl4j-1.6.2.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-discovery" MODULE="commons-discovery-0.2.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-logging" MODULE="commons-logging-1.0.4.jar" REQUIRED="true" /> 
      <IMPORT NAME="SOAP-JavaMail" MODULE="mail.jar" REQUIRED="true" />  
      <IMPORT NAME="SOAP-JAF" MODULE="activation.jar" REQUIRED="true" />   
    </IMPORTS>
  </CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>