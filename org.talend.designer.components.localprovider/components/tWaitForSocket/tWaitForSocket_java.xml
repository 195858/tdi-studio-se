<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
    VISIBLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Orchestration</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER 
      NAME="PORT" 
      FIELD="TEXT" 
      REQUIRED="true"
      NUM_ROW="1"
    >
      <DEFAULT>2003</DEFAULT>
    </PARAMETER>

    <PARAMETER 
      NAME="EOL_CHAR" 
      FIELD="TEXT" 
      REQUIRED="true"
      NUM_ROW="2"
    >
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>

    <PARAMETER 
      NAME="OUTPUT" 
      FIELD="TEXT" 
      NUM_ROW="3"
    >
      <DEFAULT>"to client : $_"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="THEN"
      FIELD="CLOSED_LIST"
      REQUIRED="true"
      NUM_ROW="4"
    >
      <ITEMS DEFAULT="continue">
        <ITEM NAME="CONTINUE" VALUE="continue" />
        <ITEM NAME="EXIT_LOOP" VALUE="exitloop" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="PRINT"
      FIELD="CHECK"
      SHOW="true"
      NUM_ROW="5"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>


  </PARAMETERS>



  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="INPUT_DATA" TYPE="STRING" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>
