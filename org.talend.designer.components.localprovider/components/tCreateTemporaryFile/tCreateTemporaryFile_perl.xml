<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="REMOVE"
      FIELD="CHECK"
      NUM_ROW="10"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DIRECTORY"
      FIELD="FILE"
      NUM_ROW="20"
    >
      <DEFAULT>File:Spec->tmpdir</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TEMPLATE"
      FIELD="TEXT"
      NUM_ROW="30"
    >
      <DEFAULT>'talend_XXXX'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SUFFIX"
      FIELD="TEXT"
      NUM_ROW="40"
    >
      <DEFAULT>'.dat'</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="FILEPATH" TYPE="STRING" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>