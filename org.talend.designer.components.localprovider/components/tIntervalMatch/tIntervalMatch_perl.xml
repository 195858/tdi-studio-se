<COMPONENT>
   <HEADER 
       PLATEFORM="ALL" 
       SERIAL="" 
       VERSION="0.103" 
       STATUS="ALPHA" 
       COMPATIBILITY="WIN32" 
       AUTHOR="Talend" 
       RELEASE_DATE="20050320A" 
       STARTABLE="false"
       DATA_AUTO_PROPAGATE="false"
   >
      <SIGNATURE></SIGNATURE>
   </HEADER>
   <DOCUMENTATION>
      <URL/>
   </DOCUMENTATION>
   
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MIN_INPUT="1" MAX_INPUT="1" MIN_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="1"
    >
      <TABLE READONLY="false">
        <COLUMN NAME="LOOKUP" TYPE="string" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="2"
      REQUIRED="true"
      REPOSITORY_VALUE="FILE_PATH"
    >
      <DEFAULT>'__COMP_DEFAULT_FILE_DIR__/lookup.csv'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ROWSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="ROW_SEPARATOR"
    >
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELDSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="FIELD_SEPARATOR"
    >
      <DEFAULT>';'</DEFAULT>
    </PARAMETER>


    <PARAMETER
      NAME="LOOKUP_INDX"
      FIELD="TEXT"
      NUM_ROW="4"
      REQUIRED="true"
    >
      <DEFAULT>0</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="MATCH"
      FIELD="COLUMN_LIST"
      REQUIRED="true"
      NUM_ROW="4"
    >
    </PARAMETER>


  </PARAMETERS>

   <CODEGENERATION>
   </CODEGENERATION>

   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
