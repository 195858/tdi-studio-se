<!-- $Id: tFileZip_java.xml 1528 2007-01-19 17:03:36 +0000 (星期五, 19 一月 2007) smallet $ -->
<COMPONENT>
   <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"      
	COMPATIBILITY="ALL"
    AUTHOR="Talend"
 	RELEASE_DATE="20050320A"
    STARTABLE="true"
   >
   <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="SOURCE" FIELD="DIRECTORY" REQUIRED="true" NUM_ROW="1">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="SUB_DIRECTROY" FIELD="CHECK" REQUIRED="true" NUM_ROW="1">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="TARGET" FIELD="FILE" REQUIRED="true" NUM_ROW="2">
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/archive.zip"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="LEVEL" FIELD="CLOSED_LIST" REQUIRED="true" NUM_ROW="3">
      <ITEMS DEFAULT="4">
        <ITEM NAME="BEST" VALUE="9" />
        <ITEM NAME="NORMAL" VALUE="4" />
        <ITEM NAME="FAST" VALUE="0" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="ALL_FILES" FIELD="CHECK" REQUIRED="true" NUM_ROW="4">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
     
    <PARAMETER NAME="MASK" FIELD="TABLE" REQUIRED="false" NUM_ROW="5" SHOW_IF="ALL_FILES == 'false'">
      <ITEMS>
     	<ITEM NAME="FILEMASK" />               
      </ITEMS>
    </PARAMETER> 
    
    <PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="6" REQUIRED="true" REPOSITORY_VALUE="ENCODING">
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>   
  </PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="Ant-Zip" MODULE="ant.jar" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION> 
  
  <RETURNS/>
  
</COMPONENT>