<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070314A"
    STARTABLE="true"
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="false"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:JDBC"
    />
	
    <PARAMETER
      NAME="URL"
      FIELD="TEXT"
      NUM_ROW="2"
      REPOSITORY_VALUE="URL"
      REQUIRED="true"
    >
      <DEFAULT>"jdbc:"</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="DRIVER_JAR"
      FIELD="MODULE_LIST"
      NUM_ROW="3"
      REPOSITORY_VALUE="DRIVER_JAR"
      REQUIRED="true"
    >
      <DEFAULT></DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="DRIVER_CLASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="DRIVER_CLASS"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="4"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>JDBC</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="USERNAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="PASSWORD"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="7"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SP_NAME"
      FIELD="TEXT"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>myfunction</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="IS_FUNCTION"
      FIELD="CHECK"
      NUM_ROW="9"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="RETURN"
      FIELD="COLUMN_LIST"
      NUM_ROW="9"
      SHOW_IF="IS_FUNCTION == 'true'"
    >
    </PARAMETER>
    
	<PARAMETER
      NAME="SP_ARGS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="10"
      NB_LINES="3"
    >
      <ITEMS>
        <ITEM NAME="COLUMN" FIELD="COLUMN_LIST"/>
        
        <ITEM NAME="TYPE" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="IN">
            <ITEM NAME="IN" VALUE="IN"/>
            <ITEM NAME="OUT" VALUE="OUT"/>
            <ITEM NAME="INOUT" VALUE="INOUT"/>
          </ITEMS>
        </ITEM>
      </ITEMS>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION></CODEGENERATION>
   
  <RETURNS/>
</COMPONENT>
