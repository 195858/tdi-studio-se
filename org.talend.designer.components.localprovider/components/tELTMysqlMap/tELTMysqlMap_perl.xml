<COMPONENT>
   
     <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
     EXTENSION="org.talend.designer.dbmap.mysql" 
     SCHEMA_AUTO_PROPAGATE="false" 
     DATA_AUTO_PROPAGATE="false" 
     HAS_CONDITIONAL_OUTPUTS="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
  		<CONNECTOR BUILTIN="true" CTYPE="TABLE" MIN_INPUT="1" MIN_OUTPUT="1"/>
	    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
		<CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="REFERENCE"/>
		<CONNECTOR CTYPE="RUN_OK"/>
		<CONNECTOR CTYPE="RUN_ERROR"/>
		<CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>


      <PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="1">
         <DEFAULT/>  
      </PARAMETER>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="2"
      REPOSITORY_VALUE="DATABASE"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
    >
      <DEFAULT>'1521'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="TEXT"
      NUM_ROW="5"
      REQUIRED="true"
    >
      <DEFAULT>'ISO-8859-15'</DEFAULT>
    </PARAMETER>
 

      <PARAMETER NAME="PREVIEW" FIELD="IMAGE" SHOW="true" NUM_ROW="6">
         <DEFAULT/>
      </PARAMETER>

   </PARAMETERS>

   <CODEGENERATION>

     <IMPORTS>
       <IMPORT NAME="DBI" MODULE="DBI" REQUIRED="true" />
       <IMPORT NAME="DBD-Oracle" MODULE="DBD::Oracle" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
   </RETURNS>
   
</COMPONENT>
