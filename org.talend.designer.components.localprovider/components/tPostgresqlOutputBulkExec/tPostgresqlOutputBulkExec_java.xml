<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070320A"
    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="2"
      REPOSITORY_VALUE="DATABASE:POSTGRESQL"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
    >
      <DEFAULT>"5432"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SID"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="6"
    >
      <DEFAULT>"employee"</DEFAULT>
    </PARAMETER>

<!-- following property should not be displayed -->

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="14"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/temp.csv"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ROWSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="15"
      REPOSITORY_VALUE="ROW_SEPARATOR"
    >
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELDSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="16"
      REPOSITORY_VALUE="FIELD_SEPARATOR"
    >
      <DEFAULT>";"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
    <TEMPLATES INPUT="tPOB" OUTPUT="tPBE">

      <TEMPLATE NAME="tPOB" COMPONENT="tPostgresqlOutputBulk">
        <LINK_TO NAME="tPBE" CTYPE="BEFORE"/>
      </TEMPLATE>
      
      <TEMPLATE NAME="tPBE" COMPONENT="tPostgresqlBulkExec"/>
      
      <TEMPLATE_PARAM SOURCE="self.HOST"   TARGET="tPBE.HOST" />
      <TEMPLATE_PARAM SOURCE="self.DBNAME" TARGET="tPBE.DBNAME" />
      <TEMPLATE_PARAM SOURCE="self.PORT"   TARGET="tPBE.PORT"   />
      <TEMPLATE_PARAM SOURCE="self.USER"   TARGET="tPBE.USER" />
      <TEMPLATE_PARAM SOURCE="self.PASS"   TARGET="tPBE.PASS" />
      <TEMPLATE_PARAM SOURCE="self.TABLE"  TARGET="tPBE.TABLE" />

      <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tPOB.FILENAME" />
      <TEMPLATE_PARAM SOURCE="self.FILENAME" TARGET="tPBE.FILENAME" />

      <TEMPLATE_PARAM SOURCE="self.ROWSEPARATOR" TARGET="tPOB.ROWSEPARATOR" />

      <TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR"  TARGET="tPOB.FIELDSEPARATOR" />
      <TEMPLATE_PARAM SOURCE="self.FIELDSEPARATOR"  TARGET="tPBE.FIELDS_TERMINATED_BY" />

    </TEMPLATES>
  </CODEGENERATION>
  <RETURNS/>
</COMPONENT>
