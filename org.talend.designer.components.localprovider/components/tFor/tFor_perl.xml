<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="FROM" FIELD="TEXT" NUM_ROW="1" REQUIRED="true">
      <DEFAULT>1</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="TO" FIELD="TEXT" NUM_ROW="2" REQUIRED="true">
      <DEFAULT>10</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="STEP" FIELD="TEXT" NUM_ROW="3" REQUIRED="true">
      <DEFAULT>1</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="INCREASE" FIELD="CHECK" NUM_ROW="4">
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="CURRENT_VALUE" TYPE="INT" AVAILABILITY="FLOW"/>
    <RETURN NAME="CURRENT_ITERATION" TYPE="INT" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>
