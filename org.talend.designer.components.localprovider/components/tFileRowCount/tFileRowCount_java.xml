<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>File/Management</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="10"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.csv"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ROWSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="20"
      REQUIRED="true"
    >
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="IGNORE_EMPTY_ROW" FIELD="CHECK" NUM_ROW="25" REQUIRED="true">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="SUPPORT_EXCEL" FIELD="CHECK" NUM_ROW="30" REQUIRED="true">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="EXCEL_SHEETS" FIELD="TEXT" NUM_ROW="35" REQUIRED="true" SHOW_IF="SUPPORT_EXCEL == 'true'">
      <DEFAULT>"Sheet1"</DEFAULT>
    </PARAMETER>
    
  </PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="Java_Excel" MODULE="jxl.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="COUNT" TYPE="id_Integer" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>