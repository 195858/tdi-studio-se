<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070305A"
    STARTABLE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE"
    />

    <PARAMETER
      NAME="TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="2"
      REPOSITORY_VALUE="TYPE"
    >
      <ITEMS DEFAULT="mysql;mysql;MYSQL">
<!-- in the value, we give "Perl dbtype; Perl driver; Talend dbtype". -->
<!-- See org.talend.core/mappingMetadataTypes.xml for Talend dbtypes  -->
        <ITEM
          NAME="MYSQL"
          VALUE="mysql;org.gjt.mm.mysql.Driver;MYSQL;Mysql5.1"
          REPOSITORY_ITEM="MYSQL"
        />
        <ITEM
          NAME="PGSQL"
          VALUE="postgresql;org.postgresql.Driver;POSTGRESQL;postgres_id"
          REPOSITORY_ITEM="POSTGRESQL"
        />
        <ITEM
          NAME="ORACLE"
          VALUE="oracle;oracle.jdbc.driver.OracleDriver;Oracle with SID;oracle_id"
          REPOSITORY_ITEM="ORACLE"
        />
        <ITEM
          NAME="ODBC"
          VALUE="undef;sun.jdbc.odbc.JdbcOdbcDriver;MICROSOFT SQL SERVER (ODBC DRIVER);MSODBC"
          REPOSITORY_ITEM="ODBC"
        />
        <ITEM
          NAME="MSSQL_ODBC"
          VALUE="mssql;sun.jdbc.odbc.JdbcOdbcDriver;MICROSOFT SQL SERVER (ODBC DRIVER);MSODBC"
          REPOSITORY_ITEM="ODBC_JDBC"
        />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
      NOT_SHOW_IF="(TYPE=='MSSQL_ODBC' or TYPE=='ODBC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
      NOT_SHOW_IF="(TYPE=='MSSQL_ODBC' or TYPE=='ODBC')"
    >
      <DEFAULT>""</DEFAULT>
      <DEFAULT IF="TYPE=='MYSQL'">"3306"</DEFAULT>
      <DEFAULT IF="TYPE=='PGSQL'">"5432"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="5"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="6"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM
          NAME="NONE"
          VALUE="NONE"
        />
        <ITEM
          NAME="DROP_CREATE_TABLE"
          VALUE="DROP_CREATE_TABLE"
          NOT_SHOW_IF="TYPE=='ODBC'"
        />
        <ITEM
          NAME="CREATE_TABLE"
          VALUE="CREATE_TABLE"
          NOT_SHOW_IF="TYPE=='ODBC'"
        />
        <ITEM
          NAME="CLEAR_TABLE"
          VALUE="CLEAR_TABLE"
        />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="DATA_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="6"
    >
      <ITEMS DEFAULT="INSERT">
        <ITEM NAME="INSERT"           VALUE="INSERT" />
        <ITEM NAME="UPDATE"           VALUE="UPDATE"/>
        <ITEM NAME="INSERT_OR_UPDATE" VALUE="INSERT_OR_UPDATE"/>
        <ITEM NAME="UPDATE_OR_INSERT" VALUE="UPDATE_OR_INSERT"/>
        <ITEM NAME="DELETE"           VALUE="DELETE"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="7"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

  <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="8"
      REQUIRED="true">
       <DEFAULT>'ISO-8859-15'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ADD_COLS"
      FIELD="TABLE"
      REQUIRED="false"
      NOT_SHOW_IF="(ACTION=='DROP_CREATE_TABLE' or ACTION=='CREATE_TABLE')"
      NUM_ROW="9"
      NB_LINES="3"
    >
      <ITEMS>

        <ITEM NAME="NAME" VALUE="'colname'" />

        <ITEM NAME="SQL" VALUE='"to_date(?, &apos;mm/dd/yyyy&apos;)"' />

        <ITEM NAME="POS" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="BEFORE">
            <ITEM NAME="BEFORE" VALUE="BEFORE"/>
            <ITEM NAME="AFTER" VALUE="AFTER"/>
            <ITEM NAME="REPLACE" VALUE="REPLACE"/>
          </ITEMS>
        </ITEM>

        <ITEM NAME="REFCOL" FIELD="COLUMN_LIST" />

      </ITEMS>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="Driver-MySql" MODULE="mysql-connector-java-3.1.13-bin.jar" REQUIRED="true" />
       <IMPORT NAME="Driver-Oracle" MODULE="ojdbc14.jar" REQUIRED="false" />
       <IMPORT NAME="Driver-Postgres" MODULE="postgresql-8.1-405.jdbc3.jar" REQUIRED="false" />
     </IMPORTS>
  </CODEGENERATION>
   
  <RETURNS>
    <RETURN NAME="NB_LINE"          TYPE="INTEGER" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_UPDATED"  TYPE="INTEGER" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_INSERTED" TYPE="INTEGER" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_DELETED"  TYPE="INTEGER" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>