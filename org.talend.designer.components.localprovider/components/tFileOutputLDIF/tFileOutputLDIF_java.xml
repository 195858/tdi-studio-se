<!-- $Id: tFileOutputLDIF.xml 291 2006-11-01 07:34:12Z nrousseau $ -->
<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="20"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.ldif"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="WRAP"
      FIELD="TEXT"
      NUM_ROW="30"
    >
      <DEFAULT>78</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CHANGETYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="40"
    >
      <ITEMS DEFAULT="modify">
        <ITEM NAME="ADD"    VALUE="add"    />
        <ITEM NAME="MODIFY" VALUE="modify" />
        <ITEM NAME="DELETE" VALUE="delete" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="CHANGES"
      FIELD="CLOSED_LIST"
      NUM_ROW="40"
      SHOW_IF="CHANGETYPE=='MODIFY'"
    >
      <ITEMS DEFAULT="add">
        <ITEM NAME="ADD"     VALUE="add"     />
        <ITEM NAME="REPLACE" VALUE="replace" />
        <ITEM NAME="DELETE"  VALUE="delete"  />
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="CREATE"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="45"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="FLUSHONROW" FIELD="CHECK" REQUIRED="true" NUM_ROW="48">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>         

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="50"
    >
      <DEFAULT/> 
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="60"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>

</COMPONENT>