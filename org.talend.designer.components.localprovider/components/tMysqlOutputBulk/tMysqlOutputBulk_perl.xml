<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="10"
      REQUIRED="true"
    >
      <DEFAULT>'__COMP_DEFAULT_FILE_DIR__/out.csv'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ROWSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="20"
    >
      <DEFAULT>"\n"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELDSEPARATOR"
      FIELD="TEXT"
      NUM_ROW="30"
    >
      <DEFAULT>';'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="APPEND"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="40"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="50"
    >
      <DEFAULT/> 
    </PARAMETER>

   </PARAMETERS>

   <CODEGENERATION/>

   <RETURNS>
     <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>