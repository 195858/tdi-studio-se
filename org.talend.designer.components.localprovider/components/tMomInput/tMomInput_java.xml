<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>Internet/MOM and JMS</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="KEEPLISTENING"
      FIELD="CHECK"
      SHOW="true"
      NUM_ROW="10"
      SHOW_IF="(SERVER == 'JBOSS') OR (SERVER == 'WEBSPHERE') OR (SERVER == 'ACTIVEMQ' AND USEMAX == 'false')"
    >
    <DEFAULT IF="(SERVER == 'JBOSS')">true</DEFAULT>
    <DEFAULT IF="(SERVER == 'WEBSPHERE')">false</DEFAULT>
    <DEFAULT IF="(SERVER == 'ACTIVEMQ')">false</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="STARTSERVER"
      FIELD="CHECK"
      SHOW_IF="SERVER == 'ACTIVEMQ'"
      NUM_ROW="60"
    >
    <DEFAULT >false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USEMAX"
      FIELD="CHECK"
      SHOW_IF="(SERVER == 'ACTIVEMQ')AND (KEEPLISTENING == 'false')"
      NUM_ROW="15"
    >
    <DEFAULT >false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TIMEOUT"
      FIELD="TEXT"
      SHOW_IF="(SERVER == 'JBOSS' and KEEPLISTENING == 'TRUE')or (SERVER == 'ACTIVEMQ' and KEEPLISTENING == 'false' and USEMAX == 'false')"
      NUM_ROW="12"
    >
    <DEFAULT>1</DEFAULT>
    </PARAMETER>

<PARAMETER
      NAME="MAXMSG"
      FIELD="TEXT"
      SHOW_IF="(SERVER == 'ACTIVEMQ' and KEEPLISTENING == 'false' and USEMAX == 'true')"
      NUM_ROW="17"
    >
    <DEFAULT>1000</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SERVER"
      FIELD="CLOSED_LIST"
      NUM_ROW="25"
    >
      <ITEMS DEFAULT="WEBSPHERE">
        <ITEM NAME="WEBSPHERE"   VALUE="WebSphere" />
        <ITEM NAME="JBOSS"   VALUE="JBoss" />
        <ITEM NAME="ACTIVEMQ" VALUE="ActiveMQ"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SERVERADDRESS"
      FIELD="TEXT"
      NUM_ROW="25"
    >
	<DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SERVERPORT"
      FIELD="TEXT"
      NUM_ROW="25"
    >
	<DEFAULT IF="(SERVER == 'JBOSS')">"1099"</DEFAULT>
	<DEFAULT IF="(SERVER == 'WEBSPHERE')">"1414"</DEFAULT>
	<DEFAULT IF="(SERVER == 'ACTIVEMQ')">"61616"</DEFAULT>
    </PARAMETER>

	<PARAMETER NAME="MQ_DERVIERS" FIELD="TEXT" NUM_ROW="30" SHOW="false">
		<DEFAULT IF="(SERVER == 'JBOSS')">jbossall-client.jar</DEFAULT>
		<DEFAULT IF="(SERVER == 'WEBSPHERE')">com.ibm.mq.jar;com.ibm.mqjms.jar;websphere-connector-1_5.jar</DEFAULT>
		<DEFAULT IF="(SERVER == 'ACTIVEMQ')">activemq-all-5.1.0.jar</DEFAULT>
	</PARAMETER>
    
   <PARAMETER
      NAME="FROM"
      FIELD="TEXT"
      SHOW="true"
      NUM_ROW="35"
      SHOW_IF="(SERVER == 'JBOSS') or (SERVER == 'ACTIVEMQ')"
    ><DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="MSGTYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="35"
      SHOW_IF="(SERVER == 'JBOSS')or (SERVER == 'ACTIVEMQ')"
    >
      <ITEMS DEFAULT="TOPIC">
        <ITEM NAME="TOPIC"   VALUE="Topic" />
        <ITEM NAME="QUEUE"   VALUE="Queue" />
      </ITEMS>
    </PARAMETER>
    
  <PARAMETER
      NAME="CHANNEL"
      FIELD="TEXT"
      NUM_ROW="35"
      SHOW_IF="(SERVER == 'WEBSPHERE')" >
   	<DEFAULT>"DC.SVRCONN"</DEFAULT>
    </PARAMETER>
    
    
        <PARAMETER
      NAME="MESSAGE_BODY_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="40"
    >
      <ITEMS DEFAULT="TEXT">
        <ITEM NAME="TEXT"   VALUE="Text" />
        <ITEM NAME="BYTES"   VALUE="Bytes" />
        <ITEM NAME="MAP"   VALUE="Map" />
      </ITEMS>
    </PARAMETER>
    

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="45"
    >
      <TABLE IF="((SERVER == 'JBOSS') or (SERVER == 'ACTIVEMQ')) AND (IS_USE_MESSAGE_ID == 'false')" READONLY="false">
        <COLUMN NAME="from" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
        <COLUMN NAME="message" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
      </TABLE>
       <TABLE IF="((SERVER == 'JBOSS') or (SERVER == 'ACTIVEMQ')) AND (IS_USE_MESSAGE_ID == 'true')" READONLY="false">
        <COLUMN NAME="from" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
        <COLUMN NAME="messageid" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
        <COLUMN NAME="message" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
      </TABLE>
      <TABLE IF="(SERVER == 'WEBSPHERE') and (IS_USE_MESSAGE_ID == 'true')" READONLY="false">
        <COLUMN NAME="from" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
        <COLUMN NAME="messageid" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
        <COLUMN NAME="message" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
      </TABLE>
		<TABLE IF="(SERVER == 'WEBSPHERE') and (IS_USE_MESSAGE_ID == 'false')" READONLY="false">
        <COLUMN NAME="from" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
        <COLUMN NAME="message" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/>
      </TABLE>
	 <TABLE IF="(MESSAGE_BODY_TYPE == 'MAP')" READONLY="false">
      </TABLE>
    </PARAMETER>
   
  <PARAMETER
      NAME="QM"
      FIELD="TEXT"
      NUM_ROW="50"
      SHOW_IF="(SERVER == 'WEBSPHERE')"
    > 
    <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
  <PARAMETER
      NAME="QUEUE"
      FIELD="TEXT"
      NUM_ROW="50"
      SHOW_IF="(SERVER == 'WEBSPHERE')"
    > 
    <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="IS_USE_MESSAGE_ID" FIELD="CHECK" REQUIRED="true" NUM_ROW="60" 
      SHOW_IF="(SERVER == 'WEBSPHERE') AND (MESSAGE_BODY_TYPE != 'MAP')">
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>
    
   <PARAMETER
      NAME="MSG_ID"
      FIELD="TEXT"
      NUM_ROW="70"
      SHOW_IF="(SERVER == 'WEBSPHERE') AND (IS_USE_MESSAGE_ID == 'true') AND (MESSAGE_BODY_TYPE != 'MAP')"
    > 
    	<DEFAULT>""</DEFAULT>
	</PARAMETER> 

    <PARAMETER NAME="COMMIT" FIELD="CHECK" REQUIRED="true" NUM_ROW="80" SHOW_IF="(SERVER == 'WEBSPHERE')">
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>
	
  </PARAMETERS>
  
	<ADVANCED_PARAMETERS>	
	
	<PARAMETER NAME="IS_TRANSACTED" FIELD="CHECK" REQUIRED="false" NUM_ROW="20" SHOW="false">
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>

	<PARAMETER NAME="ACKNOWLEDGMENT_MODE" FIELD="CLOSED_LIST" NUM_ROW="30"  SHOW_IF="(SERVER != 'WEBSPHERE')">
		<ITEMS DEFAULT="AUTO_ACKNOWLEDGE">
	        <ITEM NAME="AUTO_ACKNOWLEDGE"   VALUE="AUTO_ACKNOWLEDGE" />
			<ITEM NAME="CLIENT_ACKNOWLEDGE"   VALUE="CLIENT_ACKNOWLEDGE" />
			<ITEM NAME="DUPS_OK_ACKNOWLEDGE"   VALUE="DUPS_OK_ACKNOWLEDGE" />
		</ITEMS>
	</PARAMETER>
	    
	<PARAMETER NAME="GET_JMS_HEADER" FIELD="CHECK" NUM_ROW="40"
     	 REQUIRED="false" SHOW_IF="(SERVER != 'WEBSPHERE')" >
      <DEFAULT>"false"</DEFAULT>
    </PARAMETER>
	<PARAMETER
      NAME="JMS_HEADERS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="50"
      NB_LINES="3"
      SHOW_IF="(GET_JMS_HEADER == 'true') AND (SERVER != 'WEBSPHERE')"
    >
      <ITEMS>
        <ITEM NAME="JMS_HEADER_NAME" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="JMSDESTINATION">
            <ITEM NAME="JMSDESTINATION" VALUE="JMSDestination"/>
            <ITEM NAME="JMSDELIVERYMODE" VALUE="JMSDeliveryMode"/>
            <ITEM NAME="JMSEXPIRATION" VALUE="JMSExpiration"/>
            <ITEM NAME="JMSPRIORITY" VALUE="JMSPriority"/>
            <ITEM NAME="JMSMESSAGEID" VALUE="JMSMessageID"/>
            <ITEM NAME="JMSTIMESTAMP" VALUE="JMSTimestamp"/>
            <ITEM NAME="JMSCORRELATIONID" VALUE="JMSCorrelationID"/>
            <ITEM NAME="JMSREPLYTO" VALUE="JMSReplyTo"/>
            <ITEM NAME="JMSTYPE" VALUE="JMSType"/>
            <ITEM NAME="JMSREDELIVERED" VALUE="JMSRedelivered"/>
          </ITEMS>
        </ITEM>
        
         <ITEM NAME="REFERENCE_COLUMN" FIELD="COLUMN_LIST"/>
      </ITEMS>
    </PARAMETER>
    
	<PARAMETER NAME="GET_JMS_PROPERTIES" FIELD="CHECK" NUM_ROW="60"
     	 REQUIRED="false" SHOW_IF="(SERVER != 'WEBSPHERE')" >
      <DEFAULT>"false"</DEFAULT>
    </PARAMETER>
	<PARAMETER
      NAME="JMS_PROPERTIES"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="70"
      NB_LINES="3"
      SHOW_IF="(GET_JMS_PROPERTIES == 'true') AND (SERVER != 'WEBSPHERE')"
    >
      <ITEMS>
        <ITEM NAME="JMS_PROPERTIES_NAME" FIELD="TEXT">
        </ITEM>
         <ITEM NAME="JMS_PROPERTIES_TYPE" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="STRING">
            <ITEM NAME="BOOLEAN" VALUE="Boolean"/>
            <ITEM NAME="BYTE" VALUE="Byte"/>
            <ITEM NAME="SHORT" VALUE="Short"/>
            <ITEM NAME="INT" VALUE="Int"/>
             <ITEM NAME="LONG" VALUE="Long"/>
            <ITEM NAME="FLOAT" VALUE="Float"/>
            <ITEM NAME="DOUBLE" VALUE="Double"/>
			<ITEM NAME="STRING" VALUE="String"/>
			<ITEM NAME="OBJECT" VALUE="Object"/>
          </ITEMS>
        </ITEM>
        <ITEM NAME="REFERENCE_COLUMN" FIELD="COLUMN_LIST"/>
      </ITEMS>
    </PARAMETER>
    
	<PARAMETER NAME="GET_MQMD_FIELDS" FIELD="CHECK" NUM_ROW="80"
     	 REQUIRED="false" SHOW_IF="(SERVER == 'WEBSPHERE')" >
      <DEFAULT>"false"</DEFAULT>
    </PARAMETER>
	<PARAMETER
      NAME="MQMD_FIELDS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="90"
      NB_LINES="3"
      SHOW_IF="(GET_MQMD_FIELDS == 'true') AND (SERVER == 'WEBSPHERE')"
    >
      <ITEMS>
        <ITEM NAME="MQMD_FIELD_NAME" FIELD="CLOSED_LIST">
		 <ITEMS DEFAULT="ACCOUNTINGTOKEN">
		 	<ITEM NAME="ACCOUNTINGTOKEN" VALUE="accountingToken"/>
		 	<ITEM NAME="APPLICATIONIDDATA" VALUE="applicationIdData"/>
		 	<ITEM NAME="APPLICATIONORIGINDATA" VALUE="applicationOriginData"/>
		 	<ITEM NAME="BACKOUTCOUNT" VALUE="backoutCount"/>
		 	<ITEM NAME="CHARACTERSET" VALUE="characterSet"/>
		 	<ITEM NAME="CORRELATIONID" VALUE="correlationId"/>
		 	<ITEM NAME="ENCODING" VALUE="encoding"/>
		 	<ITEM NAME="EXPIRY" VALUE="expiry"/>
		 	<ITEM NAME="FEEDBACK" VALUE="feedback"/>
		 	<ITEM NAME="GROUPID" VALUE="groupId"/>
		 	<ITEM NAME="MESSAGEFLAGS" VALUE="messageFlags"/>
		 	<ITEM NAME="MESSAGEID" VALUE="messageId"/>
		 	<ITEM NAME="MESSAGESEQUENCENUMBER" VALUE="messageSequenceNumber"/>
		 	<ITEM NAME="MESSAGETYPE" VALUE="messageType"/>
		 	<ITEM NAME="OFFSET" VALUE="offset"/>
		 	<ITEM NAME="ORIGINALLENGTH" VALUE="originalLength"/>
		 	<ITEM NAME="PERSISTENCE" VALUE="persistence"/>
		 	<ITEM NAME="PRIORITY" VALUE="priority"/>
		 	<ITEM NAME="PUTAPPLICATIONNAME" VALUE="putApplicationName"/>
		 	<ITEM NAME="PUTAPPLICATIONTYPE" VALUE="putApplicationType"/>
			<ITEM NAME="PUTDATETIME" VALUE="putDateTime"/>
			<ITEM NAME="REPLYTOQUEUEMANAGERNAME" VALUE="replyToQueueManagerName"/>
			<ITEM NAME="REPLYTOQUEUENAME" VALUE="replyToQueueName"/>
			<ITEM NAME="REPORT" VALUE="report"/>
			<ITEM NAME="USERID" VALUE="userId"/>
          </ITEMS>
        </ITEM>
         <ITEM NAME="REFERENCE_COLUMN" FIELD="COLUMN_LIST"/>
      </ITEMS>
    </PARAMETER>
    
	</ADVANCED_PARAMETERS>
	
  <CODEGENERATION>
     <IMPORTS>
      <IMPORT NAME="JBOSS REMOTING" MODULE="jboss-remoting.jar" REQUIRED="true" />
       <IMPORT NAME="JNP-CLIENT" MODULE="jnp-client.jar" REQUIRED="true" />
       <IMPORT NAME="JBOSS MQ LIB" MODULE="jbossall-client.jar" REQUIRED="false" />
       <IMPORT NAME="WEBSPHERE MQ LIB" MODULE="com.ibm.mq.jar" REQUIRED="false" />
       <IMPORT NAME="WEBSPHERE JMS LIB" MODULE="com.ibm.mqjms.jar" REQUIRED="false" />
       <IMPORT NAME="WEBSPHERE CONNECTOR" MODULE="websphere-connector-1_5.jar" REQUIRED="false" /> 
       <IMPORT NAME="ACTIVEMQ LIB" MODULE="activemq-all-5.1.0.jar" REQUIRED="false" />
       <IMPORT NAME="JBOSS MESSAGING CLIENT" MODULE="jboss-messaging-client.jar" REQUIRED="true" />
       <IMPORT NAME="JBOSS AOP" MODULE="jboss-aop.jar" REQUIRED="true" />
       <IMPORT NAME="JBOSS COMMON" MODULE="jboss-common-core.jar" REQUIRED="true" />
       <IMPORT NAME="JBOSS MDR" MODULE="jboss-mdr.jar" REQUIRED="true" />
       <IMPORT NAME="JAVA_SSIST" MODULE="javassist.jar" REQUIRED="true" /> 
       <IMPORT NAME="TROVE" MODULE="trove.jar" REQUIRED="true" />   
       <IMPORT NAME="LOG4J" MODULE="log4j-1.2.13.jar" REQUIRED="false" />
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
  </RETURNS>

</COMPONENT>