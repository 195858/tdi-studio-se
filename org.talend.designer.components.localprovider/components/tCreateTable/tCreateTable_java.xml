<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070627A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>
  

  

  <PARAMETERS>
  <PARAMETER
      NAME="DBTYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="1"
    >
      <ITEMS DEFAULT="ACCESS">       
        <ITEM NAME="ACCESS"        VALUE="Access" />
        <ITEM NAME="DB2"           VALUE="DB2"/>
        <ITEM NAME="FIREBIRD"      VALUE="Firebird"/>
        <ITEM NAME="HSQLDB"      VALUE="HSQLdb"/>
        <ITEM NAME="INFORMIX"      VALUE="Informix"/>
        <ITEM NAME="INGRES"      VALUE="Ingres"/>
        <ITEM NAME="INTERBASE"      VALUE="Interbase"/>
        <ITEM NAME="JAVADB"      VALUE="JavaDB"/>
        <ITEM NAME="MSSQL" 		   VALUE="MSSQL"/>
        <ITEM NAME="MYSQL"         VALUE="MySQL"/>
        <ITEM NAME="DBORACLE"      VALUE="Oracle"/>
        <ITEM NAME="POSTGRE"      VALUE="Postgre"/>
        <ITEM NAME="SQLITE"      VALUE="SQLite"/>
        <ITEM NAME="SYBASE"      VALUE="Sybase"/>
        <ITEM NAME="ODBC"      VALUE="ODBC"/>       
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="SPECIALACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="1"
    >
      <ITEMS DEFAULT="NONE">       
        <ITEM NAME="NONE"        VALUE="None" />
        <ITEM NAME="CREATEONLY"           VALUE="CreateOnly"/>
        <ITEM NAME="DROPANDCREATE"      VALUE="DropAndCreate"/>
      </ITEMS>
    </PARAMETER>
     
     <PARAMETER
      NAME="EXISTINGCONNECTION"
      FIELD="CHECK"
      SHOW_IF="(DBTYPE=='MYSQL' or DBTYPE=='DBORACLE')"
      NUM_ROW="2"
    >
    <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="CONNECTION_MYSQL"
      FIELD="COMPONENT_LIST"
      FILTER="tMysqlConnection"
      NUM_ROW="2"
      SHOW_IF="(DBTYPE=='MYSQL' and EXISTINGCONNECTION == 'true')"
    />
           
    <PARAMETER
      NAME="CONNECTION_ORACLE"
      FIELD="COMPONENT_LIST"
      FILTER="tOracleConnection"
      NUM_ROW="2"
      SHOW_IF="(DBTYPE=='DBORACLE' and EXISTINGCONNECTION == 'true')"
    />    

    <PARAMETER
      NAME="ORACLETYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="2"
	  SHOW_IF="(EXISTINGCONNECTION == 'false' and DBTYPE=='DBORACLE')"  
    >
      <ITEMS DEFAULT="ORACLE SID">
        <ITEM NAME="ORACLE SID"        VALUE="Oracle SID" />
        <ITEM NAME="ORACLE SERVICE"    VALUE="Oracle Service"/>
      </ITEMS>
    </PARAMETER> 
     
     <PARAMETER
      NAME="JAVADBFRAME"
      FIELD="CLOSED_LIST"
      NUM_ROW="2"
	  SHOW_IF="(DBTYPE=='JAVADB')"  
    >
      <ITEMS DEFAULT="EMBEDED">
        <ITEM NAME="EMBEDED"        VALUE="Embeded" />
        <ITEM NAME="JCCJDBC"    	VALUE="JCCJDBC"/>
        <ITEM NAME="DERBYCLIENT"    VALUE="DERBYCLIENT"/>
      </ITEMS>
    </PARAMETER>
 
     <PARAMETER
      NAME="JAVADBNETSERVER"
      FIELD="CHECK"
      SHOW_IF="(DBTYPE=='JAVADB')"
      NUM_ROW="2"    
    >   
      <DEFAULT>false</DEFAULT>
    </PARAMETER>  
    
    
    
    
    
    
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="3"
      REPOSITORY_VALUE="DATABASE"
    />
    
    <PARAMETER
      NAME="ACCESSDBNAME"
      FIELD="FILE"
      NUM_ROW="4"
      REQUIRED="true"
      SHOW_IF="(DBTYPE=='ACCESS')"
      
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.mdb"</DEFAULT>
    </PARAMETER> 


    <PARAMETER
      NAME="DBPATH"
      FIELD="DIRECTORY"
      NUM_ROW="4"
      REQUIRED="true"
      SHOW_IF="(DBTYPE=='JAVADB' and JAVADBFRAME=='EMBEDED')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="SERVER_NAME"
      NOT_SHOW_IF="(DBTYPE=='ACCESS') or (DBTYPE=='SQLITE') or (DBTYPE=='MYSQL' and EXISTINGCONNECTION=='TRUE') or (DBTYPE=='DBORACLE' and EXISTINGCONNECTION=='TRUE') or (DBTYPE == 'JAVADB' and JAVADBFRAME=='EMBEDED') or (DBTYPE == 'ODBC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PORT"
      NOT_SHOW_IF="(DBTYPE=='FIREBIRD') or (DBTYPE=='ACCESS') or (DBTYPE=='SQLITE')  or (DBTYPE=='MYSQL' and EXISTINGCONNECTION=='TRUE') or (DBTYPE=='DBORACLE' and EXISTINGCONNECTION=='TRUE') or (DBTYPE == 'JAVADB' and JAVADBFRAME=='EMBEDED') or (DBTYPE == 'ODBC')"
    >
      <DEFAULT>"50000"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DBNAME"
      FIELD="Text"
      NUM_ROW="4"
      REQUIRED="true"
      REPOSITORY_VALUE="SID"
      NOT_SHOW_IF="(DBTYPE=='FIREBIRD') or (DBTYPE=='ACCESS') or (DBTYPE=='SQLITE')  or (DBTYPE=='MYSQL' and EXISTINGCONNECTION=='TRUE') or (DBTYPE=='DBORACLE' and EXISTINGCONNECTION=='TRUE')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
   <PARAMETER
      NAME="SQLITEDBNAME"
      FIELD="FILE"
      NUM_ROW="4"
      REQUIRED="true"
      SHOW_IF="(DBTYPE=='SQLITE')"     
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER> 
    <PARAMETER
      NAME="FIREBIRJNI"
      FIELD="DIRECTORY"
      NUM_ROW="4"
      REQUIRED="true"
      SHOW_IF="(DBTYPE=='FIREBIRD')"     
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
   <PARAMETER
      NAME="FIREBIRDDBNAME"
      FIELD="FILE"
      NUM_ROW="5"
      REQUIRED="true"
      SHOW_IF="(DBTYPE=='FIREBIRD')"     
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="INFORMIXSERVER"
      FIELD="TEXT"
      NUM_ROW="5"
      REQUIRED="true"
      SHOW_IF="(DBTYPE=='INFORMIX')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>


    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      NOT_SHOW_IF="(DBTYPE=='MYSQL' and EXISTINGCONNECTION=='TRUE') or (DBTYPE=='DBORACLE' and EXISTINGCONNECTION=='TRUE') or (DBTYPE=='SQLITE') "
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="5"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      NOT_SHOW_IF="(DBTYPE=='MYSQL' and EXISTINGCONNECTION=='TRUE') or (DBTYPE=='DBORACLE' and EXISTINGCONNECTION=='TRUE') or (DBTYPE=='SQLITE') "
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
        

    
    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="6"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="7"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="8"
    >  
      <DEFAULT IF="DBTYPE=='ACCESS'">access_id</DEFAULT>
      <DEFAULT IF="DBTYPE=='DB2'">ibmdb2_id</DEFAULT>
      <DEFAULT IF="DBTYPE=='FIREBIRD'">firebird_id</DEFAULT>
      <DEFAULT IF="DBTYPE=='HSQLDB'">hsqldb_id</DEFAULT>
      <DEFAULT IF="DBTYPE=='INFORMIX'">informix_id</DEFAULT>
      <DEFAULT IF="DBTYPE=='INGRES'">ingres_id</DEFAULT>
      <DEFAULT IF="DBTYPE=='INTERBASE'">interbase_id</DEFAULT>
      <DEFAULT IF="DBTYPE=='JAVADB'">javadb_id</DEFAULT>
      <DEFAULT IF="DBTYPE=='MSSQL'">MSODBC</DEFAULT>
      <DEFAULT IF="DBTYPE=='MYSQL'">mysql_id</DEFAULT>
      <DEFAULT IF="DBTYPE=='DBORACLE'">oracle_id</DEFAULT>
      <DEFAULT IF="DBTYPE=='POSTGRE'">postgres_id</DEFAULT>
      <DEFAULT IF="DBTYPE=='SQLITE'">sqlite_id</DEFAULT>
      <DEFAULT>mysql_id</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
   
  </PARAMETERS>
  

   <CODEGENERATION>
     <IMPORTS>
		<IMPORT NAME="Driver-DB2" MODULE="db2jcc.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-DB2-LICENSE-CISUZ" MODULE="db2jcc_license_cisuz.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-DB2-LICENSE-CU" MODULE="db2jcc_license_cu.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-MSSQL" MODULE="jtds-1.2.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-MYSQL" MODULE="mysql-connector-java-5.1.0-bin.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-ORACLE" MODULE="ojdbc14.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-POSTGRESQL" MODULE="postgresql-8.1-405.jdbc3.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-ACCESS" MODULE="Access_JDBC30.jar" REQUIRED="true" />	
		<IMPORT NAME="Driver-FIREBIRD-JAAS" MODULE="jaas.jar" REQUIRED="true" />	
		<IMPORT NAME="Driver-FIREBIRD-JAYBIRD" MODULE="jaybird-2.1.1.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-FIREBIRD-LOG4J" MODULE="log4j-core.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-FIREBIRD-MINI-J2EE" MODULE="mini-j2ee.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-INFORMIX-JDBC" MODULE="ifxjdbc.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-INFORMIX-JDBCX" MODULE="ifxjdbcx.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-INFORMIX-LANG" MODULE="ifxlang.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-INFORMIX-LSUPP" MODULE="ifxlsupp.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-INFORMIX-SQLJ" MODULE="ifxsqlj.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-JAVADB-DERBY" MODULE="derby.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-JAVADB-DERBYCLIENT" MODULE="derbyclient.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-JAVADB-DERBYNET" MODULE="derbynet.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-SQLITE-JDBC-NESTED" MODULE="sqlitejdbc-v033-nested.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-SQLITE-JDBC" MODULE="sqlitejdbc.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-SYBASE-JCONN3" MODULE="jconn3.jar" REQUIRED="true" />
		<IMPORT NAME="Driver-SYBASE-JCONN3" MODULE="jconn3.jar" REQUIRED="true" />
		
     </IMPORTS>
   </CODEGENERATION>
   
  <RETURNS>
    <RETURN NAME="NB_LINE"          TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>