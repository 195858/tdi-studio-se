<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE"
    />

    <PARAMETER
      NAME="SERVER"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>"SYBASE"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
    >
      <DEFAULT>"2648"</DEFAULT>
   </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>"sa"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="5"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="QUERYSTORE"
      FIELD="QUERYSTORE_TYPE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="7"
      REQUIRED="true"
    >
      <DEFAULT>"select * from sysobjects "</DEFAULT>
    </PARAMETER>
    
     <PARAMETER
      NAME="COMMIT_EVERY"
      FIELD="TEXT"
      NUM_ROW="10"
      REQUIRED="true"
    >
      <DEFAULT>100</DEFAULT>
    </PARAMETER>

   </PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
		<IMPORT NAME="Driver-Sybase" MODULE="jconn3.jar" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
      <RETURN NAME="QUERY" TYPE="STRING" AVAILABILITY="FLOW"/>
   </RETURNS>
</COMPONENT>