<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:SYBASE"
    />

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="2"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tSybaseConnection"
      NUM_ROW="2"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />


    <PARAMETER
      NAME="SERVER"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>'SYBASE'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="3"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>SybaseASE</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>'sa'</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="5"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="DBTABLE"
      FIELD="DBTABLE"
      NUM_ROW="6"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    <PARAMETER
      NAME="QUERYSTORE"
      FIELD="QUERYSTORE_TYPE"
      NUM_ROW="7"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>'select * from sysobjects '</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PREPARED_STATEMENT"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="9"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="STATEMENT_PARAMS"
      FIELD="TABLE"
      NUM_ROW="10"
      NB_LINES="2"
      NOT_SHOW_IF="(PREPARED_STATEMENT=='false')"
    >
      <ITEMS>
        <ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>


    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="12"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

   </PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="DBI" MODULE="DBI" REQUIRED="true" />
       <IMPORT NAME="DBD-Sybase" MODULE="DBD::Sybase" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
      <RETURN NAME="QUERY" TYPE="STRING" AVAILABILITY="FLOW"/>
   </RETURNS>
</COMPONENT>