<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="RUN_IF" MAX_OUTPUT="1"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:ORACLE"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />
    
    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="20"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tOracleConnection"
      NUM_ROW="20"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />        
    
    <PARAMETER
      NAME="CONNECTION_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="30"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <ITEMS DEFAULT="ORACLE_SID">
        <ITEM NAME="ORACLE_SID" VALUE="ORACLE_SID" />
        <ITEM NAME="ORACLE_SERVICE_NAME" VALUE="ORACLE_SERVICE_NAME"/>
        <ITEM NAME="ORACLE_OCI" VALUE="ORACLE_OCI"/>
      </ITEMS>
    </PARAMETER>    

   <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_OCI')"
    >
      <DEFAULT>"Talend-dbms"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_OCI')"
    >
      <DEFAULT>"1521"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="SID"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_OCI')" 
    >
      <DEFAULT>"Talend"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER 
	  NAME="LOCAL_SERVICE_NAME" 
	  FIELD="TEXT" 
	  NUM_ROW="50" 
	  REPOSITORY_VALUE="SID" 
	  REQUIRED="true" 
	  SHOW_IF="(USE_EXISTING_CONNECTION == 'false' and CONNECTION_TYPE == 'ORACLE_OCI')" 
	> 
	  <DEFAULT>""</DEFAULT> 
	</PARAMETER> 

    <PARAMETER 
      NAME="SCHEMA_DB"
      FIELD="TEXT"
      NUM_ROW="50"
      REPOSITORY_VALUE="SCHEMA"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"      
    >
      <DEFAULT>"ROOT"</DEFAULT>
    </PARAMETER>
          
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="60"
      REQUIRED="true"
      REPOSITORY_VALUE="USERNAME"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"      
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="60"
      REQUIRED="true"
      REPOSITORY_VALUE="PASSWORD"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"      
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="70"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>     

    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="70"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM NAME="NONE" VALUE="NONE" />
        <ITEM NAME="DROP_CREATE" VALUE="DROP_CREATE"/>
        <ITEM NAME="CREATE" VALUE="CREATE"/>
        <ITEM NAME="CREATE_IF_NOT_EXISTS" VALUE="CREATE_IF_NOT_EXISTS"/>
        <ITEM NAME="CLEAR" VALUE="CLEAR"/>
      </ITEMS>
    </PARAMETER>

    <!-- FEATURE WANTED: multiple data files to load from. but the current TABLE model doesn't support item whose field is FILE -->
    <PARAMETER
      NAME="DATA"
      FIELD="FILE"
      NUM_ROW="80"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CLT_FILE == 'false'"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.csv"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DATA_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="80"
      REQUIRED="true"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (INPUT_INTO_TABLE_CLAUSE == 'false')"
    >
      <ITEMS DEFAULT="INSERT">
        <ITEM NAME="INSERT"           VALUE="INSERT" />
        <ITEM NAME="APPEND"           VALUE="APPEND" />
        <ITEM NAME="REPLACE"          VALUE="REPLACE"/>
        <ITEM NAME="TRUNCATE"         VALUE="TRUNCATE"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="100">
      <DEFAULT/> 
    </PARAMETER>

	<PARAMETER
      NAME="MAPPING"
      FIELD="MAPPING_TYPE"
      NUM_ROW="110"
      SHOW="false"
    >       
      <DEFAULT>oracle_id</DEFAULT>
    </PARAMETER>

   </PARAMETERS>
   
   <ADVANCED_PARAMETERS>
   	<PARAMETER
      NAME="USE_EXISTING_CLT_FILE"
      FIELD="CHECK"
      NUM_ROW="5"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="CLT_FILE"
      FIELD="FILE"
      NUM_ROW="5"
      SHOW_IF="USE_EXISTING_CLT_FILE == 'true'"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/sqlldr.clt"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="RECORD_FORMAT"
      FIELD="CLOSED_LIST"
      NUM_ROW="10"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CLT_FILE == 'false'"
    >
      <ITEMS DEFAULT="DEFAULT">
        <ITEM NAME="DEFAULT"           VALUE="DEFAULT" />
        <ITEM NAME="STREAM"           VALUE="STREAM" />
        <ITEM NAME="FIXED"          VALUE="FIXED"/>
        <ITEM NAME="VARIABLE"         VALUE="VARIABLE"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="TERMINATOR_STRING"
      FIELD="TEXT"
      NUM_ROW="10"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (RECORD_FORMAT == 'STREAM')"
    >
      <DEFAULT>"\\n"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="RECORD_LENGTH"
      FIELD="TEXT"
      NUM_ROW="10"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (RECORD_FORMAT == 'FIXED')"
    >
      <DEFAULT>20</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="RECORD_LENGTH_FIELD_SIZE"
      FIELD="TEXT"
      NUM_ROW="10"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (RECORD_FORMAT == 'VARIABLE')"
    >
      <DEFAULT>3</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="INPUT_INTO_TABLE_CLAUSE"
      FIELD="CHECK"
      NUM_ROW="15"
      SHOW_IF="USE_EXISTING_CLT_FILE == 'false'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
	
	<PARAMETER 
	  NAME="INTO_TABLE_CLAUSE" 
	  FIELD="MEMO_JAVA" 
	  REQUIRED="false"
	  NUM_ROW="16" 
	  NB_LINES="4"
	  SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (INPUT_INTO_TABLE_CLAUSE == 'true')"
	>
	  <DEFAULT>"--EXAMPLE(loading data into multiple table): REPLACE INTO TABLE emp (empno POSITION(1:4) INTEGER EXTERNAL, ename POSITION(6:15) CHAR) INTO TABLE proj WHEN projno != ’ ’ (empno POSITION(1:4) INTEGER EXTERNAL,projno POSITION(25:27) INTEGER EXTERNAL)"</DEFAULT>
	</PARAMETER>
	
   
    <PARAMETER
      NAME="FIELDS_TERMINATOR"
      FIELD="CLOSED_LIST"
      NUM_ROW="20"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (INPUT_INTO_TABLE_CLAUSE == 'false')"
    >
      <ITEMS DEFAULT="OTHER">
        <ITEM NAME="NONE"           VALUE="NONE" />
        <ITEM NAME="WHITESPACE"           VALUE="WHITESPACE" />
        <ITEM NAME="EOF"          VALUE="EOF"/>
        <ITEM NAME="OTHER"         VALUE="OTHER"/>
      </ITEMS>
    </PARAMETER>
    
	<PARAMETER
      NAME="TERMINATOR_VALUE"
      FIELD="TEXT"
      NUM_ROW="20"
      REQUIRED="true"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (FIELDS_TERMINATOR == 'OTHER') AND (INPUT_INTO_TABLE_CLAUSE == 'false')"
    >
      <DEFAULT>";"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USE_FIELDS_ENCLOSURE"
      FIELD="CHECK"
      NUM_ROW="25"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (INPUT_INTO_TABLE_CLAUSE == 'false')"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FIELDS_ENCLOSURE_LEFT"
      FIELD="TEXT"
      NUM_ROW="25"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (INPUT_INTO_TABLE_CLAUSE == 'false') AND (USE_FIELDS_ENCLOSURE == 'true')"
    >
      <DEFAULT>"\""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="FIELDS_ENCLOSURE_RIGHT"
      FIELD="TEXT"
      NUM_ROW="25"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (INPUT_INTO_TABLE_CLAUSE == 'false') AND (USE_FIELDS_ENCLOSURE == 'true')"
    >
      <DEFAULT>"\""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USE_DATE_PATTERN"
      FIELD="CHECK"
      NUM_ROW="26"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (INPUT_INTO_TABLE_CLAUSE == 'false')"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
	<PARAMETER
      NAME="FIELD_CONDITION"
      FIELD="CHECK"
      NUM_ROW="30"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (INPUT_INTO_TABLE_CLAUSE == 'false')"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="FIELD_CONDITION_VALUE"
      FIELD="FILE"
      NUM_ROW="30"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (INPUT_INTO_TABLE_CLAUSE == 'false') AND (FIELD_CONDITION == 'true')"
    >
      <DEFAULT>"country='France'"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PRESERVE_BLANKS"
      FIELD="CHECK"
      NUM_ROW="35"
      SHOW_IF="USE_EXISTING_CLT_FILE == 'false'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="TRAILING_NULLCOLS"
      FIELD="CHECK"
      NUM_ROW="35"
      SHOW_IF="(USE_EXISTING_CLT_FILE == 'false') AND (INPUT_INTO_TABLE_CLAUSE == 'false')"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
        
    <PARAMETER
      NAME="OPTIONS"
      FIELD="TABLE"
      NUM_ROW="40"
      NB_LINES="5"
      SHOW_IF="USE_EXISTING_CLT_FILE == 'false'"
    >
      <ITEMS>
        <ITEM NAME="KEY" FIELD="CLOSED_LIST" >
          <ITEMS DEFAULT="BINDSIZE">
            <ITEM NAME="BINDSIZE"  VALUE="BINDSIZE"/>
            <ITEM NAME="COLUMNARRAYROWS"    VALUE="COLUMNARRAYROWS"/>
            <ITEM NAME="DIRECT"    VALUE="DIRECT"/>
            <ITEM NAME="ERRORS"    VALUE="ERRORS"/>
            <ITEM NAME="LOAD"    VALUE="LOAD"/>
            <ITEM NAME="MULTITHREADING"    VALUE="MULTITHREADING"/>
            <ITEM NAME="PARALLEL"    VALUE="PARALLEL"/>
            <ITEM NAME="READSIZE"    VALUE="READSIZE"/>
            <ITEM NAME="RESUMABLE"  VALUE="RESUMABLE"/>
            <ITEM NAME="RESUMABLE_NAME"    VALUE="RESUMABLENAME"/>
            <ITEM NAME="RESUMABLE_TIMEOUT"    VALUE="RESUMABLETIMEOUT"/>
            <ITEM NAME="ROWS"    VALUE="ROWS"/>
            <ITEM NAME="SILENT"    VALUE="SILENT"/>
            <ITEM NAME="SKIP"    VALUE="SKIP"/>
            <ITEM NAME="SKIP_INDEX_MAINTENANCE"    VALUE="SKIPINDEXMAINTENANCE"/>
          	<ITEM NAME="SKIP_UNUSABLE_INDEXES"    VALUE="SKIPUNUSABLEINDEXES"/>
          	<ITEM NAME="STREAMSIZE"    VALUE="STREAMSIZE"/>
          </ITEMS>
        </ITEM>
        <ITEM NAME="VALUE" FIELD="TEXT"/>
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="45"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CLT_FILE == 'false'"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="OUTPUT" FIELD="CLOSED_LIST" NUM_ROW="50">
       <ITEMS DEFAULT="OUTPUT_TO_CONSOLE">
          <ITEM NAME="OUTPUT_TO_CONSOLE" VALUE="OUTPUT_TO_CONSOLE" />
          <ITEM NAME="RETRIEVE_OUTPUT"   VALUE="RETRIEVE_OUTPUT" />
       </ITEMS>
    </PARAMETER>
   </ADVANCED_PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
		<IMPORT NAME="Driver-Oracle" MODULE="classes12.jar" REQUIRED="true" /> 
		<IMPORT NAME="Delimited_File_Reader" MODULE="file_delimited.jar" REQUIRED="true"/>
     </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
      <RETURN NAME="NB_LINE_DATA" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="NB_LINE_BAD" TYPE="id_Integer" AVAILABILITY="AFTER"/>
      <RETURN NAME="NB_LINE_INSERTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>
