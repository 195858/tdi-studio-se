<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070312A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Business/Salesforce</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

<PARAMETERS>
    <PARAMETER
		NAME="USE_EXISTING_CONNECTION"
		FIELD="CHECK"
	    NUM_ROW="10"
	>
		<DEFAULT>false</DEFAULT>
	</PARAMETER>    
	
	<PARAMETER
		NAME="CONNECTION"
	    FIELD="COMPONENT_LIST"
	    FILTER="tSalesforceConnection"
	    NUM_ROW="10"
	    SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
	/>
	    
    <PARAMETER
      NAME="ENDPOINT"
      FIELD="TEXT"
      NUM_ROW="20"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"https://www.salesforce.com/services/Soap/u/19.0"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="30"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"youremail@yourcompany.com"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="40"
      REQUIRED="true"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>"password"</DEFAULT>
    </PARAMETER>
	<PARAMETER 
		NAME="TIMEOUT" 
			FIELD="TEXT"
			NUM_ROW="45"
			SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<DEFAULT>60000</DEFAULT>
		</PARAMETER>
	<PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="50"
    >
      <TABLE READONLY="true">
         <COLUMN KEY="false" LENGTH="20" NAME="ServerTimestamp" TYPE="id_Date" PATTERN="&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss&apos;.000Z&apos;&quot;"/>
      </TABLE>
    
   </PARAMETER>

   </PARAMETERS>

	<ADVANCED_PARAMETERS>
  
	   <PARAMETER 
	       NAME="USE_PROXY" 
	       FIELD="CHECK" 
	       REQUIRED="true"
		   NUM_ROW="10" 
		   REPOSITORY_VALUE="UES_PROXY">
		   <DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER 
			NAME="PROXY_HOST" 
			FIELD="TEXT"
			NUM_ROW="20"
			SHOW_IF="USE_PROXY == 'true'" 
			REPOSITORY_VALUE="PROXY_HOST">
			<DEFAULT>"61.163.92.4"</DEFAULT>
		</PARAMETER>
	
		<PARAMETER 
			NAME="PROXY_PORT" 
			FIELD="TEXT"
			NUM_ROW="30" 
			SHOW_IF="USE_PROXY == 'true'" 
			REPOSITORY_VALUE="PROXY_PORT">
			<DEFAULT>"1080"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER 
			NAME="PROXY_USERNAME" 
			FIELD="TEXT" 
			NUM_ROW="40" 
			SHOW_IF="USE_PROXY == 'true'" 
			REPOSITORY_VALUE="PROXY_USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER 
			NAME="PROXY_PASSWORD" 
			FIELD="TEXT"
			NUM_ROW="50" 
			SHOW_IF="USE_PROXY == 'true'" 
			REPOSITORY_VALUE="PROXY_PASSWORD">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
		<PARAMETER 
	       NAME="NEED_COMPRESSION" 
	       FIELD="CHECK" 
	       REQUIRED="true"
		   NUM_ROW="60"
		   SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
		   <DEFAULT>false</DEFAULT>
		</PARAMETER>
		
	</ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="salesforceCRM-management" MODULE="salesforceCRMManagement.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-axis" MODULE="axis.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-jaxrpc" MODULE="jaxrpc.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-saaj" MODULE="saaj.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-wsdl" MODULE="wsdl4j-1.5.1.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-discovery" MODULE="commons-discovery-0.2.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-logging" MODULE="commons-logging-1.1.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-JavaMail" MODULE="mail.jar" REQUIRED="true" />
      <IMPORT NAME="SOAP-JAF" MODULE="activation.jar" REQUIRED="true" />      
      <IMPORT NAME="HTTP-client" MODULE="commons-httpclient-3.1.jar" REQUIRED="true" />
	  <IMPORT NAME="HTTP-codec" MODULE="commons-codec-1.3.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION> 

   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
   </RETURNS>
</COMPONENT>