<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070314A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:ORACLE"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
    >
      <DEFAULT>"1521"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>



    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="7"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SPNAME"
      FIELD="TEXT"
      NUM_ROW="9"
      REQUIRED="true"
    >
      <DEFAULT>SPName</DEFAULT>
    </PARAMETER>



    <PARAMETER
      NAME="ADD_COLS"
      FIELD="TABLE"
      REQUIRED="false"
      NOT_SHOW_IF="(TABLE_ACTION=='DROP_CREATE' or TABLE_ACTION=='CREATE' or TABLE_ACTION=='CREATE_IF_NOT_EXISTS')"
      NUM_ROW="10"
      NB_LINES="3"
    >
      <ITEMS>

        <ITEM NAME="NAME" VALUE="paraName" />

        <ITEM NAME="TYPE" FIELD="CLOSED_LIST">
          <ITEMS DEFAULT="IN">
            <ITEM NAME="IN" VALUE="IN"/>
            <ITEM NAME="OUT" VALUE="OUT"/>
            <ITEM NAME="IN_AND_OUT" VALUE="IN_AND_OUT"/>
          </ITEMS>
        </ITEM>

        <ITEM NAME="DATATYPE" FIELD="CLOSED_LIST">
           <ITEMS DEFAULT="BASIC">
            <ITEM NAME="BASIC" VALUE="Basic"/>
            <ITEM NAME="ORACLE" VALUE="Oralce DataType"/>
          </ITEMS>
        </ITEM>
        
        

      </ITEMS>
    </PARAMETER>

 
    
    <PARAMETER
      NAME="OUTPARA"
      FIELD="TEXT"
      NUM_ROW="11"
    >
      <DEFAULT></DEFAULT>
    </PARAMETER>


  </PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
		<IMPORT NAME="Driver-Oracle" MODULE="ojdbc14.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
   
  <RETURNS>
    <RETURN NAME="NB_LINE"          TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_UPDATED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_INSERTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="NB_LINE_DELETED"  TYPE="id_Integer" AVAILABILITY="AFTER"/>
    <RETURN NAME="QUERY"  TYPE="id_String" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>