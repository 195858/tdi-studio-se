<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20070226A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE"
    />

    <PARAMETER
      NAME="TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="2"
      REPOSITORY_VALUE="TYPE"
    >
      <ITEMS DEFAULT="mysql;org.gjt.mm.mysql.Driver">
        <ITEM
          NAME="MYSQL"
          VALUE="mysql;org.gjt.mm.mysql.Driver"
          REPOSITORY_ITEM="MYSQL"
        />
        <ITEM
          NAME="PGSQL"
          VALUE="postgresql;org.postgresql.Driver"
          REPOSITORY_ITEM="POSTGRESQL"
        />
        <ITEM
          NAME="ORACLE"
          VALUE="oracle;oracle.jdbc.driver.OracleDriver"
          REPOSITORY_ITEM="ORACLE"
        />
        <ITEM
          NAME="ODBC"
          VALUE="undef;sun.jdbc.odbc.JdbcOdbcDriver"
          REPOSITORY_ITEM="ODBC"
        />
        <ITEM
          NAME="MSSQL_ODBC"
          VALUE="mssql;sun.jdbc.odbc.JdbcOdbcDriver"
          REPOSITORY_ITEM="ODBC_JDBC"
        />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SERVER_NAME"
      NOT_SHOW_IF="(TYPE=='MSSQL_ODBC' or TYPE=='ODBC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PORT"
      NOT_SHOW_IF="(TYPE=='MSSQL_ODBC' or TYPE=='ODBC')"
    >
      <DEFAULT>""</DEFAULT>
      <DEFAULT IF="TYPE=='MYSQL'">"3306"</DEFAULT>
      <DEFAULT IF="TYPE=='PGSQL'">"5432"</DEFAULT>
      <DEFAULT IF="TYPE=='ORACLE'">"1521"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER 
      NAME="SCHEMA_DB"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SCHEMA" 
      NOT_SHOW_IF="(TYPE=='MSSQL_ODBC') or (TYPE=='ODBC') or (TYPE=='MYSQL')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="4"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="5"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

 	<PARAMETER
      NAME="QUERYSTORE"
      FIELD="QUERYSTORE_TYPE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="QUERY"
      FIELD="MEMO_SQL"
      NUM_ROW="7"
      REQUIRED="true"
    >
      <DEFAULT>"select id, name from employee"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="TEXT"
      NUM_ROW="8"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

   </PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="Driver-MySql" MODULE="mysql-connector-java-3.1.13-bin.jar" REQUIRED="true" />
       <IMPORT NAME="Driver-Oracle" MODULE="ojdbc14.jar" REQUIRED="false" />
       <IMPORT NAME="Driver-Postgres" MODULE="postgresql-8.1-405.jdbc3.jar" REQUIRED="false" />
     </IMPORTS>
   </CODEGENERATION>

   <RETURNS>
      <RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER"/>
      <RETURN NAME="QUERY" TYPE="STRING" AVAILABILITY="FLOW"/>
   </RETURNS>
</COMPONENT>