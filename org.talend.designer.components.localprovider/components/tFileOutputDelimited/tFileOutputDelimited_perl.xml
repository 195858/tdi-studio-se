<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="false">
		<SIGNATURE></SIGNATURE>
	</HEADER>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="REFERENCE" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="10" REPOSITORY_VALUE="DELIMITED" />

		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="20"
			REQUIRED="true" REPOSITORY_VALUE="FILE_PATH">
			<DEFAULT>'__COMP_DEFAULT_FILE_DIR__/out.csv'</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ROWSEPARATOR" FIELD="TEXT" NUM_ROW="30"
			REPOSITORY_VALUE="ROW_SEPARATOR">
			<DEFAULT>"\n"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FIELDSEPARATOR" FIELD="TEXT" NUM_ROW="40"
			REPOSITORY_VALUE="FIELD_SEPARATOR">
			<DEFAULT>';'</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CSV_OPTION" FIELD="CHECK" REQUIRED="true"
			REPOSITORY_VALUE="CSV_OPTION" NUM_ROW="41">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ESCAPE_CHAR" FIELD="TEXT" NUM_ROW="42"
			REPOSITORY_VALUE="ESCAPE_CHAR" SHOW_IF="CSV_OPTION == 'true'">
			<DEFAULT>'"'</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="TEXT_ENCLOSURE" FIELD="TEXT" NUM_ROW="43"
			REPOSITORY_VALUE="TEXT_ENCLOSURE" SHOW_IF="CSV_OPTION == 'true'">
			<DEFAULT>'"'</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="APPEND" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="50" REPOSITORY_VALUE="APPEND">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="INCLUDEHEADER" FIELD="CHECK" NUM_ROW="60">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="70">
			<DEFAULT />
		</PARAMETER>

	</PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="TEXT-CSV_XS" MODULE="Text::CSV_XS"
				REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>


	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER" />
	</RETURNS>
</COMPONENT>