<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="1"
      REPOSITORY_VALUE="DATABASE:IBM_DB2"
    />

    <PARAMETER
      NAME="USE_CATALOGED"
      FIELD="CHECK"
      NUM_ROW="1"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CATALOGED_DB_ALIAS"
      FIELD="TEXT"
      NUM_ROW="2"
      REPOSITORY_VALUE="CATALOGED_DB_ALIAS"
      SHOW_IF="USE_CATALOGED == 'true'"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="2"
      REPOSITORY_VALUE="SERVER_NAME"
      SHOW_IF="USE_CATALOGED == 'false'"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="3"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
      SHOW_IF="USE_CATALOGED == 'false'"
    >
      <DEFAULT>IBM DB2</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="2"
      REPOSITORY_VALUE="PORT"
      SHOW_IF="USE_CATALOGED == 'false'"
    >
      <DEFAULT>'50000'</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="2"
      REPOSITORY_VALUE="SID"
      SHOW_IF="USE_CATALOGED == 'false'"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="USERNAME"
      SHOW_IF="USE_CATALOGED == 'false'"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="PASSWORD"
      SHOW_IF="USE_CATALOGED == 'false'"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>


    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="70"
      SHOW_IF="USE_CATALOGED == 'false'"
      REQUIRED="true"
    >
      <DEFAULT>'ISO-8859-15'</DEFAULT>
    </PARAMETER>

   </PARAMETERS>

   <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="DBI" MODULE="DBI" REQUIRED="true" />
       <IMPORT NAME="DBD-DB2" MODULE="DBD::DB2" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>

   <RETURNS/>
</COMPONENT>