<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
       SERIAL=""
      VERSION="0.101"
       STATUS="ALPHA"
         
COMPATIBILITY="ALL"
       AUTHOR="Talend"
 RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="METTERLABEL" FIELD="TEXT" NUM_ROW="1">
        <DEFAULT>"MyLabel"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="ABSOLUTE" FIELD="CHECK" NUM_ROW="2">
    	<DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="CONNECTIONS" FIELD="CONNECTION_LIST" NUM_ROW="3" SHOW_IF = "ABSOLUTE=='flase'"  FILTER="FLOW_MAIN">
    </PARAMETER>
   
    <PARAMETER NAME="THRESHLODS" FIELD="TABLE" NUM_ROW="4" NB_LINES="5" >
      <ITEMS>
        <ITEM NAME="LABEL" VALUE="newline"/>
        <ITEM NAME="BOTTOM" VALUE="0"/>
        <ITEM NAME="TOP" VALUE="80"/>
        <ITEM NAME="COLOR" FIELD="COLOR"/>
      </ITEMS>
    </PARAMETER>
   
   </PARAMETERS>

   <CODEGENERATION/>

   <RETURNS/>

</COMPONENT>