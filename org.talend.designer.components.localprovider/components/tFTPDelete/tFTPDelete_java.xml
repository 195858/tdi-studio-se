<!-- $Id: tFTP_perl.xml 1528 2007-01-19 17:03:36 +0000 (星期五, 19 一月 2007) smallet $ -->
<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
       SERIAL=""
      VERSION="0.101"
       STATUS="ALPHA"
         
COMPATIBILITY="ALL"
       AUTHOR="Talend"
 RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Internet/FTP</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
  
  
      <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="1"
      >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>   
    
    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tFTPConnection"
      NUM_ROW="1"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />
  
    <PARAMETER 
    	NAME="HOST" 
    	FIELD="TEXT" 
    	REQUIRED="true" 
    	NUM_ROW="10"
    	SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    	>
      <DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER 
    	NAME="PORT" 
    	FIELD="TEXT" 
    	REQUIRED="true" 
    	NUM_ROW="10"
    	SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    	>
      <DEFAULT>21</DEFAULT>
    </PARAMETER>

    <PARAMETER 
    	NAME="USERNAME" 
    	FIELD="TEXT" 
    	REQUIRED="true" 
    	NUM_ROW="20"
    	SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    	>
      <DEFAULT>"anonymous"</DEFAULT>
    </PARAMETER>

    <PARAMETER 
    	NAME="PASSWORD" 
    	FIELD="TEXT" 
    	REQUIRED="true" 
    	NUM_ROW="20" 
    	NOT_SHOW_IF="((SFTP=='true') AND (AUTH_METHOD == 'PUBLICKEY')) OR (USE_EXISTING_CONNECTION == 'true')"
    	>
      <DEFAULT>"suomynona"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PRIVATEKEY"
      FIELD="FILE"
      NUM_ROW="21"
      SHOW_IF="(SFTP=='true') AND (AUTH_METHOD == 'PUBLICKEY')"
    >
      <DEFAULT>"/.ssh/id_dsa"</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="PASSPHRASE"
      FIELD="TEXT"
      NUM_ROW="22"
      SHOW_IF="(SFTP=='true') AND (AUTH_METHOD == 'PUBLICKEY')"
    >
      <DEFAULT>"secret"</DEFAULT>
    </PARAMETER>    
    
	<PARAMETER NAME="REMOTEDIR" FIELD="TEXT" REQUIRED="false" NUM_ROW="30">
      <DEFAULT>"."</DEFAULT>
    </PARAMETER>  
    <PARAMETER NAME="SFTP" FIELD="CHECK" REQUIRED="true" NUM_ROW="40">
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="AUTH_METHOD"
      FIELD="CLOSED_LIST"
      NUM_ROW="40"
      SHOW_IF="(SFTP=='true')"
    >
      <ITEMS DEFAULT="PASSWORD">
        <ITEM NAME="PUBLICKEY"  VALUE="PUBLICKEY"/>
        <ITEM NAME="PASSWORD"   VALUE="PASSWORD"/>
      </ITEMS>
    </PARAMETER>    
     
    <PARAMETER NAME="FILES" FIELD="TABLE" REQUIRED="false" NUM_ROW="50" NB_LINES="5">
      <ITEMS>
        <ITEM NAME="FILEMASK" />
      </ITEMS>
    </PARAMETER>
    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="60"
      SHOW_IF="(SFTP=='true')"
      REQUIRED="true">
       <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
   
  </PARAMETERS>
  
	<ADVANCED_PARAMETERS>
	
		<PARAMETER NAME="UES_PROXY" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="85" REPOSITORY_VALUE="UES_PROXY">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	
		<PARAMETER NAME="PROXY_HOST" FIELD="TEXT" 
			NUM_ROW="86" SHOW_IF="UES_PROXY == 'true'" REPOSITORY_VALUE="PROXY_HOST">
			<DEFAULT>"61.163.92.4"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PORT" FIELD="TEXT"
			NUM_ROW="87" SHOW_IF="UES_PROXY == 'true'" REPOSITORY_VALUE="PROXY_PORT">
			<DEFAULT>"1080"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="PROXY_USERNAME" FIELD="TEXT" 
			NUM_ROW="88" SHOW_IF="UES_PROXY == 'true'" REPOSITORY_VALUE="PROXY_USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PROXY_PASSWORD" FIELD="TEXT"
			NUM_ROW="89" SHOW_IF="UES_PROXY == 'true'" REPOSITORY_VALUE="PROXY_PASSWORD">
			<DEFAULT>""</DEFAULT>
		</PARAMETER> 	  
	
	</ADVANCED_PARAMETERS>  
  
  <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="Java-FTP" MODULE="edtftpj.jar" REQUIRED="true" />
       <IMPORT NAME="Java-SFTP" MODULE="jsch-0.1.36.jar" REQUIRED="true" />
     </IMPORTS>
   </CODEGENERATION>
  <RETURNS>
  	 <RETURN NAME="NB_FILE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  	 <RETURN NAME="CURRENT_STATUS" TYPE="id_String" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>