<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROCESS"
      FIELD="PROCESS_TYPE"
      NUM_ROW="10"
      REQUIRED="true"
    />

    <PARAMETER
      NAME="DIE_ON_CHILD_ERROR"
      FIELD="CHECK"
      NUM_ROW="11"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TRANSMIT_WHOLE_CONTEXT"
      FIELD="CHECK"
      NUM_ROW="20"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>	
	
    <PARAMETER
      NAME="CONTEXTPARAMS"
      FIELD="TABLE"
      NUM_ROW="30"
      NB_LINES="8"
    >
      <ITEMS>
        <ITEM NAME="PARAM_NAME_COLUMN" FIELD="CONTEXT_PARAM_NAME_LIST" />
        <ITEM NAME="PARAM_VALUE_COLUMN" FIELD="TEXT" />
      </ITEMS>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="CHILD_RETURN_CODE" TYPE="STRING" AVAILABILITY="AFTER"/>
  </RETURNS>

</COMPONENT>
