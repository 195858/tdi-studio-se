<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20090820A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    VISIBLE="false"
    TECHNICAL="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <FAMILIES>
    <FAMILY>Technical</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="10"
    >
      <DEFAULT/> 
    </PARAMETER>

    <PARAMETER
      NAME="DESTINATION"
      FIELD="TEXT"
      NUM_ROW="20"
      REQUIRED="true"
    >
      <DEFAULT>tThresholdVA_1</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="SUFFIXSEPARATOR" FIELD="TEXT" NUM_ROW="30"
		REQUIRED="true">
		<DEFAULT>":"</DEFAULT>
    </PARAMETER>
    
	<PARAMETER NAME="FIELDSEPARATOR" FIELD="TEXT" NUM_ROW="40"
		REQUIRED="true" REPOSITORY_VALUE="FIELD_SEPARATOR">
		<DEFAULT>"\n"</DEFAULT>
	</PARAMETER>
		    	    
	<PARAMETER NAME="ROWSEPARATOR" FIELD="TEXT" NUM_ROW="40"
		REQUIRED="true" REPOSITORY_VALUE="ROW_SEPARATOR">
		<DEFAULT>"\n\n"</DEFAULT>
	</PARAMETER>    

  </PARAMETERS>
  
  	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="ADVANCED_SEPARATOR" FIELD="CHECK" REQUIRED="true" NUM_ROW="10" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="THOUSANDS_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="11"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>","</DEFAULT>
		</PARAMETER>		
		
		<PARAMETER NAME="DECIMAL_SEPARATOR" FIELD="TEXT" REQUIRED="true" NUM_ROW="11"
			SHOW_IF="(ADVANCED_SEPARATOR == 'true')">
			<DEFAULT>"."</DEFAULT>
		</PARAMETER>
  	</ADVANCED_PARAMETERS>
  
  <CODEGENERATION/>

  <RETURNS>
  	<RETURN NAME="MAILFIELD" TYPE="id_String" AVAILABILITY="AFTER" />
  </RETURNS>
</COMPONENT>
