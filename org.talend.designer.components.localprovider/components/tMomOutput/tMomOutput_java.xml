<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE ="true"
  >
    <SIGNATURE />
  </HEADER>

  <FAMILIES>
    <FAMILY>Internet</FAMILY>
  </FAMILIES>


  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="SERVER"
      FIELD="CLOSED_LIST"
      NUM_ROW="20"
    >
      <ITEMS DEFAULT="WEBSPHERE">
        <ITEM NAME="WEBSPHERE"   VALUE="WebSphere" />
        <ITEM NAME="JBOSS"   VALUE="JBoss" />
        <ITEM NAME="ACTIVEMQ" VALUE="ActiveMQ"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SERVERADDRESS"
      FIELD="TEXT"
      NUM_ROW="20"
    >
	<DEFAULT>"localhost"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SERVERPORT"
      FIELD="TEXT"
      NUM_ROW="20"
    >
	<DEFAULT IF="(SERVER == 'JBOSS')">"1099"</DEFAULT>
	<DEFAULT IF="(SERVER == 'WEBSPHERE')">"1414"</DEFAULT>
	<DEFAULT IF="(SERVER == 'ACTIVEMQ')">61616</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="MQ_DERVIERS" FIELD="TEXT" NUM_ROW="30" SHOW="false">
		<DEFAULT IF="(SERVER == 'JBOSS')">jbossall-client.jar</DEFAULT>
		<DEFAULT IF="(SERVER == 'WEBSPHERE')">x.com.ibm.mq.jmqi.jar;x.com.ibm.mqjms.jar;x.com.ibm.mq.headers.jar;x.connector.jar;x.jms.jar;x.jta.jar;dhbcore.jar</DEFAULT>
		<DEFAULT IF="(SERVER == 'ACTIVEMQ')">activemq-all-5.1.0.jar</DEFAULT>
	</PARAMETER>
    
     <PARAMETER
      NAME="TO"
      FIELD="TEXT"
      SHOW="true"
      NUM_ROW="30"
      SHOW_IF="(SERVER == 'JBOSS')OR (SERVER == 'ACTIVEMQ')"
    > 
    <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="MSGTYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="30">
      
      <ITEMS DEFAULT="TOPIC">
        <ITEM NAME="TOPIC"   VALUE="Topic" />
        <ITEM NAME="QUEUE"   VALUE="Queue" />
      </ITEMS>
    </PARAMETER>
    
  <PARAMETER
      NAME="CHANNEL"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW_IF="(SERVER == 'WEBSPHERE')" >
   	<DEFAULT>"DC.SVRCONN"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="40"	  
    >
      
      <TABLE IF="(SERVER == 'ACTIVEMQ') and (IS_MAPMESSAGE == 'false')" READONLY="true">
        <COLUMN NAME="message" TYPE="id_String" LENGTH="255"/>
      </TABLE>

      <TABLE IF="(SERVER == 'ACTIVEMQ') and (IS_MAPMESSAGE == 'true')" READONLY="false">
        <COLUMN NAME="message" TYPE="id_String" LENGTH="255"/>
      </TABLE>

      <TABLE IF="(SERVER == 'WEBSPHERE') and (IS_MAPMESSAGE == 'false')" READONLY="true">
        <COLUMN NAME="message" TYPE="id_String" LENGTH="255"/>
      </TABLE>
      
      <TABLE IF="(SERVER == 'WEBSPHERE') and (IS_MAPMESSAGE == 'true')" READONLY="false">
        <COLUMN NAME="message" TYPE="id_String" LENGTH="255"/>      	
      </TABLE>
      
      
    </PARAMETER>
   
  <PARAMETER
      NAME="QM"
      FIELD="TEXT"
      NUM_ROW="50"
      SHOW_IF="(SERVER == 'WEBSPHERE')"
    > 
    <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
  <PARAMETER
      NAME="QUEUE"
      FIELD="TEXT"
      NUM_ROW="50"
      SHOW_IF="(SERVER == 'WEBSPHERE')"
    > 
    <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
	<PARAMETER NAME="IS_USE_MESSAGE_ID" FIELD="CHECK" REQUIRED="true" NUM_ROW="60"
		SHOW_IF="(SERVER == 'JBOSS')" >
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="IS_MAPMESSAGE" FIELD="CHECK" REQUIRED="true" NUM_ROW="70" 
		SHOW_IF="(SERVER == 'WEBSPHERE') or (SERVER == 'ACTIVEMQ')">
	  <DEFAULT>false</DEFAULT>
	</PARAMETER>
  </PARAMETERS>
  
  

  <CODEGENERATION>
   <IMPORTS>
		<IMPORT NAME="JNP-CLIENT" MODULE="jnp-client.jar" REQUIRED="true" /> 
  		<IMPORT NAME="JBOSS MQ LIB" MODULE="jbossall-client.jar" REQUIRED="false" /> 
		<IMPORT NAME="ACTIVEMQ LIB" MODULE="activemq-all-5.1.0.jar" REQUIRED="false" /> 		   
		<IMPORT NAME="MQJ08" MODULE="x.com.ibm.mq.jmqi.jar" REQUIRED="true" />
		<IMPORT NAME="MQJMS" MODULE="x.com.ibm.mqjms.jar" REQUIRED="true" />
		<IMPORT NAME="MQJ06" MODULE="x.com.ibm.mq.headers.jar" REQUIRED="true" />
		<IMPORT NAME="MQJ13" MODULE="x.connector.jar" REQUIRED="true" />
		<IMPORT NAME="MQJ15" MODULE="x.jms.jar" REQUIRED="true" />	
		<IMPORT NAME="MQJ17" MODULE="x.jta.jar" REQUIRED="true" />	
		<IMPORT NAME="MQJ21" MODULE="dhbcore.jar" REQUIRED="true" />
     </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
  </RETURNS>

</COMPONENT>