<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE />
  </HEADER>

  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>

   <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <!--<CONNECTOR CTYPE="RUN_OK"/>-->
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="10"
    >
      <DEFAULT>"127.0.0.1"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="20"
    >
      <DEFAULT>22</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USERNAME"
      FIELD="TEXT"
      NUM_ROW="30"
    >
      <DEFAULT>"username"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="AUTH_METHOD"
      FIELD="CLOSED_LIST"
      NUM_ROW="40"
    >
      <ITEMS DEFAULT="PASSWORD">
        <ITEM NAME="PUBLICKEY"  VALUE="PUBLICKEY"/>
        <ITEM NAME="PASSWORD"   VALUE="PASSWORD"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="PASSWORD"
      FIELD="TEXT"
      NUM_ROW="50"
      SHOW_IF="AUTH_METHOD == 'PASSWORD'"
    >
      <DEFAULT>"password"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PRIVATEKEY"
      FIELD="FILE"
      NUM_ROW="70"
      SHOW_IF="AUTH_METHOD == 'PUBLICKEY'"
    >
      <DEFAULT>"/.ssh/id_dsa"</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="PASSPHRASE"
      FIELD="TEXT"
      NUM_ROW="50"
      SHOW_IF="AUTH_METHOD == 'PUBLICKEY'"
    >
      <DEFAULT>"secret"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="REMOTEDIR" FIELD="DIRECTORY" REQUIRED="false" NUM_ROW="80">
      <DEFAULT>"home"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="FILELIST"
      FIELD="TABLE"
      REQUIRED="true"
      NUM_ROW="100"
    >
      <ITEMS>
     	<ITEM NAME="SOURCE" />
      </ITEMS>
    </PARAMETER>    

  </PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
    	<IMPORT  NAME="Java_SCP"  MODULE="ganymed-ssh2-build211beta4.jar" REQUIRED="true"/>
    </IMPORTS>
  </CODEGENERATION>
<RETURNS/>
</COMPONENT>