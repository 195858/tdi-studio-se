<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MIN_INPUT="1" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:MYSQL"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    />

    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="20"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tMysqlConnection"
      NUM_ROW="20"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />

    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="PORT"
    >
      <DEFAULT>"3306"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="40"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="41"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="40"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="50"
      REQUIRED="true"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="60"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SURROGATE_KEY"
      FIELD="COLUMN_LIST"
      NUM_ROW="61"
    >
      <DEFAULT></DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SK_CREATION"
      FIELD="CLOSED_LIST"
      NUM_ROW="61"
    >
      <ITEMS DEFAULT="AUTO_INCREMENT">
        <ITEM NAME="AUTO_INCREMENT"   VALUE="AUTO_INCREMENT" />
        <ITEM NAME="INPUT_FIELD"      VALUE="INPUT_FIELD"/>
        <ITEM NAME="ROUTINE"          VALUE="ROUTINE"/>
        <ITEM NAME="TABLE_MAX"        VALUE="TABLE_MAX"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SK_DB_SEQUENCE"
      FIELD="TEXT"
      NUM_ROW="61"
      SHOW_IF="SK_CREATION == 'DB_SEQUENCE'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SK_INPUT_FIELD"
      FIELD="PREV_COLUMN_LIST"
      NUM_ROW="61"
      SHOW_IF="SK_CREATION == 'INPUT_FIELD'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SK_ROUTINE"
      FIELD="TEXT"
      NUM_ROW="61"
      SHOW_IF="SK_CREATION == 'ROUTINE'"
    >
      <DEFAULT>Numeric.sequence("seq", 1, 1)</DEFAULT>
    </PARAMETER>
	
    <PARAMETER
      NAME="SOURCE_KEYS"
      FIELD="TABLE"
      NUM_ROW="62"
      NB_LINES="2"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="USE_L1"
      FIELD="CHECK"
      NUM_ROW="70"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L1_FIELDS"
      FIELD="TABLE"
      NUM_ROW="71"
      NB_LINES="5"
      SHOW_IF="USE_L1 == 'true'"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="USE_L2"
      FIELD="CHECK"
      NUM_ROW="80"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L2_FIELDS"
      FIELD="TABLE"
      NUM_ROW="81"
      NB_LINES="5"
      SHOW_IF="USE_L2 == 'true'"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="L2_STARTDATE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="82"
      SHOW_IF="USE_L2 == 'true'"
    >
    </PARAMETER>

    <PARAMETER
      NAME="L2_ENDDATE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="82"
      SHOW_IF="USE_L2 == 'true'"
    >
    </PARAMETER>

    <PARAMETER
      NAME="USE_L2_ACTIVE"
      FIELD="CHECK"
      NUM_ROW="83"
      SHOW_IF="USE_L2 == 'true'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L2_ACTIVE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="83"
      SHOW_IF="(USE_L2_ACTIVE=='true' and USE_L2=='true')"
    >
    </PARAMETER>

    <PARAMETER
      NAME="USE_L2_VERSION"
      FIELD="CHECK"
      NUM_ROW="84"
      SHOW_IF="USE_L2 == 'true'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L2_VERSION_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="84"
      SHOW_IF="(USE_L2_VERSION=='true' and USE_L2=='true')"
    >
    </PARAMETER>

    <PARAMETER
      NAME="USE_L3"
      FIELD="CHECK"
      NUM_ROW="90"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="L3_FIELDS"
      FIELD="TABLE"
      NUM_ROW="100"
      NB_LINES="5"
      SHOW_IF="USE_L3 == 'true'"
    >
      <ITEMS>
        <ITEM NAME="CURRENT_VALUE" FIELD="PREV_COLUMN_LIST" />
        <ITEM NAME="PREV_VALUE" FIELD="COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="DEBUG"
      FIELD="CHECK"
      NUM_ROW="110"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
    	<IMPORT NAME="Driver-MySql" MODULE="mysql-connector-java-5.1.0-bin.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
   
  <RETURNS/>
</COMPONENT>