			} else if (typeToGenerate.equals("Object")) {
				isObject = true;
				if (!value.equals("null")) {
					%>
					if (<%=value%> == null) {
					<%
				}
				%>
				<%=statement%>.setNull(<%=counter%>, java.sql.Types.OTHER);
				<%
			} else if (typeToGenerate.equals("Boolean")) {
