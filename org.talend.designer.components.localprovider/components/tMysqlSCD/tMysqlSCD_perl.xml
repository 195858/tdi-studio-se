<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    EXTENSION="org.talend.designer.scd"
  >
    <SIGNATURE />
  </HEADER>
  
  <DOCUMENTATION>
    <URL />
  </DOCUMENTATION>
  
  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MIN_INPUT="1" MAX_INPUT="1" MAX_OUTPUT="0" />
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0" />
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK" />
    <CONNECTOR CTYPE="COMPONENT_ERROR" />
    <CONNECTOR CTYPE="RUN_IF" />
  </CONNECTORS>
  
  <PARAMETERS>     
    
    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:MYSQL"
    />
    
    <PARAMETER
      NAME="USE_EXISTING_CONNECTION"
      FIELD="CHECK"
      NUM_ROW="20"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="CONNECTION"
      FIELD="COMPONENT_LIST"
      FILTER="tMysqlConnection"
      NUM_ROW="20"
      SHOW_IF="USE_EXISTING_CONNECTION == 'true'"
    />
    
    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="SERVER_NAME"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="PORT"
    >
      <DEFAULT>'3306'</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="30"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="40"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="PASS"
      FIELD="TEXT"
      NUM_ROW="40"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="TABLE"
      FIELD="TEXT"
      NUM_ROW="50"
      REQUIRED="true"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="TABLE_ACTION"
      FIELD="CLOSED_LIST"
      NUM_ROW="51"
    >
      <ITEMS DEFAULT="NONE">
        <ITEM
          NAME="NONE"
          VALUE="NONE"
        />
        <ITEM
          NAME="CREATE_IF_NOT_EXISTS"
          VALUE="CREATE_IF_NOT_EXISTS"
        />
      </ITEMS>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      NUM_ROW="55"
    >
      <DEFAULT>''</DEFAULT>
    </PARAMETER>
    
     <PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="60">
         <DEFAULT/>  
    </PARAMETER>
    
    <PARAMETER
      NAME="SOURCE_KEYS"
      FIELD="TABLE"
      NUM_ROW="61"
      NB_LINES="2"
      SHOW="false"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="USE_L1"
      FIELD="CHECK"
      NUM_ROW="70"
      SHOW="false"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="L1_FIELDS"
      FIELD="TABLE"
      NUM_ROW="71"
      NB_LINES="5"
      SHOW_IF="USE_L1 == 'true'"
      SHOW="false"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="USE_L2"
      FIELD="CHECK"
      NUM_ROW="80"
      SHOW="false"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="L2_FIELDS"
      FIELD="TABLE"
      NUM_ROW="81"
      NB_LINES="5"
      SHOW_IF="USE_L2 == 'true'"
      SHOW="false"
    >
      <ITEMS>
        <ITEM NAME="NAME" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="L2_STARTDATE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="82"
      SHOW_IF="USE_L2 == 'true'"
      SHOW="false"
    >
    </PARAMETER>
    
    <PARAMETER
      NAME="L2_ENDDATE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="82"
      SHOW_IF="USE_L2 == 'true'"
      SHOW="false"
    >
    </PARAMETER>
    
    <PARAMETER
      NAME="USE_L2_ACTIVE"
      FIELD="CHECK"
      NUM_ROW="83"
      SHOW_IF="USE_L2 == 'true'"
      SHOW="false"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="L2_ACTIVE_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="83"
      SHOW_IF="USE_L2_ACTIVE == 'true'"
      SHOW="false"
    >
    </PARAMETER>
    
    <PARAMETER
      NAME="USE_L2_VERSION"
      FIELD="CHECK"
      NUM_ROW="84"
      SHOW_IF="USE_L2 == 'true'"
      SHOW="false"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="L2_VERSION_FIELD"
      FIELD="COLUMN_LIST"
      NUM_ROW="84"
      SHOW_IF="USE_L2_VERSION == 'true'"
      SHOW="false"
    >
    </PARAMETER>
    
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="10"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
      REQUIRED="true"
    >
      <DEFAULT>'ISO-8859-15'</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DEBUG"
      FIELD="CHECK"
      NUM_ROW="20"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
  
  </ADVANCED_PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="DBI" MODULE="DBI" REQUIRED="true" />
      <IMPORT NAME="DBD-mysql" MODULE="DBD::mysql" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>
  
  <RETURNS />
</COMPONENT>