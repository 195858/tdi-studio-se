<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
  	<PARAMETER NAME="CLIENT" FIELD="TEXT" NUM_ROW="1" GROUP="CONNECTION" REQUIRED="true">
		<DEFAULT>"000"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="USERID" FIELD="TEXT" NUM_ROW="2" GROUP="CONNECTION" REQUIRED="true">
		<DEFAULT>"TALEND"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="PASSWORD" FIELD="TEXT" NUM_ROW="3" GROUP="CONNECTION" REQUIRED="true">
		<DEFAULT>"FRANCE"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="LANGUAGE" FIELD="TEXT" NUM_ROW="4" GROUP="CONNECTION" REQUIRED="true">
		<DEFAULT>"EN"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="HOSTNAME" FIELD="TEXT" NUM_ROW="5" GROUP="CONNECTION" REQUIRED="true">
		<DEFAULT>"TOTOTITI"</DEFAULT>
	</PARAMETER>
  	<PARAMETER NAME="SYSTEMNUMBER" FIELD="TEXT" NUM_ROW="6" GROUP="CONNECTION" REQUIRED="true">
		<DEFAULT>"00"</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="7">
		<DEFAULT />
	</PARAMETER>
	
  	<PARAMETER NAME="SAP_FUNCTION" FIELD="TEXT" NUM_ROW="8" REQUIRED="true">
		<DEFAULT>"STFC_CONNECTION"</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="MAPPING_INPUT" FIELD="TABLE" REQUIRED="true" NUM_ROW="9" NB_LINES="10">
		<ITEMS>
			<ITEM NAME="SAP_PARAMETER_VALUE" FIELD="String" />
									
			<ITEM NAME="SAP_PARAMETER_TYPE" FIELD="CLOSED_LIST">
				<ITEMS DEFAULT="INPUT_SINGLE">
					<ITEM NAME="INPUT_SINGLE" VALUE="input_single" />
					<ITEM NAME="INPUT_STRUCTURE" VALUE="input_structure" />
					<ITEM NAME="INPUT_TABLE" VALUE="input_table" />
					<ITEM NAME="TABLE_INPUT" VALUE="table_input" />
				</ITEMS>
			</ITEM>
						
			<ITEM NAME="SAP_TABLE_NAME" FIELD="String" />			
			<ITEM NAME="SAP_PARAMETER_NAME" FIELD="String" />					
		</ITEMS>
	</PARAMETER>	
	
	<PARAMETER NAME="MAPPING_OUTPUT" FIELD="TABLE" REQUIRED="true" NUM_ROW="10" NB_LINES="15">
		<ITEMS BASED_ON_SCHEMA="true">
			<ITEM NAME="SAP_PARAMETER_TYPE" FIELD="CLOSED_LIST">
				<ITEMS DEFAULT="OUTPUT_SINGLE">
					<ITEM NAME="TABLE_OUTPUT" VALUE="table_output" />
					<ITEM NAME="OUTPUT_SINGLE" VALUE="output_single" />
					<ITEM NAME="OUTPUT_STRUCTURE" VALUE="output_structure" />
					<ITEM NAME="OUTPUT_TABLE" VALUE="output_table" />
				</ITEMS>
			</ITEM>
						
			<ITEM NAME="SAP_TABLE_NAME" FIELD="String" />			
			<ITEM NAME="SAP_PARAMETER_NAME" FIELD="String" />
		</ITEMS>
	</PARAMETER>	
	
  	<PARAMETER NAME="SAP_ITERATE_OUT_TYPE" FIELD="CLOSED_LIST" NUM_ROW="11">
		<ITEMS DEFAULT="OUTPUT_SINGLE">
			<ITEM NAME="TABLE_OUTPUT" VALUE="table_output" />
			<ITEM NAME="OUTPUT_SINGLE" VALUE="output_single" />
			<ITEM NAME="OUTPUT_STRUCTURE" VALUE="output_structure" />
			<ITEM NAME="OUTPUT_TABLE" VALUE="output_table" />
		</ITEMS>		
	</PARAMETER>
	
  	<PARAMETER NAME="SAP_ITERATE_OUT_TABLENAME" FIELD="TEXT" NUM_ROW="11" NOT_SHOW_IF="SAP_ITERATE_OUT_TYPE == 'OUTPUT_SINGLE'" REQUIRED="true">
		<DEFAULT></DEFAULT>
	</PARAMETER>			

  </PARAMETERS>
  
  <CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="sapjco-ntintel-2.1.8" MODULE="sapjco.jar" REQUIRED="true" />       
     </IMPORTS>
  </CODEGENERATION>
  
  <RETURNS/>
  
</COMPONENT>
