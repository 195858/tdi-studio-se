<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="10"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>

    <PARAMETER
      NAME="FILENAME"
      FIELD="FILE"
      NUM_ROW="20"
      REQUIRED="true"
    >
      <DEFAULT>"__COMP_DEFAULT_FILE_DIR__/out.xls"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SHEETNAME"
      FIELD="TEXT"
      NUM_ROW="30"
    >
      <DEFAULT>"Sheet1"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="INCLUDEHEADER"
      FIELD="CHECK"
      NUM_ROW="40"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="CREATE"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="40"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="APPEND_FILE"
      FIELD="CHECK"
      NUM_ROW="45"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="APPEND_SHEET"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="45"
      SHOW_IF="APPEND_FILE == 'true'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>



    <PARAMETER
      NAME="SCHEMA"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="50"
    >
      <DEFAULT/> 
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="60"
      REQUIRED="true"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

  </PARAMETERS>

  <CODEGENERATION>
    <IMPORTS>
      <IMPORT NAME="Java_Excel" MODULE="jxl.jar" REQUIRED="true" />
    </IMPORTS>
  </CODEGENERATION>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>

</COMPONENT>
