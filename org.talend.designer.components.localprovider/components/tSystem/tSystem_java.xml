<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"    MAX_OUTPUT="0" MAX_INPUT="1" />
    <CONNECTOR CTYPE="RUN_ERROR" MAX_OUTPUT="0" MAX_INPUT="1" />
    <CONNECTOR CTYPE="RUN_IF" MAX_INPUT="1" />
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="COMMAND" FIELD="MEMO_JAVA" REQUIRED="true" NUM_ROW="1">
      <DEFAULT>"cmd /c echo HELLO"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="OUTPUT" FIELD="CLOSED_LIST" NUM_ROW="2">
       <ITEMS DEFAULT="OUTPUT_TO_CONSOLE">
          <ITEM NAME="OUTPUT_TO_CONSOLE" VALUE="OUTPUT_TO_CONSOLE" />
          <ITEM NAME="RETRIEVE_OUTPUT"   VALUE="RETRIEVE_OUTPUT" />
          <ITEM NAME="NORMAL_OUTPUT"   VALUE="NORMAL_OUTPUT" />
       </ITEMS>
    </PARAMETER>
    
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="5" SHOW_IF="OUTPUT=='NORMAL_OUTPUT'" READONLY="true">
  	  <TABLE>
		<COLUMN NAME="outputline" TYPE="id_String" LENGTH="255" />
	  </TABLE>
  	</PARAMETER>
  </PARAMETERS>
  
  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="OUTPUT" TYPE="id_String" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
