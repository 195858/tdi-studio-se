<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="false"
    SCHEMA_AUTO_PROPAGATE="false"
    DATA_AUTO_PROPAGATE="false"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MIN_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="IN_SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="1">
      <DEFAULT/>
    </PARAMETER>
    
    <PARAMETER 
      NAME="AUTOCAST"
      FIELD="CHECK"
      NUM_ROW="3"
    >
    <DEFAULT>false</DEFAULT>
    </PARAMETER>   
    
    <PARAMETER
      NAME="MANUALTABLE"
      FIELD="TABLE"
      NUM_ROW="5"
      NB_LINES="5"
      SHOW_IF=" AUTOCAST == 'false'"
    >
      <ITEMS>
      	<ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
        <ITEM NAME="OUTPUT_COLUMN" FIELD="COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>
    
  </PARAMETERS>
  
   <ADVANCED_PARAMETERS/>
	   

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
