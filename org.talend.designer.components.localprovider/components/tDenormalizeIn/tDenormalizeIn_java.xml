<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.101"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
    VISIBLE="false"
    TECHNICAL="true"
  >
    <SIGNATURE></SIGNATURE>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
  
    <PARAMETER NAME="ORIGIN" FIELD="TEXT" NUM_ROW="2" REQUIRED="true">
      <DEFAULT>array_tArray_1</DEFAULT>
    </PARAMETER>    
  
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="4">
      <DEFAULT/>
    </PARAMETER>

    <PARAMETER
      NAME="DELIMITER"
      FIELD="TEXT"
      REQUIRED="true"
      NUM_ROW="5"
    >
      <DEFAULT>","</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="MERGE"
      FIELD="CHECK"
      REQUIRED="true"
      NUM_ROW="6"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>    

    <PARAMETER
      NAME="GROUPS"
      FIELD="TABLE"
      REQUIRED="false"
      NUM_ROW="8"
      NB_LINES="5"
    >
      <ITEMS>
        <ITEM NAME="INPUT_COLUMN" FIELD="PREV_COLUMN_LIST" />
      </ITEMS>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION/>

  <RETURNS>
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>
  </RETURNS>
</COMPONENT>
