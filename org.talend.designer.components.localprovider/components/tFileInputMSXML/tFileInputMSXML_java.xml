<COMPONENT>

	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20080104"
		STARTABLE="true" HAS_CONDITIONAL_OUTPUTS="true">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>File/Input</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR MULTI_SCHEMA="true" CTYPE="FLOW" MAX_INPUT="0" MIN_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="1" REQUIRED="true" REPOSITORY_VALUE="FILE_PATH">
			<DEFAULT>"__COMP_DEFAULT_FILE_DIR__/in.xml"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ROOT_LOOP_QUERY" FIELD="TEXT" NUM_ROW="2"
			REPOSITORY_VALUE="XPATH_QUERY">
			<DEFAULT>"/mailbox/emails/email"</DEFAULT>
		</PARAMETER>
		
		<PARAMETER NAME="SCHEMAS" FIELD="TABLE" NUM_ROW="5"	NB_LINES="6">
			<ITEMS>
				<ITEM NAME="SCHEMA" FIELD="SCHEMA_TYPE" />	
				<ITEM NAME="LOOP_PATH" FIELD="TEXT" />							
				<ITEM NAME="MAPPING" FIELD="SCHEMA_XPATH_QUERYS" />
				<ITEM NAME="CREATE_EMPTY_ROW" FIELD="CHECK" />
			</ITEMS>
		</PARAMETER>
		
	</PARAMETERS>
	
	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="GENERATION_MODE" FIELD="CLOSED_LIST" NUM_ROW="10">
	      <ITEMS DEFAULT="DOM4J">
	        <ITEM NAME="DOM4J" VALUE="Dom4j" />
	        <ITEM NAME="SAX" VALUE="SAX"/>
	      </ITEMS>
	    </PARAMETER>  
	    
		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="20"
			REQUIRED="true" REPOSITORY_VALUE="ENCODING">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>
		
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>				
			<IMPORT NAME="Java_DOM4J1.6" MODULE="dom4j-1.6.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="Java_JAXEN1.1" MODULE="jaxen-1.1.1.jar"
				REQUIRED="true" />
			<IMPORT NAME="Java_talendSAX" MODULE="TalendSAX.jar"
				REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
