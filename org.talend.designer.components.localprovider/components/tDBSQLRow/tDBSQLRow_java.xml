<!-- $Id: tDBSQLRow_perl.xml 1528 2007-01-19 17:03:36 +0000 (星期五, 19 一月 2007) smallet $ -->
<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="true" DATA_AUTO_PROPAGATE="false">
		<SIGNATURE />
	</HEADER>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" />
		<CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000" BASE_SCHEMA="FLOW" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
  	    <CONNECTOR CTYPE="THEN_RUN" MAX_INPUT="1"/>
		<CONNECTOR CTYPE="REFERENCE" />
		<CONNECTOR CTYPE="RUN_OK" />
		<CONNECTOR CTYPE="RUN_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="1" REPOSITORY_VALUE="DATABASE:ODBC" />

	<PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="3"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>Generic ODBC</DEFAULT>
    </PARAMETER>
    
		<PARAMETER NAME="DBNAME" FIELD="TEXT" NUM_ROW="3"
			REPOSITORY_VALUE="SID">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="4"
			REPOSITORY_VALUE="USERNAME">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="PASS" FIELD="TEXT" NUM_ROW="4"
			REPOSITORY_VALUE="PASSWORD">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA_FLOW" FIELD="SCHEMA_TYPE" NUM_ROW="5">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		
    <PARAMETER
      NAME="SCHEMA_REJECT"
      FIELD="SCHEMA_TYPE"
      REQUIRED="true"
      NUM_ROW="5"
      CONTEXT="REJECT"
      SHOW="true">
      <TABLE READONLY="true">
        <COLUMN NAME="errorCode" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
      </TABLE>
    </PARAMETER>		

	<PARAMETER
      NAME="DBTABLE"
      FIELD="DBTABLE"
      NUM_ROW="5"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
		<PARAMETER NAME="QUERYSTORE" FIELD="QUERYSTORE_TYPE"
			NUM_ROW="6">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="QUERY" FIELD="MEMO_SQL" NUM_ROW="7">
			<DEFAULT>"select id, name from employee"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="COMMIT_EVERY" FIELD="TEXT" NUM_ROW="8"
			REQUIRED="true">
			<DEFAULT>10000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="8">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
	</PARAMETERS>

	<CODEGENERATION></CODEGENERATION>

	<RETURNS>
		<RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW" />
	</RETURNS>
</COMPONENT>