<!-- $Id: tDBSQLRow_perl.xml 1528 2007-01-19 17:03:36 +0000 (星期五, 19 一月 2007) smallet $ -->
<COMPONENT>
  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"
    
    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="BEFORE" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="AFTER" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="REFERENCE"/>
    <CONNECTOR CTYPE="RUN_OK"/>
    <CONNECTOR CTYPE="RUN_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

  <PARAMETERS>
    <PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" NUM_ROW="1" REPOSITORY_VALUE="DATABASE" />

    <PARAMETER NAME="TYPE" FIELD="CLOSED_LIST" NUM_ROW="2" REPOSITORY_VALUE="TYPE">
      <ITEMS DEFAULT="undef;sun.jdbc.odbc.JdbcOdbcDriver">
        <ITEM NAME="ODBC"       VALUE="undef;sun.jdbc.odbc.JdbcOdbcDriver" REPOSITORY_ITEM="ODBC"/>
      </ITEMS>
    </PARAMETER>

    <PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="3" REPOSITORY_VALUE="SERVER_NAME" NOT_SHOW_IF="(TYPE=='MSSQL_ODBC' or TYPE=='ODBC')">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="3" REPOSITORY_VALUE="PORT" NOT_SHOW_IF="(TYPE=='MSSQL_ODBC' or TYPE=='ODBC')">
      <DEFAULT>""</DEFAULT>
      <DEFAULT IF="TYPE=='MYSQL'">"3306"</DEFAULT>
      <DEFAULT IF="TYPE=='PGSQL'">"5432"</DEFAULT>
<!--         <DEFAULT IF="TYPE=='PGSQL'">"1521"</DEFAULT> -->
    </PARAMETER>

    <PARAMETER NAME="DBNAME" FIELD="TEXT" NUM_ROW="3" REPOSITORY_VALUE="SID">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER 
      NAME="SCHEMA_DB"
      FIELD="TEXT"
      NUM_ROW="3"
      REPOSITORY_VALUE="SCHEMA" 
      NOT_SHOW_IF="(TYPE=='MSSQL_ODBC') or (TYPE=='ODBC') or (TYPE=='MYSQL')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="4" REPOSITORY_VALUE="USERNAME">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="PASS" FIELD="TEXT" NUM_ROW="4" REPOSITORY_VALUE="PASSWORD">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="5">
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="QUERYSTORE"
      FIELD="QUERYSTORE_TYPE"
      NUM_ROW="6"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="QUERY" FIELD="MEMO_SQL" NUM_ROW="7">
      <DEFAULT>"select id, name from employee"</DEFAULT>
    </PARAMETER>

    <PARAMETER NAME="ENCODING" FIELD="TEXT" NUM_ROW="8" REQUIRED="true">
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="12"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
  </PARAMETERS>

  <CODEGENERATION></CODEGENERATION>

  <RETURNS>
    <RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW"/>
  </RETURNS>
</COMPONENT>