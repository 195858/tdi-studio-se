<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20050320A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>File/Input</FAMILY>
  </FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true"
			NUM_ROW="10" REPOSITORY_VALUE="EXCEL" />

		<PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="10"
			REQUIRED="true" REPOSITORY_VALUE="FILE_PATH">
			<DEFAULT>'__COMP_DEFAULT_FILE_DIR__/in.xls'</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ALL_SHEETS" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="20" REPOSITORY_VALUE="SELECT_ALL_SHEETS">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SHEETLIST" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="30" NB_LINES="5" SHOW_IF="ALL_SHEETS == 'false'" REPOSITORY_VALUE="SHEET_LIST">
			<ITEMS>
				<ITEM NAME="SHEETNAME" VALUE="'first sheet'" REPOSITORY_ITEM="EXCEL_SHEETNAME"/>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="HEADER" FIELD="TEXT" NUM_ROW="40" REPOSITORY_VALUE="HEADER">
			<DEFAULT>0</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="FOOTER" FIELD="TEXT" NUM_ROW="40" REPOSITORY_VALUE="FOOTER">
			<DEFAULT>0</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LIMIT" FIELD="TEXT" NUM_ROW="40" REPOSITORY_VALUE="LIMIT">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="FIRST_COLUMN" FIELD="TEXT" NUM_ROW="50" REPOSITORY_VALUE="FIRST_COLUMN">
			<DEFAULT>'1'</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="LAST_COLUMN" FIELD="TEXT" NUM_ROW="50" REPOSITORY_VALUE="LAST_COLUMN">
			<DEFAULT />
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true"
			NUM_ROW="60" />

	</PARAMETERS>

	<ADVANCED_PARAMETERS>

		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="10"
			REQUIRED="true" REPOSITORY_VALUE="ENCODING">
			<DEFAULT>'ISO-8859-15'</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="WITH_FORMAT" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="20">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="Spreadsheet-ParseExcel"
				MODULE="Spreadsheet::ParseExcel" REQUIRED="true" />
			<IMPORT NAME="List-MoreUtils" MODULE="List::MoreUtils"
				REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="INTEGER" AVAILABILITY="AFTER" />
		<RETURN NAME="CURRENT_SHEET" TYPE="STRING" AVAILABILITY="FLOW" />
		<RETURN NAME="CURRENT_SHEET_NAME" TYPE="STRING"
			AVAILABILITY="FLOW" />
		<RETURN NAME="CURRENT_SHEET_POSITION" TYPE="STRING"
			AVAILABILITY="FLOW" />
	</RETURNS>
</COMPONENT>