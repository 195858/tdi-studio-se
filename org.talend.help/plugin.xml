<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.help.toc">
         
	<toc
            file="user_guide.xml"
            primary="true"/>
    <toc
            file="developer_guide.xml"
            primary="true"/>   
    <toc
            file="perlfunc.xml"/>         
         
   </extension>
   
   <extension
         point="org.eclipse.help.contexts">
         <contexts file="contexts.xml"/>
   </extension>
   
      	<extension
		point="org.eclipse.ui.actionSets">
   
   		<actionSet
			id="org.talend.help.perl"
			label="%HelpAction.label"
			visible="true">
			<action 
				id="org.talend.help.perl.OpenPerlHelpAction"
				class="org.talend.help.perl.OpenPerlHelpAction"
				menubarPath="help/additions"
				label="%HelpAction.label"/>
		</actionSet>
   </extension>
  
</plugin>
