            actionProviderId="org.talend.repository.viewer.PerspectiveFilterActionProvider"
            perspectiveId="org.talend.rcp.perspective">
         <includes>
            <contentExtension
                  pattern="org.talend.repository.viewer.code.*">
            </contentExtension>
            <contentExtension
                  pattern="org.talend.repository.viewer.metadata.*">
            </contentExtension>
         </includes>
      </perspectiveFilter>
